  <Sql>
    <key>GetByCriteria</key>
    <query>
      SELECT *
      FROM   wb_resumenmd
      WHERE  resmdmes BETWEEN To_date('{0}', 'yyyy-MM-dd') AND
      To_date('{1}', 'yyyy-MM-dd')
    </query>
  </Sql>

  <Sql>
    <key>VerificarExistencia</key>
    <query>
      select
      *
      from
      wb_resumenmd
      where
      resmdmes = to_date('{0}', 'YYYY-MM-DD')
    </query>
  </Sql>
  
  <Sql>
    <key>GetMaxMdvalor</key>
    <query>
      SELECT *
      FROM  (SELECT *
      FROM   wb_resumenmd
      WHERE  resmdmes BETWEEN To_date('{0}', 'yyyy-MM-dd') AND
      To_date('{1}', 'yyyy-MM-dd')
      ORDER  BY resmdvalor DESC)
      WHERE  ROWNUM = 1
    </query>
  </Sql>
