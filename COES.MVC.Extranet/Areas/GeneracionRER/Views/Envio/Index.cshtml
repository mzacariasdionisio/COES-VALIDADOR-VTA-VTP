@model COES.Servicios.Aplicacion.FormatoMedicion.FormatoModel
@{
    ViewBag.Title = "Generación RER";
    Layout = "~/Views/Shared/ViewFormato.cshtml";
}

<script src="~/Areas/GeneracionRER/Content/Scripts/generacionRER.js?v=2.1.7"></script>

<script src="~/Content/Scripts/highstock.js"></script>

<div class="form-title_intranet">
    <div class="content-titulo">Generación RER - Carga de datos</div>
    <div class="content-botonera">
        <span style="font-family: Arial, Helbetica, sans-serif; font-size: 12px">Horizonte de tiempo: </span>
        <select id="cbTipoFormato" name="cbTipoFormato" style="width: 90px; margin-bottom: 6px;">
            @foreach (var item in Model.ListaFormato)
            {
                <option value="@item.Formatcodi">@item.Periodo</option>
            }
        </select>
    </div>
</div>

<div id="cargaDatos">
    <input type="hidden" id="hfFormato1" value="@Model.IdFormato" />

    <div id="mensajePrincipal" class="action-message" style="display: none; margin: 0; margin-bottom: 10px">Por favor seleccione empresa y la fecha.</div>

    <div id="viewHojaFormato">

    </div>
</div>

<div id="validaciones" style="z-index:9999999999;" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Detalle de Errores</span></div>
    <div id="idTerrores" style="height:370px;">
    </div>
</div>

<div id="enviosanteriores" style="z-index:9999999998;" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Envios Anteriores</span></div>
    <div id="idEnviosAnteriores">

    </div>
</div>

@*<div class="panel-ieod" id="idIeod">
    </div>*@

<div class="general-popup" id="mantenimientos" style="min-width: 700px">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Equipos en Mantenimiento que Indisponen a la Unidad de Generación</span></div>
    <div id="idMantenimiento">

    </div>
</div>

<div class="general-popup" id="eventos" style="min-width:700px">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Eventos que Indisponen a la Unidad de Generación</span></div>
    <div id="idEvento">

    </div>
</div>

<div id="leyenda" style="z-index:9999999999;" class="popup_app">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Leyenda</span></div>
    <div id="idLeyenda" style="height:370px;">
    </div>
</div>