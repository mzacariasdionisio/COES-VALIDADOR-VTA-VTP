@model COES.MVC.Extranet.Areas.IEOD.Models.GestionAdministradorModel
@{
    ViewBag.Title = "Cumplimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<link href="~/Areas/IEOD/Content/Css/css.css" rel="stylesheet" />
<script src="~/Content/Scripts/multiselect.js"></script>
<script src="~/Areas/IEOD/Content/Scripts/administradorCumplimiento.js"></script>
<script src="~/Content/Scripts/dataTables.fixedColumns.min.js"></script>

<div class="form-title_intranet">
    <div class="content-titulo">Reporte de Cumplimiento</div>
    <div class="content-botonera">
        <input type="button" id="btnBuscar" value="Consultar" />
        <input type="button" id="btnExportar" value="Exportar" style="display: none" />
    </div>
</div>


<div class="content-hijo" id="mainLayout">
    <div class="search-content">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Formato:</td>
                <td>
                    <select id="cbFormato" style="width: 330px;">
                        @foreach (var item in Model.ListaFormato)
                        {
                            <option value="@item.Formatcodi">@item.Formatnombre</option>
                        }
                    </select>
                    <input type="hidden" id="hfFormato" />
                </td>

                <td>Empresa:</td>
                <td style="width:200px;">
                    <div id="empresas"></div>
                </td>
            </tr>
            <tr>
                <td>Fecha Desde:</td>
                <td><input type="text" id="fechaInicio" name="fechaInicio" value="@Model.FechaInicio" /></td>
                <td>Fecha Hasta:</td>
                <td><input type="text" id="fechaFin" name="fechaFin" value="@Model.FechaFin" /></td>
            </tr>
        </table>
    </div>
    <div class="content-tabla">
        <div class="tabla-header">
        </div>
        <div style="clear:both; height:5px"></div>
        <div id="listado"></div>
    </div>

</div>


