@model COES.Servicios.Aplicacion.FormatoMedicion.FormatoModel
@{
    ViewBag.Title = "Generación Despacho";
    Layout = "~/Views/Shared/ViewDespacho.cshtml";
}

<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />
<link href="~/Areas/RDO/Content/Css/DespachoGeneracion/hoja.css?" rel="stylesheet" />

<script src="~/Content/Scripts/highstock.js"></script>
<script src="~/Content/Scripts/jquery.easytabs.min.js"></script>
<script src="~/Content/Scripts/multiselect.js"></script>
<script src="~/Content/Scripts/plupload.full.min.js"></script>
<script src="~/Content/Scripts/Handson/moment.js"></script>
<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<script src="~/Areas/RDO/Content/Scripts/DespachoGeneracion/Formato/Hoja/global.js"></script>
<script src="~/Areas/RDO/Content/Scripts/DespachoGeneracion/Formato/Hoja/hojaGlobal.js"></script>
<script src="~/Areas/RDO/Content/Scripts/DespachoGeneracion/Formato/Hoja/hojaFormatoGrilla.js"></script>
<script src="~/Areas/RDO/Content/Scripts/DespachoGeneracion/Formato/Hoja/hojaFormato.js"></script>
<script src="~/Areas/RDO/Content/Scripts/DespachoGeneracion/despachoGeneracion.js"></script>

<div class="form-title_intranet">
    <div class="content-titulo">Generación de Despacho</div>
    <div class="content-botonera">
        <span style="font-family: Arial, Helbetica, sans-serif; font-size: 12px">Horizonte de tiempo: </span>
        <select id="cbTipoFormato" name="cbTipoFormato" style="width: 90px; margin-bottom: 6px;">
            @foreach (var item in Model.ListaFormato)
            {
                <option value="@item.Formatcodi">@item.Periodo</option>
            }
        </select>
    </div>
</div>

<div id="cargaDatos">
    <input type="hidden" id="hfFormato1" value="@Model.IdFormato" />

    <div id="mensajePrincipal" class="action-message" style="display: none; margin: 0; margin-bottom: 10px">Por favor seleccione empresa y la fecha.</div>

    <div id="viewHojaFormato">

    </div>
</div>

<div id="validaciones" style="z-index:9999999999;" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Detalle de Errores</span></div>
    <div id="idTerrores" style="height:370px;">
    </div>
</div>

<div id="enviosanteriores" style="z-index:9999999998;" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Envios Anteriores</span></div>
    <div id="idEnviosAnteriores">

    </div>
</div>

<div id="leyenda" style="z-index:9999999999;" class="popup_app">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Leyenda</span></div>
    <div id="idLeyenda" style="height:370px;">
    </div>
</div>