@model COES.MVC.Extranet.Areas.RechazoCarga.Models.EnvioArchivosMagnitudModel
@{
    ViewBag.Title = "Envío de Archivos de Magnitud";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style type="text/css">
    .controlDropdown {
        width: 300px !important;
    }
</style>
<div class="form-title">
    ENVÍO DE ARCHIVOS DE MAGNITUD DE CARGA RECHAZADA
</div>
<div class="form-main" id="mainLayout" style="height: 900px;">
    <div class="action-message" id="mensaje">Complete los datos, busque y seleccione cuadro</div>
    <div class="search-content" style="margin-bottom:0px; padding:15px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Suministrador: </td>
                <td>
                    @Html.DropDownList("suministrador", new SelectList(Model.Suministradores, "Emprcodi", "Emprrazsocial"), new { @class = "controlDropdown" })
                </td>
                <td>Tipo: </td>
                <td>
                    <select id="tipo" name="tipo" class="controlDropdown">
                        <option value="" selected="selected">--SELECCIONE--</option>
                        <option value="P">Preliminar - Scada</option>
                        <option value="D">Definitivo - Contadores</option>
                    </select>
                </td>
                <td></td>

            </tr>
            <tr>
                <td>Programa: </td>
                <td colspan="3">
                    @Html.DropDownList("programa", new SelectList(Model.Programas, "Rcprogcodi", "Rcprognombre"), new { style = "width:100%" })
                </td>
                <td></td>
            </tr>
            <tr>
                <td>Cuadro: </td>
                <td colspan="3">
                    <select id="cuadro" name="cuadro" style="width:100%">
                        <option value="0" selected="selected">--SELECCIONE--</option>                      
                    </select>
                    
                </td>
                <td>
                    <input type="hidden" id="fechaInicioDetalle" name="fechaInicioDetalle" />
                    <input type="hidden" id="fechaFinDetalle" name="fechaFinDetalle" />
                </td>
                <td><input type="button" value="Consultar" id="btnConsultar" name="btnConsultar" class="form-action" /></td>
            </tr>            
        </table>
    </div>
    <br />
    <div class="content-hijo">

        <div id="tab-container" class="tab-container">
            <ul class='etabs'>
                <li class='tab'><a href="#tabResumen">Resumen</a></li>
                <li class='tab'><a href="#tabDetalle">Adjuntar Registro</a></li>
            </ul>
            <div class='panel-container'>

                <div id="tabResumen">
                    <div class="search-content">
                        <table class="content-tabla-search">
                            <tr>
                                <td rowspan="2">
                                    <table cellpadding="0" cellspacing="0" border="0">
                                        <tr>
                                            <td class="content-action">
                                                <a href="#" id="btnDescargarFormato">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/bajar.png" /><br />
                                                        <span>Descargar formato</span>
                                                    </div>
                                                </a>
                                            </td>
                                            <td class="content-action">
                                                <a href="#" id="btnSelectExcel">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/subir.png" /><br />
                                                        <span>Importar formato</span>
                                                    </div>
                                                </a>
                                            </td>
                                            <td class="content-action">
                                                <a href="#" id="btnEnviarDatos">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/enviar.png" /><br />
                                                        <span>Enviar datos</span>
                                                    </div>
                                                </a>
                                            </td>
                                            <td class="content-action">
                                                <a href="#" id="btnMostrarErrores">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/errores.png" /><br />
                                                        <span>Mostrar errores</span>
                                                    </div>
                                                </a>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="content-tabla" id="" style="">

                        <div id="spl-subasta_1">
                            <div>
                                <div class="bodyexcel" id='detalleFormato'></div>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="tabDetalle" style="">

                    <div class="search-content">
                        <table class="content-tabla-search">
                            <tr>
                                <td>Cliente</td>
                                <td colspan="2">
                                    <select id="cbClientes" name="cbClientes" style="width: 450px">
                                        <option value="0">-- SELECCIONE --</option>
                                    </select>
                                </td>
                                <td></td>
                                <td rowspan="2">
                                    <table cellpadding="0" cellspacing="0" border="0">
                                        <tr>
                                            <td class="content-action">
                                                <a href="#" id="btnDescargarFormatoDetalle">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/bajar.png" /><br />
                                                        <span>Descargar formato</span>
                                                    </div>
                                                </a>
                                            </td>
                                            <td class="content-action">
                                                <a href="#" id="btnSelectExcelDetalle">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/subir.png" /><br />
                                                        <span>Importar formato</span>
                                                    </div>
                                                </a>
                                            </td>
                                            <td class="content-action">
                                                <a href="#" id="btnEnviarDatosDetalle">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/enviar.png" /><br />
                                                        <span>Enviar datos</span>
                                                    </div>
                                                </a>
                                            </td>
                                            <td class="content-action">
                                                <a href="#" id="btnMostrarErroresDetalle">
                                                    <div class="content-item-action">
                                                        <img src="~/Areas/RechazoCarga/Content/Images/errores.png" /><br />
                                                        <span>Mostrar errores</span>
                                                    </div>
                                                </a>
                                            </td>
                                        </tr>
                                    </table>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="3"><div id="mensajeFechas"></div></td>

                            </tr>
                        </table>
                    </div>

                    <div class="content-tabla" id="" style="">

                        <div id="spl-subasta_2">
                            <div>
                                <div class="bodyexcel" id='detalleCliente'></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>
<div id="validacionesDetalle" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Detalle de Errores Adjuntar Registro</span></div>
    <div id="idTerroresDetalle" style="height:370px;">

    </div>
</div>
<div id="validaciones" style="z-index:9999999999;">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Detalle de Errores</span></div>
    <div id="idTerrores" style="height:370px;">

    </div>
</div>

<!--Hoja de estilos -->
<link href="~/Areas/RechazoCarga/Content/Css/css.css" rel="stylesheet" />

<!--Menu contextual-->
<script src="~/Content/Scripts/contextmenu.js"></script>
<link href="~/Content/Css/contextmenu.css" rel="stylesheet" />
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />
<script src="~/Areas/RechazoCarga/Content/Scripts/envioArchivosMagnitud.js"></script>
<script src="~/Areas/RechazoCarga/Content/Scripts/util.js"></script>
<script src="~/Areas/RechazoCarga/Content/Scripts/json2.js"></script>
<script src="~/Content/Scripts/plupload.full.min.js"></script>
<script src="~/Content/Scripts/handsontable.full.min.js"></script>

<!--Tabs-->
<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<script src="~/Content/Scripts/jquery.easytabs.min.js?v=1.1"></script>
