
@{
    ViewBag.Title = "Datos del Tramitador";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Areas/RegistroIntegrante/Content/Css/css.css" rel="stylesheet" />
<link href="~/Areas/RegistroIntegrante/Content/Css/solicitud.css" rel="stylesheet" />
<script src="~/Areas/RegistroIntegrante/Content/Scripts/contacto.js?v=1.12"></script>
<script src="~/Areas/RegistroIntegrante/Content/Scripts/messaje-operation.js"></script>
<style>

    .b-close {
        cursor: pointer;
        position: absolute;
        right: 10px;
        top: 5px;
    }
</style>

<div class="form-title" style="color: #278CCC; margin-bottom:0px;">Datos Persona de Contacto</div>
<div class="home-content" style="line-height:20px">
    <input name="btnAgregarContacto" type="button" id="btnAgregarContacto" value="+ Agregar Persona de Contacto" class="button" style="width:200px; margin-bottom: 20px;">
    <div id="divContacto">        
        <div id="listadoContactos">

        </div>
    </div>
    <br />
</div>


<div id="popupAgregar" class="general-popup" style="min-width:700px">
    <div><span class="button b-close"><span>X</span></span></div>

    <div class="popup-text" id="contenidoAgregar">
        <div class="popup-title"><span>Agregar Persona de Contacto</span></div>
        <div class="popup-text" id="contenidoAgregar">
            <div id="mensaje" class="action-message" style="display: none; width: 95%">Por favor complete los datos solicitados.</div>
            <table class="table-form-content">
                <tr>
                    <td class="label">Nombres:</td>
                    <td>
                        <input type="text" id="txtNombres" class="DatosRequeridos Required" data-validator-id="spntxtNombres" data-required="true" style="width: 330px" /><span id="spntxtNombres">(*)</span>
                    </td>
                </tr>
                <tr>
                    <td class="label">Apellidos:</td>
                    <td>
                        <input type="text" id="txtApellidos" class="DatosRequeridos Required" data-validator-id="spntxtApellidos" data-required="true" style="width: 330px" /><span id="spntxtApellidos">(*)</span>
                    </td>
                </tr>
                <tr>
                    <td class="label">Cargo de la Empresa:</td>
                    <td>
                        <input type="text" id="txtCargoEmpresa" class="DatosRequeridos Required" data-validator-id="spntxtCargoEmpresa" data-required="true" style="width: 330px" /><span id="spntxtCargoEmpresa">(*)</span>
                    </td>
                </tr>
                <tr>
                    <td class="label">Teléfono:</td>
                    <td>
                        <input type="text" id="txtTelefono" class="DatosRequeridos Required" data-validator-id="spntxtTelefono" data-required="true" style="width: 330px" /><span id="spntxtTelefono">(*)</span>
                    </td>
                </tr>
                <tr>
                    <td class="label">Teléfono Móvil:</td>
                    <td>
                        <input type="text" id="txtMovil" class="DatosRequeridos Required" data-validator-id="spntxtMovil" data-required="true" style="width: 330px" /><span id="spntxtMovil">(*)</span>
                    </td>
                </tr>
                <tr>
                    <td class="label">Correo electrónico:</td>
                    <td>
                        <input type="text" id="txtCorreo" class="DatosRequeridos Required" data-validator-id="spntxtCorreo" data-required="true" style="width: 330px" /><span id="spntxtCorreo">(*)</span>
                    </td>
                </tr>
                <tr>
                    <td colspan="2" style="text-align:center;">
                        <input id="btnCancelarNuevo" type="button" value="Cancelar" style="margin: 5px 0 5px 0;" />
                        <input id="btnGrabarNuevo" type="button" value="Grabar" style="margin: 5px 0 5px 0;" />
                    </td>
                </tr>
            </table>
        </div>
</div>
</div>


<div id="popupEdicion" class="general-popup" style="min-width:700px">
    <div><span class="button b-close"><span>X</span></span></div>

    <div class="popup-text" id="contenidoEdicion">
    </div>
</div>


<div id="MessagesClose">
    <div class="popup-title"><span>Eliminar Contacto</span></div>
    <div class="popup-text content-messajes-text">

    </div>
    <div class="formulario-boton">
        <input id="btnAceptarMessages" type="button" value="Aceptar" />
        <input id="btnCancelarMessages" type="button" value="Cancelar" style="display:none;" />
    </div>
</div>


