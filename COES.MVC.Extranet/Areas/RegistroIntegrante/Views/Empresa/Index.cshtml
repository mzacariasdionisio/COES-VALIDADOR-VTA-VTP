@model COES.MVC.Extranet.Areas.RegistroIntegrante.Models.EmpresaModel
@{
    ViewBag.Title = "Datos de Empresa";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <link href="~/Areas/RegistroIntegrante/Content/Css/solicitud.css" rel="stylesheet" />    
    <link href="~/Areas/RegistroIntegrante/Content/Css/form.css" rel="stylesheet" />
    <script src="~/Areas/RegistroIntegrante/Content/Scripts/empresa.js?v=1.11"></script>
}

<style>
    .  {
        text-transform: uppercase;
    }
</style>

<div class="form-title" style="color: #278CCC; margin-bottom:0px;">Modificar Datos de Empresa</div>
<div class="home-content" style="line-height:20px">
    <div style="width:200px; text-align:left; margin-bottom:10px">
       
    </div>
    <div id="divRegistro">
        <div id="mensaje" class="action-message" style="display: none; width: 97.5%">Por favor complete los datos solicitados.</div>
        <form id="frmModificarEmpresa" name="frmModificarEmpresa">

            <div class="content-registro">
                <table style="width:100%">
                    <tr>
                        <td class="registro-label ">Número de RUC:</td>
                        <td class="registro-control">
                            <input type="text" id="txtNumeroRuc"   style="width:300px" value="@Model.Emprruc" maxlength="11" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Nombre Comercial:</td>
                        <td class="registro-control">
                            <input type="text" id="txtNombreComercial"   style="width:500px" value="@Model.Emprnombrecomercial" maxlength="255" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Denominación o Razón Social:</td>
                        <td class="registro-control">
                            <input type="text" id="txtDenominacionRazonSocial"   style="width:500px" value="@Model.Emprrazsocial" maxlength="255" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Domicilio Legal:</td>
                        <td class="registro-control">
                            <input type="text" id="txtDomicilioLegal"   style="width:500px" value="@Model.Emprdomiciliolegal" maxlength="255" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Sigla:</td>
                        <td class="registro-control">
                            <input type="text" id="txtSigla"   style="width:150px" value="@Model.Emprsigla" maxlength="3" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Número de Partida Registral:</td>
                        <td class="registro-control">
                            <input type="text" id="txtNumeroPartidaRegistral"   style="width:300px" value="@Model.Emprnumpartidareg" maxlength="50" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Teléfono:</td>
                        <td class="registro-control">
                            <input type="text" id="txtTelefono"   style="width:300px" value="@Model.Emprtelefono" maxlength="50" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Fax:</td>
                        <td class="registro-control">
                            <input type="text" id="txtFax"   style="width:300px" value="@Model.Emprfax" maxlength="50" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label ">Página Web:</td>
                        <td class="registro-control">
                            <input type="text" id="txtPaginaWeb"   style="width:500px" value="@Model.Emprpagweb" maxlength="50" />
                        </td>
                    </tr>
                </table>
                
                <div style="margin:auto; margin-top:20px; width:300px">
                    <input type="button" id="btnEditar" value="Editar" />
                    <input type="button" id="btnCancelar" value="Cancelar" style="display:none" />
                    <input type="button" id="btnGrabar" value="Grabar" style="display:none" />
                </div>
            </div>
</form>
        
    </div>

    <br />
</div>








