@{
    Layout = null;
}
<script src="~/Areas/Resarcimientos/Content/Scripts/ntcse-rechazocarga-grilla.js"></script>
<link href="~/Areas/Resarcimientos/Content/Css/reporte.css" rel="stylesheet" />
<table id="tblTramites" border="0" class="pretty tabla-formulario">
    <thead>
        <tr>
            <th>
                Empresa Generadora
            </th>
            <th>
                C&oacute;digo COES <br /> del Evento
            </th>
            <th>
                Fecha de Inicio<br /> dd/mm/aaaa hh:mm
            </th>
            <th>
                Fecha de Fin<br /> dd/mm/aaaa hh:mm
            </th>
            <th>
                SED<br /> Nombre
            </th>
            <th>
                SED<br />KV
            </th>
            <th>
                C&oacute;digo Alimentador
            </th>
            <th>
                Duraci&oacute;n dK<br />hh:mm
            </th>
            <th>
                Ef
            </th>
            <th>
                ENS f,k<br />(kwh)
            </th>
            <th>
                Compensaci&oacute;n<br />(USD)
            </th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            foreach (var mp in Model)
            {
                string @nreg = "nreg" + @mp.RRCKEY;
                <tr id="@nreg" class="btnOpenFileManagerRegistro" style="background-color:@mp.BackgroundColor;" data-registro="@mp.RRCKEY" title="Precionar Doble Click para Ver Empresas">
                    <td class="select-btnclick">@mp.RRCEMPRESAGENERADORANOMBRE</td>
                    <td class="select-btnclick">@mp.RRCEVENCODINOMB</td>
                    <td class="select-btnclick">@string.Format("{0:" + @COES.MVC.Extranet.Helper.Constantes.FormatoFechaFull + "}", @mp.RRCFECHAINICIO)</td>
                    <td class="select-btnclick">@string.Format("{0:" + @COES.MVC.Extranet.Helper.Constantes.FormatoFechaFull + "}", @mp.RRCFECHAFIN)</td>
                    <td class="select-btnclick">@mp.RRCSUBESTACIONDSTRB</td>
                    <td class="select-btnclick" style="text-align:right !important;">@string.Format("{0:0.00}", @mp.RRCNIVELTENSIONSED)</td>
                    <td class="select-btnclick">@mp.RRCCODIALIMENTADOR</td>
                    <td class="select-btnclick" style="text-align:right !important;">@mp.RRCDURACION</td>
                    <td class="select-btnclick" style="text-align:right !important;">@string.Format("{0:0.00}", @mp.RRCEF)</td>
                    <td class="select-btnclick" style="text-align:right !important;">@string.Format("{0:0.000}", @mp.RRCENERGIAENS)</td>
                    <td class="select-btnclick" style="text-align:right !important;">@string.Format("{0:0.00}", @mp.RRCCOMPENSACION)</td>
                </tr>
            }
        }
    </tbody>
</table>
