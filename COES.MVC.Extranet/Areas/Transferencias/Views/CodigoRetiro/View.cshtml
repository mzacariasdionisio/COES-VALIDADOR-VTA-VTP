@model COES.MVC.Extranet.Areas.Transferencias.Models.CodigoRetiroModel
@{
    ViewBag.Title = "Solicitud de Códigos";
}

<fieldset>
    <legend>Solicitud Código Retiro de Energía</legend>
    <table cellspacing="2" cellpadding="5">
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.CliNombre, "Cliente : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.CliNombre)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.BarrNombBarrTran, "Barra Transferencia : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.BarrNombBarrTran)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.SoliCodiRetiFechaInicio, " Fecha de Inicio : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.TextBoxFor(model => model.Entidad.SoliCodiRetiFechaInicio, "{0:dd/MM/yyyy}", new { @readonly = true, style = "background-color: #FFFFFF; border: 0px; color: #111; font-size: 12px; text-indent: 0px;" })</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.SoliCodiRetiFechaFin, " Fecha Fin : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.TextBoxFor(model => model.Entidad.SoliCodiRetiFechaFin, "{0:dd/MM/yyyy}", new { @readonly = true, style = "background-color: #FFFFFF; border: 0px; color: #111; font-size: 12px; text-indent: 0px;" })</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.TipoContNombre, " Tipo de Contrato: ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.TipoContNombre)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.TipoUsuaNombre, " Tipo de Usuario: ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.TipoUsuaNombre)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.SoliCodiRetiCodigo, " Código: ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.SoliCodiRetiCodigo)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.SoliCodiRetiDescripcion, " Descripción: ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.SoliCodiRetiDescripcion)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.Entidad.SoliCodiRetiDetalleAmplio, " Mayor Detalle: ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.Entidad.SoliCodiRetiDetalleAmplio)</div>
            </td>
        </tr>
    </table>

</fieldset>
<div>
    <span class="button b-close"><span>X</span></span>
</div>