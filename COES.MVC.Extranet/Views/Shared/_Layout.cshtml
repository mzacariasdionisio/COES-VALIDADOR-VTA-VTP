</head>
<body>  
    <div class="layout">     
    
    @Html.Action("Header","Home", new {model = Model, area=""})       
    <div style="clear:both"></div>
     
    <div class="content" id="contentPrincipal">
         <div class="content-menu" id="contentMenu">
            @Html.Action("Menu", "Home", new { model = Model, area = "" })
        </div>
        <div style="clear:both"></div>
        <div id="loading" class="loading">
            <div class="loading-image"><img src="~/Content/Images/loading.gif" /></div>
            <div class="loading-text">Procesando...</div>&nbsp;
        </div>
        <section>           
            <div id="divGeneral">
                @RenderBody()
            </div>
            <div id="divTemporal" class="oculto">
                <iframe id="frameContenido" class="frame"></iframe>
                <div class="clear"></div>
            </div>
        </section>
    </div>   
    </div>
    <footer>
        <div class="footer" id="contentFooter">
            <span>COES © Todos los derechos reservados</span>
            <br />
            <span>Av. Los Conquistadores N° 1144, piso 2 - San Isidro, Lima - PERÚ Teléfono: (511) 611-8585</span>
        </div>
    </footer>
</body>
<script type="text/javascript">
        $(document).ready(function () {
            $('.menu').jqsimplemenu();
        });
</script>
