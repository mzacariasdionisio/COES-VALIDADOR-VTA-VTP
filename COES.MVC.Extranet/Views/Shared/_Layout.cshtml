        var siteRoot = "@Url.Content("~/")";
        //var initialURL = "@(Request.Url.GetLeftPart(UriPartial.Authority) + Url.Content("~"))";
		var initialURL = "https://www.coes.org.pe/extranet";
    </script>

    <script src="~/Content/Scripts/jquery.js"></script>
    <script src="~/Content/Scripts/datepicker.js?v=1.2"></script>
    <script src="~/Content/Scripts/menu.js?v=1.0"></script>
    <script src="~/Content/Scripts/datatable.js"></script>
