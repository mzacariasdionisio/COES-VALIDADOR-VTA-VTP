@model COES.MVC.Intranet.Areas.Admin.Models.ContactoModel
@{
    ViewBag.Title = "Administrar Comités";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Areas/Admin/Content/Scripts/comite.js"></script>
<link href="~/Content/Css/form.css" rel="stylesheet" />

<div class="form-title">
    <div class="content-titulo">Administración de Comités</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevoComite" value="Nuevo" />
        <input type="button" id="btnCancelarComite" value="  Volver  " />
    </div>
</div>

<div class="content-hijo">
    <div id="listado">
    </div>
    <br />
    <br />
</div>

<div id="popupEdicionComite" class="general-popup" style="width:500px">
    <div><span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Datos del Comité</span></div>
    <div class="popup-text" id="contenidoEdicionComite">

    </div>
</div>

<div id="popupListasComite" class="general-popup" style="width:500px">
    <div><span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Lista de Distribución</span></div>
    <div class="popup-text" id="contenidoListasComite">

    </div>
</div>