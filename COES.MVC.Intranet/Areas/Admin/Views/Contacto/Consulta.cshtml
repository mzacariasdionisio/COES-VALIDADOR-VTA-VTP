@model COES.MVC.Intranet.Areas.Admin.Models.ContactoModel
@{
    ViewBag.Title = "Administración de Contactos";
    Layout = "~/Views/Shared/_Master.cshtml";
}
<script src="~/Areas/Admin/Content/Scripts/contacto.js?v=1.0"></script>
<link href="~/Content/Css/form.css" rel="stylesheet" />

<div class="content-hijo">

    <div id="mensaje"></div>

    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Tipo de empresa:</td>
                <td>
                    <select id="cbTipoEmpresa">
                        <option value="">-TODOS-</option>
                        @foreach (var item in Model.ListaTipoEmpresas)
                        {
                            <option value="@item.Tipoemprcodi">@item.Tipoemprdesc</option>
                        }
                    </select>
                </td>
                <td>Empresa:</td>
                <td>
                    <select id="cbEmpresa">
                        <option value="">-TODOS-</option>
                        @foreach (var item in Model.ListaEmpresas)
                        {
                            <option value="@item.Emprcodi">@item.Emprnomb</option>
                        }
                    </select>
                </td>
                <td>Fuente:</td>
                <td>
                    <select id="cbFuente">
                        <option value="">-TODOS-</option>
                        <option value="R">Registro Integrantes</option>
                        <option value="E">Extranet</option>
                        <option value="P">Otros</option>
                    </select>
                </td>
                <td>
                    <input type="button" id="btnConsultar" value="Consultar" />
                    <input type="button" id="btnExportar" value='Exportar' />
                </td>
            </tr>
        </table>
    </div>

    <div id="listado">
    </div>

    <br />
    <br />
</div>

<div id="popupEdicion" class="general-popup" style="width:600px">
    <div><span class="button b-close"><span>X</span></span></div>

    <div class="popup-text" id="contenidoEdicion" style="margin-top:0">

    </div>
</div>

<input type="hidden" id="hfIndicadorPublico" value="S" />