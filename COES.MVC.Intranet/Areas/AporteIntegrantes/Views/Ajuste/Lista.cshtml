@model COES.MVC.Intranet.Areas.AporteIntegrantes.Models.BaseModel
<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th hidden>Id</th>
            <th>Año de inicio de Proyección</th>
            <th>Mes de inicio de Proyección</th>
            <th>Nombre de Ajuste</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaAjuste)
        {
            <tr id="fila_@item.Caiajcodi">
                <td hidden>@item.Caiajcodi</td>
                <td>@item.Caiajanio</td>
                <td>@item.Caiajmes</td>
                <td>@item.Caiajnombre</td>
                <td style="width:30px;"><a href="#" id="view_@item.Caiajcodi" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>
                <td style="width:30px;">
                    @if (Model.bEditar)
                    {
                        <a href="@Url.Action("Edit", new { caiajcodi = item.Caiajcodi })"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar el registro" alt="Editar el registro" /></a>
                    }
                </td>
                <td style="width:30px;"><a href="@Url.Action("Index", "AjusteEmpresa", new { caiajcodi = item.Caiajcodi, caiajetipoinfo = "E" })"><img src="@Url.Content("~/Areas/AporteIntegrantes/Content/Images/hidroelectrica.jpg")" title="Configurar Empresas de Energía" alt="Configurar Empresas de Energía" /></a></td>
                <td style="width:30px;"><a href="@Url.Action("Index", "AjusteEmpresa", new { caiajcodi = item.Caiajcodi, caiajetipoinfo = "T" })"><img src="@Url.Content("~/Areas/AporteIntegrantes/Content/Images/torre.png")" title="Configurar Empresas de Transmisión" alt="Configurar Empresas de Transmisión" /></a></td>
                <td style="width:30px;">
                    @if (Model.bEliminar)
                    {
                        <a href="#" id="eliminar_@item.Caiajcodi" class="delete"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
<div id="popup">    </div>