@model COES.MVC.Intranet.Areas.AporteIntegrantes.Models.BaseModel
@{
    ViewBag.Titulo = "Detalle del Presupuesto";
}
<fieldset>
    <legend>Presupuesto</legend>
    <table cellspacing="2" cellpadding="5">
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.EntidadAjuste.Caiajnombre, "Nombre del Ajuste : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.EntidadAjuste.Caiajnombre)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.EntidadAjuste.Caiajmes, "Año : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.EntidadAjuste.Caiajmes)</div>
            </td>
        </tr>
        <tr>
            <td>
                <div class="editor-label">@Html.LabelFor(model => model.EntidadAjuste.Caiajanio, "Mes Inicio : ")</div>
            </td>
            <td>
                <div class="editor-label">@Html.DisplayFor(model => model.EntidadAjuste.Caiajanio)</div>
            </td>
        </tr>
    </table>
</fieldset>
<div>
    <span class="button b-close"><span>X</span></span>
</div>