@model COES.MVC.Intranet.Areas.AporteIntegrantes.Models.BaseModel
<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th>Año de inicio de información</th>
            <th>Mes de inicio de información</th>
            <th>Nombre Presupuesto</th>
            <th>Nro. de meses de información</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaPresupuesto)
        {
            <tr id="fila_@item.Caiprscodi">
                <td>@item.Caiprsanio</td>
                <td>@item.Caiprsmesinicio</td>
                <td>@item.Caiprsnombre</td>
                <td>@item.Caiprsnromeses</td>
                <td style="width:30px;"><a href="#" id="view_@item.Caiprscodi" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>
                <td style="width:30px;">
                    @if (Model.bEditar)
                    {
                        <a href="@Url.Action("Edit", new { caiprscodi = item.Caiprscodi })"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar el registro" alt="Editar el registro" /></a>
                    }
                </td>
                <td style="width:30px;"><a href="@Url.Action("Index", "Ajuste", new { caiprscodi = item.Caiprscodi })"><img src="@Url.Content("~/Content/Images/recalculo.jpg")" title="Crear el ajuste" alt="Crear el ajuste" /></a></td>
                <td style="width:30px;">
                    @if (Model.bEliminar)
                    {
                        <a href="#" id="eliminar_@item.Caiprscodi" class="delete"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
<div id="popup">    </div>