@model COES.MVC.Intranet.Areas.AporteIntegrantes.Models.UnidadesVsBarrasModel
<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th style="text-align:left">Empresa</th>
            <th style="text-align:left">Central</th>
            <th style="text-align:left">Unidad</th>
            <th style="text-align:left">Punto de Medición</th>
            <th style="text-align:left">Barra</th>
            <th style="text-align:left">Fecha de Vigencia</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaEquiunidbarr)
        {
            <tr id="fila_@item.Caiunbcodi">
                <td style="text-align:left">@item.Emprnomb</td>
                <td style="text-align:left">@item.Equinombcen</td>
                <td style="text-align:left">@item.Equinombuni</td>
                <td style="text-align:left">@item.Ptomedielenomb</td>
                <td style="text-align:left">@item.Barrnombre</td>
                <td style="text-align:left">@item.Caiunbfecvigencia.ToString("dd/MM/yyyy")</td>
                <td style="width:30px;"><a href="#" id="view_@item.Caiunbcodi" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>
                <td style="width:30px;">
                    @if (Model.bEditar)
                    {
                        <a href="@Url.Action("Edit", new { id = item.Caiunbcodi })"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar el registro" alt="Editar el registro" /></a>
                    }
                </td>
                <td style="width:30px;">
                    @if (Model.bEliminar)
                    {
                        <a href="#" id="eliminar_@item.Caiunbcodi" class="delete"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
<div id="popup">    </div>
