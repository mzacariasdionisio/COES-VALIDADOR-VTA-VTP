@model COES.MVC.Intranet.Areas.CPPA.Models.CPPAModel
@Html.AntiForgeryToken()
@{
    Page.Title = "Pagina en construcción";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">
    // Datos JSON inyectados directamente en el script
    var listRevision = @Html.Raw(ViewBag.ListRevision);
</script>

<script src="~/Areas/TransfPotencia/Content/Scripts/load.js"></script>
<script src="~/Content/Scripts/plupload.full.min.js"></script>

<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />

<link href="~/Areas/Hidrologia/Content/Css/css.css?v=1.1" rel="stylesheet" />

<script src="~/Areas/CPPA/Content/Scripts/transmisores.js"></script>


<div class="form-title">
    <div class="content-titulo" style="width:1160px">Cargar totales de Transmisores (T)</div>
</div>
<div class="ast">

    <div class="content-hijo">

        <div class='action-message' id='mensaje' style="margin-bottom:5px; margin-top:0; "></div>

        <div class="search-content" style="margin-bottom:10px">

            <table class="content-tabla-search" style="width:auto">
                <tr>
                    <td><div class="editor-label">Año</div></td>
                    <td>
                        <div class="editor-label">
                            <select id="cbAnio" name="Year" style="width:90px;">
                                <option value="">-Seleccionar-</option>
                                @foreach (var item in Model.ListaAnio)
                                {
                                    <option value="@item.Entero1">@item.String1</option>
                                }
                            </select>
                        </div>
                    </td>

                    <td><div class="editor-label">Ajuste</div></td>
                    <td>
                        <div class="editor-label">
                            <select id="cbAjuste" name="ajuste" style="width:90px;" disabled>
                                <option value="">-Seleccionar-</option>
                            </select>
                        </div>
                    </td>

                    <td><div class="editor-label">Revisión</div></td>
                    <td>
                        <div class="editor-label">
                            <select id="cbRevision" name="revision" style="width:120px;" disabled>
                                <option value="">-Seleccionar-</option>
                            </select>
                        </div>
                    </td>

                    <td>
                        <input type="button" id="btnConsultar" value="Consultar" />
                    </td>

                    <td>

                        @*<div class="search-content" style="margin-bottom:0px; padding:10px; padding-top:3px; padding-bottom:9px; display:none; border: none;" id="divAcciones">*@
                            
                        <table class="table-search" cellpadding="0" cellspacing="0" border="0" style="width:auto">
                                <tr>
                                    <td class="content-action">
                                        <a href="#" id="btnDescargarFormato">
                                            <div class="content-item-action">
                                                <img src="~/Areas/DemandaCP/Content/Images/bajar.png" /><br />
                                                <span>Descargar</span>
                                            </div>
                                        </a>
                                    </td>
                                    <td class="content-action">
                                        <a href="#" id="btnSelectExcel">
                                            <div class="content-item-action">
                                                <img src="~/Areas/DemandaCP/Content/Images/subir.png" /><br />
                                                <span>Importar</span>
                                            </div>
                                        </a>
                                    </td>
                                    <td class="content-action">
                                        <a href="#" id="btnEnviarDatos">
                                            <div class="content-item-action">
                                                <img src="~/Areas/DemandaCP/Content/Images/enviar.png" /><br />
                                                <span>Grabar</span>
                                            </div>
                                        </a>
                                    </td>
                                    <td class="content-action">
                                        <a href="#" id="btnMostrarErrores">
                                            <div class="content-item-action">
                                                <img src="~/Areas/DemandaCP/Content/Images/errores.png" /><br />
                                                <span>Ver errores</span>
                                            </div>
                                        </a>
                                    </td>
                                    <td class="content-action">
                                        <a href="#" id="btnVerEnvios">
                                            <div class="content-item-action">
                                                <img src="~/Areas/DemandaCP/Content/Images/envios.png" /><br />
                                                <span>Ver envíos</span>
                                            </div>
                                        </a>
                                    </td>
                                </tr>
                            </table>

                        @*</div>*@

                    </td>

                </tr>

            </table>

        </div>

        <div id="grillaExcel" style="height:auto"></div>

    </div>

</div>

<div id="popup"><span class="button b-close"><span>X</span></span></div>

<div id="enviosanteriores" style="z-index:9999999998;">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Envios Anteriores</span></div>
    <div id="idEnviosAnteriores">

    </div>
</div>