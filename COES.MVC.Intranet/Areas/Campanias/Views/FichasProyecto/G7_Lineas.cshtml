@model COES.MVC.Intranet.Areas.Campanias.Models.CampaniasModel
<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />
<script src="~/Areas/Campanias/Content/Scripts/G7Lineas.js"></script>
<script src="~/Areas/Campanias/Content/Scripts/G7LFichaA.js"></script>
<script src="~/Areas/Campanias/Content/Scripts/G7LFichaB.js"></script>
<script src="~/Areas/Campanias/Content/Scripts/G7LInfAdi.js"></script>

<div id="tab-container-ficha" class='tab-container'>
    <div class="tab-container" id="tab-container-2">
        <ul class='tab'>
            <li class='tab'><a href="javascript:void(0);" onclick="openFicha('fichaa',this);">Ficha A</a></li>
            <li class='tab'><a href="javascript:void(0);" onclick="openFicha('fichab',this);">Ficha B</a></li>
            <li class="tab"><a href="javascript:void(0);" onclick="openFicha('fichaadic',this);">Información Adicional</a></li>
        </ul>
    </div>


    <div class='panel-container'>
        <div id="GLFichaA" class="ficha">
            <fieldset style="border: solid #DDDDDD 1px">

                <legend style="margin-left: 1em; padding: 0.2em 0.8em ">ASPECTOS GENERALES</legend>
                <table style="width:100%">
                    <tr>
                        <td class="registro-label">Nombre o denominación de la línea:</td>
                        <td colspan="4" class="registro-control">
                            <input type="text" style="width:100%" name="txtNombreLinea" id="txtNombreLinea" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Fecha de puesta en servicio:</td>
                        <td class="registro-control">
                            <input type="text" style="width:100%" name="txtFecPuestaServ" id="txtFecPuestaServ" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Subestaciones que conecta</td>
                        <td class="registro-label" colspan="2">Inicio</td>
                        <td class="registro-label" colspan="2">Fin</td>
                    </tr>
                    <tr>
                        <td class="registro-label"></td>
                        <td class="registro-control">
                            <select name="txtSubInicio" id="txtSubInicio">
                                <option value="">Seleccione</option>
                            </select>
                        </td>
                        <td class="registro-control">
                            <input type="text" placeholder="Otro" id="txtOtroSubInicio" name="txtOtroSubInicio" style="width:100px" />
                        </td>
                        <td class="registro-control">
                            <select name="txtSubFin" id="txtSubFin">
                                <option value="">Seleccione</option>
                            </select>
                        </td>
                        <td class="registro-control">
                            <input type="text" placeholder="Otro" style="width:100px" id="txtOtroSubFin" name="txtOtroSubFin" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Empresa propietaria:</td>
                        <td colspan="4" class="registro-control">
                            <input type="text" style="width:100%" id="txtEmpresaPropietaria" name="txtEmpresaPropietaria" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Ubicación del proyecto, en coordenadas:</td>
                        <td colspan="2" class="registro-control">
                          
                        </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td colspan="3">
                            <table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tablaUbicacionLin" aria-describedby="Evento">
                                <thead>
                                    <tr>
                                        <th scope="col">Archivo</th>
                                        <th scope="col">Acción</th>
                                    </tr>
                                </thead>
                            </table>
                        </td>
                    </tr>
                </table>

            </fieldset>
            <fieldset style="border: solid #DDDDDD 1px">

                <legend style="margin-left: 1em; padding: 0.2em 0.8em ">CARACTERÍSTICAS FÍSICAS Y ELÉCTRICAS</legend>
                <table style="width:100%">
                    <tr>
                        <td class="registro-label">Nivel de tensión(kV):</td>
                        <td class="registro-control">
                            <input type="text" class="txtnumber" placeholder="0.0000" id="txtnivelTension" name="txtnivelTension" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Capacidad de Corriente en condiciones normales (A):</td>
                        <td class="registro-control">
                            <input type="text" class="txtnumber" placeholder="0.0000" id="txtCapacidadCorriente" name="txtCapacidadCorriente" />
                        </td>
                    <tr>
                        <td class="registro-label">Capacidad de Corriente en condiciones de emergencia (A):</td>
                        <td class="registro-control">
                            <input type="text" class="txtnumber" placeholder="0.0000" id="txtCapacidadCorrienteA" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Tiempo de sobrecarga(horas):</td>
                        <td class="registro-control">
                            <input type="text" class="txtinteger" placeholder="" id="txtTiempoSobrecarga" name="txtTiempoSobrecarga" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Número de temas:</td>
                        <td class="registro-control">
                            <input type="text" class="txtnumber" placeholder="0.0000" id="txtNumTemas" name="txtNumTemas" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Longitud total(km):</td>
                        <td class="registro-control">
                            <input type="text" class="txtnumber" placeholder="0.0000" id="txtLongitud" name="txtLongitud" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Configuración geométrica típica (Adjuntar planos):</td>
                        <td colspan="2" class="registro-control" style="width:70%">
                             
                        </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td colspan="3">
                            <table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tablaConfiguracionGeometrica" aria-describedby="Evento">
                                <thead>
                                    <tr>
                                        <th scope="col">Archivo</th>
                                        <th scope="col">Acción</th>
                                    </tr>
                                </thead>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Longitud de vano promedio(m):</td>
                        <td class="registro-control">
                            <input type="text" class="txtnumber" placeholder="0.0000" id="txtLongitudVan" name="txtLongitudVan" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Tipo y materia de estructura de soporte:</td>
                        <td class="registro-control">
                            <input type="text" placeholder="" id="txtTipoMateria" name="txtTipoMateria" />
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Ruta geográfica (Adjuntar planos):</td>
                        <td colspan="2" class="registro-control">
                             
                        </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td colspan="3">
                            <table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tablaRutaGeografica" aria-describedby="Evento">
                                <thead>
                                    <tr>
                                        <th scope="col">Archivo</th>
                                        <th scope="col">Acción</th>
                                    </tr>
                                </thead>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td class="registro-label">Perfil longitudinal (Adjuntar planos)</td>
                        <td colspan="2" class="registro-control">
                            
                        </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td colspan="3">
                            <table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tablaPerfilLongi" aria-describedby="Evento">
                                <thead>
                                    <tr>
                                        <th scope="col">Archivo</th>
                                        <th scope="col">Acción</th>
                                    </tr>
                                </thead>
                            </table>
                        </td>
                    </tr>
                </table>
                <h4>Parámetros Físicos de Líneas de Transmisión</h4>
                <div class="search-content3">
                    <div style="display:table">
                        <div style="display:table-row">
                            <div style="display: table-cell; text-align: right;">
                                <div style="display:table;" id="barra">
                                    <div style="display:table-row">
                                        <div class="content-action" style="display: table-cell;">
                                            <a href="#" class="btnActive" id="btnDescargarFormato" onclick="exportarTablaExcelLinea1()">
                                                <div class="content-item-action">
                                                    <img src="~/Content/Images/bajar.png" /><br />
                                                    <span>Descargar</span>
                                                </div>
                                            </a>
                                        </div>
                                        <div class="content-action" style="display: table-cell;">
                                             
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <table style="width:100%">
                    <tr>
                        <td class="registro-label" colspan="3"><div id="tablaLineasHojaA1" class="tableHandson" style="overflow: hidden;"></div></td>
                    </tr>
                </table>
                <h4>Parámetros Eléctricos de Líneas de Transmisión</h4>
                <div class="search-content3">
                    <div style="display:table">
                        <div style="display:table-row">
                            <div style="display: table-cell; text-align: right;">
                                <div style="display:table;" id="barra">
                                    <div style="display:table-row">
                                        <div class="content-action" style="display: table-cell;">
                                            <a href="#" class="btnActive" id="btnDescargarFormato" onclick="exportarTablaExcelLinea2()">
                                                <div class="content-item-action">
                                                    <img src="~/Content/Images/bajar.png" /><br />
                                                    <span>Descargar</span>
                                                </div>
                                            </a>
                                        </div>
                                        <div class="content-action" style="display: table-cell;">
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <table style="width:100%">
                    <tr>
                        <td class="registro-label" colspan="3"><div id="tablaLineasHojaA2" class="tableHandson" style="overflow: hidden;"></div></td>
                    </tr>
                </table>
            </fieldset>
            <fieldset style="border: solid #DDDDDD 1px">

                <legend style="margin-left: 1em; padding: 0.2em 0.8em ">SISTEMA DE PROTECCIÓN</legend>
                <table style="width:100%">
                    <tr>
                        <td colspan="2" class="registro-label">Descripción del sistema de protección principal:</td>
                    </tr>
                    <tr>
                        <td>
                        </td>
                        <td class="registro-control">
                            <input type="text" placeholder="" style="width: 100%" id="txtSistemProteccion" name="txtSistemProteccion" />
                        </td>
                    </tr>
                    <tr>
                        <td colspan="2" class="registro-label">Descripción del sistema de protección de respaldo:</td>
                    </tr>
                    <tr>
                        <td>
                        </td>
                        <td class="registro-control">
                            <input type="text" placeholder="" style="width: 100%" id="txtDescripcionSistema" name="txtDescripcionSistema" />
                        </td>
                    </tr>
                </table>
            </fieldset>

            <table style="width:100%">
                <tr>
                    <td>Descripción general del proyecto</td>
                </tr>
                <tr>
                    <td>
                        <textarea style="width:100%" id="txtDescripcionGeneral" name="txtDescripcionGeneral"></textarea>
                    </td>
                </tr>
            </table>
            <br />
            <br />
            <table style="width:100%">
                <tr>
                    <td colspan="4" style="text-align: center;">
                        
                        <a class="cancel-button btnActive" href="javascript:void(0);" onclick="cerrarModalValidarReg()">Cancelar</a>
                    </td>
                </tr>
            </table>


        </div>
        <div id="GLFichaB" class="ficha">
            <fieldset style="border: solid #DDDDDD 1px">

                <legend style="margin-left: 1em; padding: 0.2em 0.8em ">REQUISITOS PARA LOS PROYECTOS DE TRANSMISIÓN DE CORRIENTE ALTERNA</legend>
                <div class="search-content3">
                    <div style="display:table">
                        <div style="display:table-row">
                            <div style="display: table-cell; text-align: right;">
                                <div style="display:table;" id="barra">
                                    <div style="display:table-row">
                                        <div class="content-action" style="display: table-cell;">
                                            <a href="#" class="btnActive" id="btnDescargarFormato" onclick="exportarTablaExceCheckBox(hotFichaB)">
                                                <div class="content-item-action">
                                                    <img src="~/Content/Images/bajar.png" /><br />
                                                    <span>Descargar</span>
                                                </div>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <table style="width:100%">
                    <tr>
                        <td class="registro-label" colspan="3"><div id="detalleFormatoHojaB" class="tableHandson"></div></td>
                    </tr>
                </table>
                <table style="width:auto">
                    <tr>
                        <td>Fecha (mes/año) de Puerta en Operación Comercial:</td>
                        <td class="registro-control">
                            <input type="text" id="txtFechaPuestaOperacion" name="txtFechaPuestaOperacion" />
                        </td>
                    </tr>
                </table>
            </fieldset>
        </div>
        <div id="FichaAdi" class="ficha">
            <fieldset style="border: solid #DDDDDD 1px">
                <legend style="margin-left: 1em; padding: 0.2em 0.8em ">Documentos</legend>

                <table style="width:100%">
                    <tr>
                    </tr>
                    <tr>
                        <td class="registro-label"></td>
                    </tr>
                </table>

                <table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tablaLinInfoAdicional" aria-describedby="Archivos">
                    <thead>
                        <tr>
                            <th scope="col">Descripción</th>
                            <th scope="col">Archivo</th>
                            <th scope="col">Acción</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </fieldset>
        </div>

    </div>
</div>
