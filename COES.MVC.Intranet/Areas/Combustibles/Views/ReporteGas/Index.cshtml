
@model COES.MVC.Intranet.Areas.Combustibles.Models.CombustibleGasModel

@{
    ViewBag.Title = "Reporte de envío Combustible";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link href="~/Areas/Combustibles/Content/Css/Gas/general.css" rel="stylesheet" />
<script src="~/Areas/Combustibles/Content/Scripts/Gas/reporteGeneral.js?v=1.0"></script>

<div class="form-title">
