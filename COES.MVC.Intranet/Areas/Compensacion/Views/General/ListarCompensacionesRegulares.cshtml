@model COES.MVC.Intranet.Areas.Compensacion.Models.CompensacionesRegularesGeneralModel

<form id="Form1" method="post" enctype="multipart/form-data">
    <table class="pretty tabla-icono" id="tabla">
        <thead>
            <tr>       
                <th>Empresa</th>
                <th>Calificación</th>
                <th>Modo Operación</th>
                <th>Inicio</th>
                <th>Fin</th>
                <th>Potencia</th>
                <th>Consumo</th>
                <th>CVC</th>
                <th>CVNC</th>
                <th>CV</th>
                <th>Compensación (S/.) </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListCompensacionesRegulares)
            {
                <tr class="menu-contextual">     
                    <td style="text-align:left">@item.Emprnomb</td>
                    <td style="text-align:left">@item.Subcausadesc</td>
                    <td style="text-align:left">@item.Gruponomb</td>
                    <td>@(((DateTime)item.Crcbehorini).ToString("dd/MM/yyyy HH:mm"))</td>
                    <td>@(((DateTime)item.Crcbehorfin).ToString("dd/MM/yyyy HH:mm"))</td>                   
                    <td style="text-align:right">@item.Crcbepotencia</td>
                    <td style="text-align:right">@item.Crcbeconsumo</td>
                    <td style="text-align:right">@item.Crcbecvc</td>
                    <td style="text-align:right">@item.Crcbecvnc</td>
                    <td style="text-align:right">@item.Crcbecvt</td>
                    <td style="text-align:right">@item.Crcbecompensacion</td>
                </tr>
            }
        </tbody>
    </table>
</form>

