@model COES.MVC.Intranet.Areas.Compensacion.Models.General.IncrementosReduccionesModel

<form id="Form1" method="post" enctype="multipart/form-data">
    <table class="pretty tabla-icono" id="tabla">
        <thead>
            <tr>
                <th>Modo Operación</th>
                <th>Fecha</th>
                <th>Incrementos</th>
                <th>Reducciones</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListVceArrparIncredGenDTO)
            {
                <tr class="menu-contextual">                
                    <td>@item.Gruponomb</td>
                    <td>@item.ApinrefechaDesc</td>
                    <td>@item.Apinrenuminc</td>
                    <td>@item.Apinrenumdis</td>
                    <td>
                        <a href="JavaScript:modificarIncrementoReduccion('@item.PecaCodi', '@item.Grupocodi', '@item.ApinrefechaDesc');" title="Modificar">
                            <img src="~/Content/Images/btn-edit.png" alt="">
                        </a>
                        <a href="JavaScript:eliminarIncrementoReduccion('@item.PecaCodi', '@item.Grupocodi', '@item.ApinrefechaDesc');" title="Eliminar">
                            <img src="~/Content/Images/btn-cancel.png" alt="">
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</form>

