<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th>Periodo Inicio</th>
            <th>Periodo Fin</th>
            <th>Central </th>
            <th>URS</th>
            <th>PBF<sub>subir</sub> (MW)</th>
            <th>[2021].PBF<sub>bajar</sub> (MW)</th>
            <th>PrBF<sub>subir</sub> (S/. / MW-mes)</th>
            <th>[2021].PrBF<sub>bajar</sub> (S/. / MW-mes)</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaProvisionbase)
        {
            string sPK = item.Vcrpbcodi +"_";
        <tr id="fila_@sPK">
            <td class="text_left" style="text-align:left">@item.Vcrpbperiodoini.ToString("dd/MM/yyyy")</td>
            <td class="text_left" style="text-align:left">@item.Vcrpbperiodofin.ToString("dd/MM/yyyy")</td>
            <td class="text_left" style="text-align:left">@item.Equinomb</td>
            <td class="text_left" style="text-align:left">@item.Gruponomb</td>
            <td class="text_left" style="text-align:right">@item.Vcrpbpotenciabf</td>
            @*//ASSETEC: 202010*@
            <td class="text_left" style="text-align:right">@item.Vcrpbpotenciabfb</td>
            <td class="text_left" style="text-align:right">@item.Vcrpbpreciobf</td>
            <td class="text_left" style="text-align:right">@item.Vcrpbpreciobfb</td>
            <td style="width:30px;"><a href="#" id="view_@sPK" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>
            <td>
                @if (Model.bEditar)
                {
                    <a href="@Url.Action("Edit", new { vcrpbcodi = item.Vcrpbcodi}) "><img src="@Url.Content("~/Content/Images/btn-edit.png") " title="Editar el registro" alt="Editar el registro" /></a>
                }
            </td>
            <td style="width:30px;">
                @if (Model.bEliminar)
                {
                    <a href="#" id="view_@sPK" class="delete"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a>
                }
            </td>
        </tr>
        }
    </tbody>
</table>
<div id="popup"></div>
