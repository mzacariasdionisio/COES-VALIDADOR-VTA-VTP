@model COES.MVC.Intranet.Areas.Coordinacion.Models.RegistroObservacionModel
<script src="~/Areas/Coordinacion/Content/Scripts/busquedacanal.js?v=1.28"></script>

<table border="0" class="pretty tabla-icono" id="tablaCanal">
    <thead
        <tr>
            <th>Seleccionar</th>
            <th>Empresa</th>
            <th>Zona</th>
            <th>Nombre</th>
            <th>ICCP</th>
            <th>Unidad</th>
            <th>Abrev</th>
            <th>Tipo</th>
            <th>Motivo</th>

        </tr>
    </thead>
    <tbody>
        @if (Model.ListadoCanal.Count > 0)
        {
            foreach (var item in Model.ListadoCanal)
            {
                <tr>
                    <td>
                        <input type="checkbox" id="cbSeleccionar" value="@item.Canalcodi|@item.Motivo|@item.Tiempo|@item.Calidad|@item.Caida" class="check" />
                    </td>
                    <td>@item.Emprnomb</td>
                    <td>@item.Zonanomb</td>
                    <td>@item.Canalnomb</td>
                    <td>@item.Canaliccp</td>
                    <td>@item.Canalunidad</td>
                    <td>@item.Canalabrev</td>
                    <td>@item.Canaltipo</td>
                    <td>@item.Motivo</td>
                </tr>
            }
        }
    </tbody>
</table>