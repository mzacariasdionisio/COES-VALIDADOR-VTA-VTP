            <th>Last User</th>
            <th>Last Date</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.ListaCongestionConjunto.Count > 0)
        {
            foreach (var item in Model.ListaCongestionConjunto)
            {
                <tr>
                    <td>
                        <a href="JavaScript:editarEjecutado(@item.Congescodi);"><img src="~/Content/Images/btn-edit.png" /></a>
                        <a href="JavaScript:eliminarEjecutado(@item.Congescodi);"><img src="~/Content/Images/btn-cancel.png" /></a>
                    </td>
                    <td>@item.Grulinnombre</td>
                    <td>
                        @if (item.Congesfecinicio != null)
                        {
                            <span>@(((DateTime)item.Congesfecinicio).ToString("dd/MM/yyyy HH:mm:ss"))</span>
                        }
                    </td>
                    <td>
                        @if (item.Congesfecfin != null)
                        {
                            <span>@(((DateTime)item.Congesfecfin).ToString("dd/MM/yyyy HH:mm:ss"))</span>
                        }
                    </td>
                    <td>@item.Lastuser</td>
                    <td>
                        @if (item.Lastdate != null)
                        {
                            <span>@(((DateTime)item.Lastdate).ToString("dd/MM/yyyy HH:mm:ss"))</span>
                        }
                    </td>
                </tr>
            }
        }
    </tbody>
</table>