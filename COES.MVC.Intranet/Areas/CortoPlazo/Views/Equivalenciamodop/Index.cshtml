@model COES.MVC.Intranet.Areas.CortoPlazo.Models.BusquedaCmEquivalenciamodopModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Areas/CortoPlazo/Content/Scripts/equivalenciamodop.js?v=1.15"></script>
<link href="~/Areas/CortoPlazo/Content/Css/css.css" rel="stylesheet" />
<script src="~/Content/Scripts/inputmaskmin.js"></script>

<div class="form-title">
    <div class="form-title">
        <div class="content-titulo">Equivalencia Modo Operaci&oacute;n - NCP</div>
        <div class="content-botonera">
            <input type="button" id="btnEditar" value="Edici&oacute;n tipo Excel" />
        </div>
    </div>
</div>

        <div class="content-hijo" id="mainLayout">
            <div class="search-content" style="margin-bottom:10px">
                <table class="content-tabla-search" style="width:auto">
                    <tr>
                        <td>Modo:</td>
                        <td>
                            <select id="cbGruponomb" name="Gruponomb" style="width:300px">
                                <option value="0">TODOS</option>
                                @foreach (var item in Model.ListaPrGrupo)
                                {
                                    <option value="@item.Grupocodi">@item.Gruponomb</option>
                                }
                            </select>
                        </td>

                        <td>
                            <input type="button" id="btnBuscar" value="Buscar" style="margin-left:0" />
                        </td>

                    </tr>

                </table>
            </div>

            <div class="content-tabla">
                <div class="tabla-header">
                    <div class="table-paginado" id="paginado"></div>
                </div>
                <div class="table-list" id="listado"></div>
            </div>
        </div>

<div id="popupEdicion" class="general-popup">
    <div><span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Equivalenciamodop</span></div>
    <div class="popup-text" id="contenidoEdicion">
    </div>
</div>
