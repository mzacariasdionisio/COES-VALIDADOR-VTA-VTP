@model COES.MVC.Intranet.Areas.CortoPlazo.Models.VersionReporteModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Areas/CortoPlazo/Content/Scripts/barradesenergizada.js?v=2"></script>
<script src="~/Content/Scripts/jquery.format.js"></script>
<link href="~/Areas/CortoPlazo/Content/Css/comparativo.css" rel="stylesheet" />

<div class="form-title">
    <div class="content-titulo">Reporte de Barras Desenergizadas</div>
    <div class="content-botonera">
        <input type="button" id="btnExportar" value='Exportar a Excel' />
    </div>
</div>

<div class="content-hijo">

    <div id="mensaje"></div>

    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Fecha inicio:</td>
                <td>
                    <input type="text" id="txtFechaInicio" value="@Model.FechaInicio" />
                </td>
                <td>Fecha fin:</td>
                <td>
                    <input type="text" id="txtFechaFin" value="@Model.FechaFin" />
                </td>
                <td>
                    <input type="button" id="btnConsultar" value="Consultar" />
                </td>
            </tr>
        </table>
    </div>

    <div id="listado">

    </div>

    <br />
    <br />
</div>
