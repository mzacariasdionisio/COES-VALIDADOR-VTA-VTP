@model COES.MVC.Intranet.Areas.CostoOportunidad.Models.PeriodoModel
<div class="popup-title" style="margin-bottom:10px"><span>Histórico de envío a liquidaciones</span></div>

<table border="0" class="pretty tabla-icono" id="tablaLiquidacion">
    <thead>
        <tr>
            <th>Periodo CO</th>
            <th>Versión CO</th>
            <th>Periodo aplicatio RSF </th>
            <th>Recálculo aplicativo RSF</th>
            <th>Usuario</th>
            <th>Fecha</th>           
        </tr>
    </thead>

    <tbody>
        @if (Model.ListadoEnvio.Count > 0)
        {
            foreach (var item in Model.ListadoEnvio)
            {
                <tr>                    
                    <td>@item.Periododesc</td>
                    <td>@item.Versiondesc</td>
                    <td>@item.Periodonomb</td>
                    <td>@item.Versionnomb</td>
                    <td>@item.Coenliusuario</td>  
                    <td>
                        @if (item.Coenlifecha != null)
                        {
                            <span>@(((DateTime)item.Coenlifecha).ToString("dd/MM/yyyy hh:mm:ss"))</span>
                        }
                    </td>
                </tr>
            }
        }
    </tbody>
</table>