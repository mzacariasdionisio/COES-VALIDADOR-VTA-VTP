@model COES.MVC.Intranet.Areas.DemandaPO.Models.DatosSirpitModel

<script src="~/Areas/DemandaPO/Content/Scripts/datosSirpit/datosSubestacion.js"></script>
<div id="main"
     data-msg="@Model.Mensaje"
     data-tpo="@Model.TipoMensaje"
     style="margin-bottom: 5px;">
    <div>
        <select id="selSubestacion" style="width:180px">
            @foreach (var item in Model.cboSubestaciones)
            {
            <option value="@item.Dposubcodi">@item.Dposubcodiexcel - @item.Dposubnombre</option>
            }
        </select>
    </div><br />
    <table id="dt"
           class="pretty tabla-icono"></table>
</div>