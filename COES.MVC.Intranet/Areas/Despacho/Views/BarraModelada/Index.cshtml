@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Areas/Despacho/Content/Scripts/IndexBarraModelada.js"></script>
<div class="form-title">
    <div class="content-titulo">Barras Modeladas</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevo" value="Nueva Barra Modelada" />
    </div>
</div>
<div class="content-hijo">
    <div class="action-message" id="mensaje"></div>
    <div id="listado"></div>
</div>
<div id="popupDetalle">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Detalle de Barra Modelada</span></div>
    <div id="detBarraModelada"></div>
</div>
<div id="popupNuevo" class="popupGeneral">
    <link href="~/Content/Css/datatables.css" rel="stylesheet" />
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Nueva Barra Modelada</span>
    </div>
    <div id="nuevaBarraModelada"></div>
</div>
<div id="popupEditar" class="popupGeneral">
    <link href="~/Content/Css/datatables.css" rel="stylesheet" />
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title">
        <span>Editar Barra Modelada</span>
    </div>
    <div id="editarBarraModelada"></div>
</div>