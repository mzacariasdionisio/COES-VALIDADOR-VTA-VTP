<link href="~/Content/Css/tabs.css" rel="stylesheet" />

<link href="~/Areas/Equipamiento/Content/Css/fichatecnica.css?v=1.2.0" rel="stylesheet" />
<script src="~/Areas/Equipamiento/Content/Scripts/fichatecnica/FTFormatoextranet/formatocim.js?v=1.2.2"></script>
<script src="~/Areas/Equipamiento/Content/Scripts/fichatecnica/FTFormatoextranet/formatoftvista.js?v=1.2.1"></script>
<script src="~/Areas/Equipamiento/Content/Scripts/fichatecnica/FTFormatoextranet/formularioFTdetalleVista.js?v=1.2.2"></script>
<script src="~/Areas/Equipamiento/Content/Scripts/fichatecnica/FTFormatoextranet/formularioArchivosDato.js?v=1.2.1"></script>

<div class="form-title">
    <div class="content-titulo">Conexión, Integración y Modificación</div>
    <div style="position: relative; height: 100px;">
        <a href="#" id="btnManualUsuario" style="text-decoration: none; position: absolute; top: 63px; right: 20px;">
            <div style="text-align: center; line-height: 15px; font-size: 12px;">
                <img src="~/Content/Images/prn-ico-manual.png" style="height: 25px;"><br>
                <span style="color: #278CCC;">Manual de Usuario</span>
            </div>
        </a>
    </div>
</div>

<div class="content-hijo" id="mainLayout">
    <div id="mensaje"></div>

    <div id="tab-container" class='tab-container'>
        <ul class='etabs'>
            <li class='tab'><a href="#tabLista">Listado</a></li>
            <li class='tab'><a href="#tabDetalle">Detalle</a></li>
        </ul>
        <div class='panel-container'>

            <div id="tabLista">

                <div id="vistaListado">

                    <div id="div_listado" class="content-tabla">

                    </div>
                </div>

            </div>

            <div id="tabDetalle">

                <div id="div_filtro" class="content-tabla">

                </div>

                <div id="div_detalle" class="content-tabla">

                </div>
            </div>

        </div>
    </div>

</div>

<div>
    <input type="hidden" id="hdFamcodiSeleccionado" />
    <input type="hidden" id="hdCatecodiSeleccionado" />
    <input type="hidden" id="hdIdFichaSeleccionado" />
    <input type="hidden" id="hdEstadoFichaSeleccionado" />
    <input type="hidden" id="hdTipoFichaSeleccionado" />
    <input type="hidden" id="hfJsonTree" />
</div>

<div id="treeContainer" style="display:none;">
    @*<table id="treeFT">

        </table>*@
</div>

<div id="popupAnotacion" class="general-popup" style="z-index: 9999999999; width: 600px ">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Detalle de Parámetro</span></div>
    <input type="hidden" id="hfRegFtitcodi" value="" />
    <div style="padding: 20px;">
        <input id="campo_anotacion_editar" type="text" style="width:560px">
    </div>

    <div id="bloqueBotones">
        <div style="width: 150px; margin: 0 auto; text-align: center; margin-top:15px; ">
            <input type="submit" id="btnGuardarAnotacion" value="Guardar">
            <input type="button" id="btnCancelarAnotacion" value="Cancelar" onclick="cerrarPopup('popupAnotacion')">
        </div>
    </div>
</div>

<div id="popupInstructivo" class="general-popup" style="z-index: 9999999999; width: 600px ">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Intructivo de llenado</span></div>      
    <div id="bloqueDato">
        <div id="mensaje_instructivo" style="padding: 10px; text-align: center;">

        </div>
        
    </div>
</div>
