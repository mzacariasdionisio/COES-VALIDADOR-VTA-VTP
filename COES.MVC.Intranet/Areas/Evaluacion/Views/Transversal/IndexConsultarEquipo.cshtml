@model COES.MVC.Intranet.Areas.Evaluacion.Models.TransversalModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .text-wrap {
        white-space: normal;
    }

    .width-200 {
        width: 200px;
    }
</style>
<div class="form-title">
    <div class="content-titulo">Consultar tipo de equipo</div>
    <div class="content-botonera">
        <input type="button" value="Regresar" id="btnRegresar" name="btnRegresar" class="form-action" style="margin-top: 15px" />
    </div>

</div>
<div id="mensaje"></div>
<input type="hidden" id="hCodigoId" name="hCodigoId" style="width: 96%;" value="@Model.CodigoId" maxlength="20" />
<input type="hidden" id="hParthReturn" name="hParthReturn" style="width: 96%;" value="@Model.PathReturn" maxlength="20" />

<div class="content-hijo" id="mainLayout">
    <div id="idpanel" style="overflow:auto; background-color:#fff">

        <div class="search-content" style="margin-bottom:0px; padding:15px">
            <table class="content-tabla-search" style="width:auto">
                <tr>
                    <th style="width: 6%;"></th>
                    <th style="width: 8%;"></th>
                    <th style="width: 7%;"></th>
                    <th style="width: 10%;"></th>
                    <th style="width: 8%;"></th>
                    <th style="width: 13%;"></th>
                    <th style="width: 8%;"></th>
                    <th style="width: 25%;"></th>
                    <th style="width: 6%;"></th>
                    <th style="width: 9%;"></th>
                </tr>
                <tr id="SeccionProgramaExistente">
                    <td>Código ID: </td>
                    <td>
                        <input type="text" id="fCodigoId" name="fCodigoId" style="width: 96%;" value="@Model.CodigoId" disabled maxlength="20" />
                    </td>
                    <td>Código: </td>
                    <td>
                        <input type="text" id="fCodigo" name="fCodigo" style="width: 96%;" value="@Model.Codigo" disabled maxlength="20" />
                    </td>
                    <td>Ubicación: </td>
                    <td>
                        <input type="text" id="fUbicacion" name="fUbicacion" style="width: 96%;" value="@Model.Ubicacion" disabled maxlength="20" />
                    </td>
                    <td>Empresa: </td>
                    <td>
                        <input type="text" id="fEmpresa" name="fEmpresa" style="width: 96%;" value="@Model.Empresa" disabled maxlength="20" />
                    </td>
                    <td>Área: </td>
                    <td>
                        <input type="text" id="fArea" name="fArea" style="width: 96%;" value="@Model.Area" disabled maxlength="20" />
                    </td>
                </tr>
            </table>

        </div>
        <br />
        <div class="content-hijo" id="mainLayout">
            <div id='lista'></div>
        </div>
    </div>
</div>

<!--Menu contextual-->
<link href="~/Areas/Evaluacion/Content/Css/css.css" rel="stylesheet" />
<script src="~/Content/Scripts/contextmenu.js"></script>
<link href="~/Content/Css/contextmenu.css" rel="stylesheet" />
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />
<script src="~/Areas/Evaluacion/Content/Scripts/ConsultarEquipo.js"></script>
<script src="~/Areas/Evaluacion/Content/Scripts/util.js"></script>
<script src="~/Areas/Evaluacion/Content/Scripts/json2.js"></script>
<script src="~/Content/Scripts/plupload.full.min.js"></script>
<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<script src="~/Content/Scripts/inputmaskmin.js"></script>
<link href="~/Content/Css/form.css" rel="stylesheet" />


