@model COES.MVC.Intranet.Areas.Eventos.Models.AnalisisFallasModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

    SelectList slResponsable = new SelectList(ViewBag.ResponsableEvento, "value", "text", Model.oAnalisisFallaDTO.AFERESPONSABLE);  // new SelectList(ViewBag.ResponsableEvento, "value", "text");
    SelectList slResponsbleReunion = new SelectList(ViewBag.ResponsableReunion, "RESPCOD", "RESPNAME");
    SelectList slAsistenteResponsble = new SelectList(ViewBag.AsistenteResponsable, "Emprcodi", "Emprnomb");
}

<style>
    .aPopupEmpresa {
        position: absolute;
        cursor: pointer;
    }

    .aPopupEmpresaPropietaria {
        position: absolute;
        cursor: pointer;
    }

    #tblEmpresaBuscar {
        border-collapse: collapse;
    }

        #tblEmpresaBuscar tr {
            background-color: transparent;
        }

            #tblEmpresaBuscar tr:hover td {
                cursor: pointer;
                background-color: #f3f8aa;
            }

    #tblFuerzaMayor {
        border-collapse: collapse;
    }

        #tblFuerzaMayor tr {
            background-color: transparent;
        }

            #tblFuerzaMayor tr:hover td {
                cursor: pointer;
                background-color: #f3f8aa;
            }

    .Selected {
        background-color: #ffd800;
    }

    #busquedaEquipo {
        top: 40px !important;
    }

    .modal-loading-background {
        height: 100vh;
        width: 100vw;
        z-index: 999;
        position: fixed;
        top: 0;
        left: 0;
        display: flex;
        align-items: center;
        justify-content: center;
        background: rgb(0, 0, 0, 0.4);
        visibility: hidden;
    }

    .modal-loading {
        display: flex;
        justify-content: center;
        align-items: center;
        background-color: #fff;
        border: 3px solid #3D90CB;
        border-radius: 5px;
        z-index: 1000;
        gap: 40px;
        height: 100px;
        width: 250px;
    }

</style>

<link href="~/Content/Css/popup.css" rel="stylesheet" />
<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<link href="~/Content/Css/form.css" rel="stylesheet" />

<div class="modal-loading-background">
    <div class="modal-loading">
        <div class="estado-image"><img src="~/Content/Images/loading.gif" alt="Carga" /></div>
        <div class="estado-text">Cargando...</div>
    </div>
</div>

<div class="form-title">
    <div class="content-titulo">
        Análisis de Falla - Detalle
    </div>
    <div class="content-botonera">
        @if (Model.grabar)
        {
            <input type="button" id="btngrabargeneral" value='Grabar' />
            <div id="loadingcarga" class="loading">
                <div class="estado-image"><img src="~/Content/Images/loading.gif" alt="Carga" /></div>
                <div class="estado-text">Cargando...</div>
            </div>
        }
    </div>
</div>

<div class="content-hijo" id="mainLayout">
    <div id="tab-container" class="tab-container" style="min-height:700px">
        <ul class='etabs'>
            <li class='tab'><a href="#EventoCtaf">Evento CTAF</a></li>
            <li class='tab' id="IdEventoSco"><a href="#Evento">Evento SCO</a></li>
            <li class='tab'><a href="#Reunion">Reunion CTAF</a></li>
            <li class='tab'><a href="#Informe">Informe Técnico</a></li>
            <li class='tab'><a href="#Compensacion">Compensación</a></li>
            <li class='tab'><a id="aReconsideraciones" href="#Reconsideracion">Reconsideraciones/Apelaciones</a></li>
        </ul>
        <div class="panel-container">
            <div id="EventoCtaf">
                <div class="content-registro" style="padding-top:40px;">
                    <table style="width:100%;">
                        <tr>
                            <td style="max-width: 4px;" class="registro-label">Codigo</td>
                            <td class="registro-label">
                                EV -&nbsp;&nbsp;
                                @if (Model.grabar)
                                {
                                    <input style="width:50px;" id="txtAFECORRCtaf" value="@Model.oAnalisisFallaDTO.AFECORR" />
                                }
                                else
                                {
                                    <input style="width:50px;" id="txtAFECORRCtaf" value="@Model.oAnalisisFallaDTO.AFECORR" disabled />
                                }

                                -<input id="txtEventoAnioCtaf" style="width:50px" disabled value="@Model.oAnalisisFallaDTO.AFEANIO" />
                            </td>
                        </tr>
                    </table>
                </div>
                <div style="clear:both; height:5px;"></div>
                <div class="content-registro" style="width:auto">
                    <table border="0" class="pretty tabla-adicional" cellspacing="0" width="100%" id="tabla" aria-describedby="report">
                        <thead>
                            <tr>
                                <th scope="col"></th>
                                <th scope="col">Tipo</th>
                                <th scope="col">Empresa</th>
                                <th scope="col">Ubicación</th>
                                <th scope="col">Familia</th>
                                <th scope="col">Equipo</th>
                                <th scope="col">Inicio</th>
                                <th scope="col">CIER</th>
                                <th scope="col">Causa</th>
                                <th scope="col">Interrup.</th>
                                <th scope="col">R</th>
                                <th scope="col">Descripción</th>
                                <th scope="col">Final</th>
                                <th scope="col">Usuario</th>
                                <th scope="col">Fecha</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.ListaEventosSco)
                            {
                                var id = "cb" + item.EVENCODI;
                                var _valor = @item.EVENCODI + "|" + @item.AFECODI;

                                <tr>
                                    <td>
                                        <input type="checkbox" id="@id" class="chCtaf" onchange="Check(this)" value="@_valor" />
                                    </td>
                                    <td>@item.TIPOEVENABREV</td>
                                    <td>@item.EMPRNOMB</td>
                                    <td>@item.AREADESC</td>
                                    <td>@item.FAMABREV</td>
                                    <td>@item.EQUIABREV</td>
                                    <td>
                                        @if (item.EVENINI != null)
                                        {
                                            <span>@(((DateTime)item.EVENINI).ToString("dd/MM/yyyy HH:mm:ss"))</span>
                                        }
                                    </td>
                                    <td>@item.CAUSAEVENABREV</td>
                                    <td>@item.SUBCAUSAABREV</td>
                                    <td>@item.EVENINTERRUP</td>
                                    <td>@item.EVENRELEVANTE</td>
                                    <td>@item.EVENASUNTO</td>
                                    <td>
                                        @if (item.EVENFIN != null)
                                        {
                                            <span>@(((DateTime)item.EVENFIN).ToString("dd/MM/yyyy HH:mm:ss"))</span>
                                        }
                                    </td>
                                    <td>@item.LASTUSER</td>
                                    <td>
                                        @if (item.LASTDATE != null)
                                        {
                                            <span>@(((DateTime)item.LASTDATE).ToString("dd/MM/yyyy HH:mm"))</span>
                                        }
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>

                <div class="content-registro" style="width:auto">
                    <div style="clear:both; height:15px;"></div>
                    <div class="formato-titulo popup-title" style="color: #278CCC; margin-bottom:0px;"><span>Informe de empresa involucradas</span></div>
                    <div style="clear:both; height:5px;"></div>
                    @*<div id="listadoSco"></div>*@
                    @*<div class="bodyexcel" id='listadoScoHandsontable' style="overflow-y: auto"></div>*@
                    <div id='listadoScoHandsontable'></div>
                    <div style="clear:both; margin-bottom:40px"></div>
                </div>

                <div class="content-registro" style="width:auto">
                    <div style="padding-top:20px;">
                        <table style="width:100%;">
                            <tr>
                                <td class="registro-label">Responsable</td>
                                <td class="registro-control">@Html.DropDownList("cboEventoResponsable", slResponsable, new { @style = "width:250px" })</td>

                                <td class="registro-label">Estado:</td>
                                <td class="registro-control">

                                    @if (Model.oAnalisisFallaDTO.AFEESTADO == "A")
                                    {
                                        <input name="chkEstado" type="radio" /><span>Vigente</span>
                                        <input id="chkEstadoAnulado" name="chkEstado" type="radio" checked /><span>Anulado</span>
                                        <input id="chkEstadoArchivado" name="chkEstado" type="radio" /><span>Archivado</span>
                                    }
                                    else if (Model.oAnalisisFallaDTO.AFEESTADO == "X")
                                    {
                                        <input name="chkEstado" type="radio" /><span>Vigente</span>
                                        <input id="chkEstadoAnulado" name="chkEstado" type="radio" /><span>Anulado</span>
                                        <input id="chkEstadoArchivado" name="chkEstado" type="radio" checked /><span>Archivado</span>
                                    }
                                    else
                                    {
                                        <input name="chkEstado" type="radio" checked /><span>Vigente</span>
                                        <input id="chkEstadoAnulado" name="chkEstado" type="radio" /><span>Anulado</span>
                                        <input id="chkEstadoArchivado" name="chkEstado" type="radio" /><span>Archivado</span>

                                    }
                                </td>
                                <td class="registro-control"><textarea id="txtAFEESTADOMOTIVO" rows="3" cols="40">@Model.oAnalisisFallaDTO.AFEESTADOMOTIVO</textarea></td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <div id="Evento">
                <div id="DetalleEventoSel"></div>
            </div>
            <div id="Reunion">
                <div style="padding-top:20px;">
                    <div class="formato-titulo popup-title" style="color: #278CCC; margin-bottom:0px;"><span>Convocatoria</span></div>
                    <div style="clear:both; height:5px;"></div>

                    <div class="content-registro" style="width:auto">
                        <table cellpadding="5" style="width:100%;">
                            <tr>
                                <td colspan="4" class="registro-label" style="font-weight:bold">Fecha de Citación</td>
                                <td colspan="4" class="registro-label" style="font-weight:bold">Fecha de Reunión</td>
                                <td colspan="4" class="registro-label" style="font-weight:bold">Tipo de Reunión</td>
                                <td colspan="2" id="lblSala" class="registro-label" style="font-weight:bold">Sala</td>
                            </tr>
                            <tr>
                                <td rowspan="2" class="registro-label">Nominal</td>
                                <td rowspan="2" class="registro-control"><input type="text" id="dtCitacionNominal" value="@Model.oAnalisisFallaDTO.AFECITFECHANOMINALstr" style="width:100px" disabled /></td>
                                <td rowspan="2" class="registro-label">Elaboración</td>
                                <td rowspan="2" class="registro-control"><input type="text" id="dtCitacionElaboracion" value="@Model.oAnalisisFallaDTO.AFECITFECHAELABstr" style="width:100px" /></td>
                                <td rowspan="2" class="registro-label">Nominal</td>
                                <td rowspan="2" class="registro-control"><input type="text" id="dtReunionNominal" value="@Model.oAnalisisFallaDTO.AFEREUFECHANOMINALstr" style="width:100px" disabled /></td>
                                <td rowspan="2" class="registro-label">Programada</td>
                                <td rowspan="2" class="registro-control"><input type="text" id="dtReunionProgramada" value="@Model.oAnalisisFallaDTO.AFEREUFECHAPROGstr" style="width:100px" />&nbsp;<input type="text" id="dtReunionHora" placeholder="__:__" value="@Model.oAnalisisFallaDTO.AFEREUHORAPROG" style="width:100px" /></td>
                                <td colspan="4" class="registro-control" style="vertical-align: central; text-align: center;">
                                    <select id="cboReunionTipoReunion" style="width:130px">
                                        <option value="PRESENCIAL">PRESENCIAL</option>
                                        <option value="NO PRESENCIAL">NO PRESENCIAL</option>
                                    </select>
                                    <input type="hidden" id="hfReunionTipoReunion" value="@Model.oAnalisisFallaDTO.AFECONVTIPOREUNION" />
                                </td>
                                <td rowspan="2" class="registro-control">
                                    @if (Model.grabar && Model.oAnalisisFallaDTO.AFSALA > 0)
                                    {
                                        <input type="text" id="txtSala" value="@Model.oAnalisisFallaDTO.AFSALA" style="width:50px" />
                                        <input type="button" id="btnReservarSala" value='Reservar Sala' />
                                    }
                                    else if (Model.grabar && Model.oAnalisisFallaDTO.AFSALA == 0)
                                    {
                                        <input type="text" id="txtSala" value="" style="width:50px" />
                                        <input type="button" id="btnReservarSala" value='Reservar Sala' />
                                    }
                                </td>
                            </tr>
                            <tr id="HPresencial">
                                <td class="registro-label">Hora Inicio</td>
                                <td class="registro-control" style="vertical-align:central">
                                    <input type="time" class="without_ampm" id="horaInicioReunion" name="HorarioReunion" data-mask="00:00" data-mask-selectonfocus="true" value="@Model.oAnalisisFallaDTO.AFEREUHORINI" />
                                </td>
                                <td class="registro-label">Hora Fin</td>
                                <td class="registro-control" style="vertical-align:central">
                                    <input type="time" class="without_ampm" id="horaFinReunion" name="HorarioReunion" data-mask="00:00" data-mask-selectonfocus="true" value="@Model.oAnalisisFallaDTO.AFEREUHORFIN" />
                                </td>
                            </tr>
                        </table>

                        <div style="padding-top:20px">
                            <table style="width:100%">
                                <tr>
                                    <td class="registro-label">Empresa Involucrada</td>
                                    <td class="registro-control">
                                        @*<input type="text" id="txtReunionEmpresaInvolucrada" data-name="" data-id="" /> &nbsp;&nbsp;<a class="aPopupEmpresa" data-target="txtReunionEmpresaInvolucrada"><img src="~/Content/Images/btn-open.png" /></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" id="btnReunionAgregar" value='Agregar' />*@
                                        <select id="txtReunionEmpresaInvolucrada" autofocus="autofocus" autocorrect="off" autocomplete="off">
                                            <option value="" selected="selected"></option>
                                        </select>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                                        @if (Model.grabar)
                                        {
                                            <input type="button" id="btnReunionAgregar" value='Agregar' />
                                        }
                                    </td>
                                </tr>
                            </table>
                            <div style="padding-top:20px">
                                <table style="width:100%" border="0" class="pretty tabla-adicional">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>EMPRESA</th>
                                            <th>LAST USER</th>
                                            <th>LASR DATE</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tbodyReunionEmpresaInvolucrada">

                                        @if (Model.lsEmpresaInvolucradaReunion != null)
                                        {
                                            foreach (var item in Model.lsEmpresaInvolucradaReunion)
                                            {
                                                <tr id="@string.Format("rowReunionEmpresaInvolucrada{0}",item.EMPRCODI)" data-id="@item.EMPRCODI">
                                                    <td>
                                                        @if (Model.grabar)
                                                        {
                                                            <a href="#" onclick="EliminarEmpresaInvolucradaReunion(@item.EMPRCODI)">
                                                                <img src="~/Content/Images/btn-cancel.png" style="margin-top: 5px;" />
                                                            </a>
                                                        }

                                                    </td>
                                                    <td>@item.EMPRNOMB</td>
                                                    <td>@item.LASTUSER</td>
                                                    <td>@item.LASTDATEstr</td>
                                                </tr>
                                            }
                                        }
                                    </tbody>
                                </table>
                                @if (Model.lsEmpresaInvolucradaReunion != null && Model.lsEmpresaInvolucradaReunion.Count > 0)
                                {
                                    <input type="hidden" id="hfReunionEmpresaInvolucradaMaxId" value="@Model.lsEmpresaInvolucradaReunion.Max(x=> x.EMPRCODI)" />
                                }
                                else
                                {
                                    <input type="hidden" id="hfReunionEmpresaInvolucradaMaxId" value="0" />
                                }

                            </div>
                            <div style="padding-top:20px">
                                <table style="width:100%">
                                    <tr>
                                        <td class="registro-control">

                                            @if (Model.grabar)
                                            {

                                                <input type="button" id="btnReunionAgregarEmpresaInvolucrada" onclick="BajarCitacion();" value='Generar Citación' />
                                            }
                                        </td>
                                        <td class="registro-control">

                                            @if (Model.grabar)
                                            {
                                                <input type="file" id="flCitacionReunion" accept=".pdf" style="display:none;" />
                                                <input name="btnSubirCitacionReunion" type="button" id="btnSubirCitacionReunion" value="Cargar Archivo">
                                            }

                                            <label id="lblCitacionReunion">@Model.oAnalisisFallaDTO.AFECONVCITACIONFECHAstr</label>

                                        </td>


                                        <td class="registro-control">
                                            <a href="#" onclick="VerCitacionReunion()" id="btnVerCitacionReunion" style="display:none;">
                                                <img src="~/Content/Images/btn-open.png" style="margin-top: 5px;" />
                                            </a>
                                        </td>
                                        <td class="registro-control">
                                            @if (Model.grabar)
                                            {
                                                <a href="#" onclick="EliminarCitacionReunion()" id="btnEliminarCitacionReunion" style="display:none;">
                                                    <img src="~/Content/Images/btn-cancel.png" style="margin-top: 5px;" />
                                                </a>
                                            }

                                        </td>

                                        <td class="registro-control">
                                            @if (Model.grabar)
                                            {
                                                <input type="button" id="btnEnviarCorreo" value='Enviar Correo' />
                                            }
                                        </td>
                                    </tr>
                                </table>
                            </div>
                            <div style="padding-top:20px">
                                <div class="formato-titulo popup-title" style="color: #278CCC; margin-bottom:0px;"><span>Reunión CTAF</span></div>
                                <div style="clear:both; height:5px;"></div>
                                <div id="Responsable_NoPresencial" style="padding-top:20px">
                                    <table style="width:100%">
                                        <tr>
                                            <td class="registro-label">Responsable</td>
                                            <td class="registro-control">
                                                @Html.DropDownList("cboReunionResponsable", slResponsbleReunion, new { @style = "width:250px" })
                                                @if (Model.grabar)
                                                {
                                                    <input type="button" id="btnReunionResponsableAgregar" value='Agregar' />
                                                }
                                            </td>
                                        </tr>
                                    </table>

                                    <div style="padding-top:20px">
                                        <table style="width: 100%;" border="0" class="pretty tabla-adicional">
                                            <thead>
                                                <tr>
                                                    <th></th>
                                                    <th>RESPONSABLE DEL ACTA DE INFORME CTAF </th>
                                                </tr>
                                            </thead>
                                            <tbody id="tbodyReunionEmpresaCTAF">

                                                @if (Model.lsReunionResponsable != null)
                                                {
                                                    foreach (var item in Model.lsReunionResponsable)
                                                    {
                                                        <tr id="@string.Format("rowReunionResponsable{0}",item.RESEVENCODI)" data-id="@item.RESEVENCODI">
                                                            <td>
                                                                @if (Model.grabar)
                                                                {
                                                                    <a href="#" onclick="EliminarReunionResponsable(@item.RESEVENCODI)">
                                                                        <img src="~/Content/Images/btn-cancel.png" style="margin-top: 5px;" />
                                                                    </a>
                                                                }

                                                            </td>
                                                            <td>@item.RESPNAME</td>
                                                        </tr>
                                                    }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                                <div id="AsistenteResponsable_Presencial" style="padding-top: 20px;">
                                    <a id="cargarAut" style="display:none" onclick='cargarAut()'>cargar automaticamente</a>
                                    <div class="formato-titulo popup-title" style="color: #278CCC; margin-bottom:0px;"><span>Asistencia de los representantes</span></div>
                                    <div style="clear:both; height:5px;"></div>
                                    <div style="padding-top:20px">
                                        <table style="width:100%">
                                            <tr>
                                                <td class="registro-label">Empresa</td>
                                                <td class="registro-control" style="width: 300px !important;">
                                                    @Html.DropDownList("cboAsistenteResponsable", slAsistenteResponsble, new { @style = "width:250px" })
                                                </td>
                                                <td class="registro-label">Asistente</td>
                                                <td class="registro-control">
                                                    <input type="text" id="txtAsistenteResponsable" value="" style="width: 250px; display: inline-block;">
                                                    @if (Model.grabar)
                                                    {
                                                        <input type="button" id="btnAsistenteResponsableAgregar" value='Agregar' />
                                                    }
                                                </td>
                                            </tr>
                                        </table>
                                        <div style="padding-top:20px">
                                            <div id='listadoAsistenteHandsontable'></div>
                                        </div>

                                    </div>
                                </div>


                                <table cellpadding="8" style="width:100%">
                                    <tr>
                                        <td></td>
                                        <td></td>
                                        <td></td>
                                        @*<td>Observaciones de la Reunión</td>*@
                                    </tr>
                                    <tr>
                                        <td class="registro-control">
                                            @if (Model.grabar)
                                            {
                                                <input type="button" id="btnGenerarActa" onclick="BajarActa();" value='Generar Acta' />
                                            }
                                        </td>
                                        <td class="registro-label">
                                            SELECCIONAR ACTA CT-AF
                                        </td>
                                        <td class="registro-control">
                                            @if (Model.grabar)
                                            {
                                                <input type="file" accept=".pdf" id="flActaReunion" style="display:none;" />
                                                <input name="btnSubirActaReunion" type="button" id="btnSubirActaReunion" value="Cargar Archivo">
                                            }
                                            <label id="lblActaReunion">@Model.oAnalisisFallaDTO.AFERCTAEACTAFECHAstr</label>
                                        </td>
                                        <td class="registro-control">
                                            @*<a href="#" onclick="VerActaReunion()" id="btnVerActaReunion" style="display:none;">
                                                    <img src="~/Content/Images/btn-open.png" style="margin-top: 5px;" />
                                                </a>*@
                                            <p>Fecha límite de atención a comentarios:</p>
                                        </td>
                                        <td class="registro-control">
                                            @*@if (Model.grabar)
                                                {
                                                    <a href="#" onclick="EliminarActaReunion()" id="btnEliminarActaReunion" style="display:none;">
                                                        <img src="~/Content/Images/btn-cancel.png" style="margin-top: 5px;" />
                                                    </a>
                                                }*@
                                        </td>
                                        <td class="registro-control" rowspan="2">
                                            @*<textarea id="txtAFERCTAEOBSERVACION" rows="5" cols="40">@Model.oAnalisisFallaDTO.AFERCTAEOBSERVACION</textarea>*@
                                            <input type="button" id="btnEnviarCorreoActa" value='Enviar Correo' />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td class="registro-control">
                                            @if (Model.grabar)
                                            {
                                                <input type="button" onclick="BajarInformeCTAF();" value='Generar Informe CTAF' />
                                            }
                                        </td>
                                        <td class="registro-label">SELECCIONAR INFORME CT-AF</td>
                                        <td class="registro-control">
