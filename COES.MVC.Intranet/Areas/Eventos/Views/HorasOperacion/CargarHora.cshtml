@model COES.MVC.Intranet.Areas.Eventos.Models.HoraModel

<script type="text/javascript">
    $(function () {
        $('#btnAceptarHora').click(function () {
            agregarHora();
        });
    });
</script>

<input type="text" id="txtHoraInicio" value="@Model.TxtInicio" />
<input type="text" id="txtHoraFin" value="@Model.TxtFin" />
<input type="button" id="btnAceptarHora" value="Aceptar" />
<input type="hidden" id="hfIndicadorEdicion" value="@Model.IndicadorEdicion" />
<input type="hidden" id="hfIndice" value="@Model.Indice" />