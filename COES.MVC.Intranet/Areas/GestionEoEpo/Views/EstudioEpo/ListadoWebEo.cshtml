@model  IEnumerable<COES.Dominio.DTO.Sic.EpoEstudioEoDTO>

<style>
    .text-left {
        text-align: left !important;
    }

    .text-right {
        text-align: right !important;
    }

</style>

<table border="0" class="pretty tabla-icono" cellspacing="0" width="100%" id="tabla">
    <thead>
        <tr>
            <th>Código<br />Proyecto</th>
            <th>Código<br />Estudio</th>
            <th>Nombre Estudio</th>
            <th>Responsable Estudio</th>
            <th>Fecha<br />Presentación EO</th>
            <th>Fecha<br />Conformidad EO</th>
            <th>Gestor del Proyecto</th>
            <th>Estado</th>
            <th>Resumen<br />Ejecutivo</th>
            <th>Vigencia</th>
            <th>Gestión</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td width="8%" class="text-left">@item.Esteocodiproy</td>
            <td width="8%" class="text-left">@item.Esteocodiusu</td>
            <td width="8%" class="text-left">@item.Esteonomb</td>
            <td width="8%" class="text-left">@item.Username</td>
            <td width="8%">@(item.Esteofechaini.HasValue ? (item.Esteofechaini.Value.ToString("dd/MM/yyyy") != "01/01/0001" ? item.Esteofechaini.Value.ToString("dd/MM/yyyy") : "") : "")</td>
            <td width="8%">@(item.Esteofechafin.HasValue ? (item.Esteofechafin.Value.ToString("dd/MM/yyyy") != "01/01/0001" ? item.Esteofechafin.Value.ToString("dd/MM/yyyy") : "") : "")</td>
            <td width="12%" class="text-left">@item.Emprnomb</td>
            <td width="8%">@item.Estadescripcion</td>
            <td width="8%">
                @if (!string.IsNullOrEmpty(item.Esteoresumenejecutivoenl))
                {
                    <a target="_blank" href="@item.Esteoresumenejecutivoenl">Descargar</a>
                }
            </td>
            <td width="10%">@item.EsteoVigencia</td>
            <td width="8%">
                <a href="#" onclick="javascript: verdetalleEo(@item.Esteocodi)">Ver Detalle</a>
            </td>
        </tr>
        }
    </tbody>
</table>