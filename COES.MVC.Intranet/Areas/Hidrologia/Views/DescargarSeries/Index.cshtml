@model  COES.Servicios.Aplicacion.FormatoMedicion.FormatoModel
@{
    ViewBag.Title = "Descargar Series - Hidrologicas";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <script src="~/Content/Scripts/jquery.easytabs.min.js"></script>
    <link href="~/Content/Css/tabs.css" rel="stylesheet" />
    @*<script src="~/Content/Scripts/highcharts.js"></script>
        <script src="~/Content/Scripts/highcharts-exporting.js"></script>
    *@
}
@*<script src="~/Areas/Hidrologia/Content/Scripts/GraficosSeries.js"></script>
*@
<script src="~/Content/Scripts/inputmaskmin.js"></script>
<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />
<link href="~/Areas/Hidrologia/Content/Css/css.css?v=1.1" rel="stylesheet" />

<script src="~/Content/Scripts/jquery.format.js"></script>
<script src="~/Content/Scripts/multiselect.js"></script>
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<link href="~/Content/Css/Bootstrap/bootstrap-grid.css" rel="stylesheet" />
<link href="~/Content/Css/Bootstrap/bootstrap-grid.min.css" rel="stylesheet" />
<script src="~/Areas/Hidrologia/Content/Scripts/DescargarSeries.js"></script>
<style>
    input[type="button"]:disabled {
        background-color: #d3d3d3;
        color: #6c757d;
        border: 1px solid #ced4da;
        cursor: not-allowed;
    }
</style>
<div class="form-title">
    <div class="content-titulo"> Descargar Series</div>
</div>
<div class="content-hijo">
    <div id="tab-container" class='tab-container'>
        <ul class='etabs'>
            <li class='tab' id="tab7Paso7"><a href="#paso7">Tabla Vertical</a></li>
            <li class='tab' id="tab8Paso8"><a href="#paso8">Matrices Mensuales</a></li>
            <li class='tab' id="tab9Paso9"><a href="#paso9">Modelo Perseo</a></li>

        </ul>
        <div class='panel-container'>
            <div id="paso7">
                <div class="search-content" style="margin-bottom:0px; padding:10px; width: 100%;">
                    <div class="row mb-3">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-4">
                                    <span> Tipo de Serie : </span>
                                </div>
                                <div class="col-md-8">
                                    <select id="cbTipoSerieTab7" name="cbTipoSerieTab7" style="width: -webkit-fill-available !important;">
                                        @foreach (var item in Model.ListaTipoSerie)
                                        {
                                            <option value="@item.Tiposeriecodi">@item.Tiposerienomb</option>
                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6">
                                    <span> Tipo de Punto de Medición : </span>
                                </div>
                                <div class="col-md-6">
                                    <select id="cbTipoPuntoMedicionTab7" name="cbTipoPuntoMedicionTab7" onchange="habilitarRangeFecha(this);">
                                        <option value="">--SELECCIONE--</option>
                                        @foreach (var item in Model.ListaTipoPuntoMedicion)
                                        {
                                            <option value="@item.TipoPtoMediCodi">@item.TipoPtoMediNomb</option>
                                        }
                                    </select>
                                </div>
                            </div>

                        </div>
                    </div>

                    <div class="row mb-3">
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <span>Desde: </span>
                                        </div>
                                        <div class="col-md-9">
                                            <input type="text" id="txtFechaDesdeTab7" name="FechaInicio" value="@Model.FechaIni" disabled="disabled" style="width: 100% !important;" />
                                        </div>

                                    </div>

                                </div>
                                <div class="col-md-6">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <span> Hasta: </span>
                                        </div>
                                        <div class="col-md-9">
                                            <input type="text" id="txtFechaHastaTab7" name="FechaFin" value="@Model.FechaFin" disabled="disabled" style="width: 100% !important;" />
                                        </div>

                                    </div>

                                </div>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6">
                                    <span> Cuenca : </span>
                                </div>
                                <div class="col-md-6">
                                    <select id="cbCuencaTab7" name="cbCuencaTab7" disabled="disabled" onchange="habilitarBtnDescargar(this);">
                                        <option value="">--SELECCIONE--</option>

                                    </select>
                                </div>
                            </div>

                        </div>
                    </div>


                    <div class="row mb-3" style="text-align: center;">
                        <div class="col-md-12">
                            <input type="button" id="btnExportarTab7" value="Descargar" onclick="exportarExcelGraficoTablaVertical()" disabled="disabled" style="margin-top: 10px" />
                        </div>

                    </div>

                </div>
            </div>
            <div id="paso8">
                <div class="table-list" id="listadotab8"></div>

            </div>
            <div id="paso9">
                <div class="table-list" id="listadotab9"></div>

            </div>
        </div>
    </div>
</div>

