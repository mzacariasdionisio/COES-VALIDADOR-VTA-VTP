@model COES.MVC.Intranet.Areas.Hidrologia.Models.ModeloEmbalseModel
@{
    ViewBag.Title = "Modelo de Embalse";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Content/Scripts/multiselect.js"></script>
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />

<script src="~/Content/Scripts/jquery.easytabs.min.js?v=1.1"></script>
<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<link href="~/Content/Css/form.css" rel="stylesheet" />

<link href="~/Areas/Hidrologia/Content/Css/configuracionModeloEmbalse.css?v=1.4" rel="stylesheet" />
<script src="~/Areas/Hidrologia/Content/Scripts/configuracionModeloEmbalse.js?v=1.4"></script>

<div class="form-title">
    <div class="content-titulo">Configuración de Modelo de Embalse</div>

    <div class="content-botonera">
        <input type="button" id="btnNuevo" value="Nuevo">
    </div>
</div>

<div class="content-hijo" id="mainLayout">
    <div id="mensaje" class="action-message" style="margin:0; margin-bottom:10px; display: none"></div>

    <div id="tab-container" class='tab-container'>
        <ul class='etabs'>
            <li class='tab'><a href="#vistaListado">Listado</a></li>
            <li class='tab'><a href="#vistaDetalle">Detalle</a></li>
        </ul>
        <div class='panel-container'>

            <div id="vistaListado">
                <div id="div_reporte" class="content-tabla">

                </div>
            </div>

            <div id="vistaDetalle">
                <div id="div_detalle" class="content-tabla" style="display: none">

                </div>
            </div>
        </div>
    </div>

</div>

<div id="div_historial" class="general-popup" style="z-index:9999999998;">
    <div> <span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Historial Versiones</span></div>
    <div class="popup-text" style="margin-top:5px">
        <div id="vistaVersion">
            <div class="content-tabla">
                <div style="clear:both; height:5px"></div>

                <div class="content-tabla">
                    <div class="table-list" id="listadoHistorial" style="width: 600px"></div>
                </div>
            </div>
        </div>
    </div>
</div>

<input type="hidden" id="hdFechaActual" value="@Model.Fecha" />