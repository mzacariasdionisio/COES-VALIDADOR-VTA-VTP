@model COES.MVC.Intranet.Areas.Hidrologia.Models.HidrologiaModel

<div style="clear:both; height:15px"></div>
<table border="0" class="pretty tabla-adicional" cellspacing="0" width="640" id="tabla">
    <thead>
        <tr>
            <th rowspan="2">Cuenca</th>
            <th colspan="3"> Recurso Origen</th>            
        </tr>
        <tr>
            <th>Empresa</th>
            <th>Tipo Recurso</th>
            <th>Nombre Recurso</th>               
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaRecursosCuenca)
        {
            <tr>
                <td>@item.Cuencanomb</td>  
                <td>@item.Emprnomb</td>
                <td>@item.Famnomb</td>
                <td>@item.Equinomb</td>                     
            </tr>
        }
    </tbody>
</table>
