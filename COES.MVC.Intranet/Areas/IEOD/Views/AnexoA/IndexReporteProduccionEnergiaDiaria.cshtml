@model COES.MVC.Intranet.Areas.IEOD.Models.BusquedaIEODModel
@{
    ViewBag.Title = "Anexo A";
    Layout = "~/Areas/IEOD/Views/Shared/ViewSharedNumerales.cshtml";
}
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<script src="~/Content/Scripts/multiselect.js"></script>
<link href="~/Areas/IEOD/Content/Css/css.css?v=1.6" rel="stylesheet" />

<script src="~/Content/Scripts/highcharts.js"></script>
<script src="~/Content/Scripts/fixedcolumn.js?v=1.2"></script>

<link href="~/Areas/IEOD/Content/Css/menuPR5.css?v=1.6" rel="stylesheet" />
<script src="~/Areas/IEOD/Content/Scripts/anexoa/pr5cargarmenu.js?v=1.7"></script>
<script src="~/Areas/IEOD/Content/Scripts/anexoa/pr5globalFuncion.js?v=1.7"></script>

<script src="~/Areas/IEOD/Content/Scripts/anexoa/produccionenergiadiaria.js?id=1.8"></script>

@Html.Partial("CargarMenu")

<div class="form-title">
    <div class="content-titulo">
        <div class="title_anexoa">@Html.Raw(Model.TituloWeb)</div>
    </div>
</div>

<div class="content-hijo" id="mainLayout">

    <div class="search-content">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Empresas:</td>
                <td>
                    <select id="cbEmpresa" name="cbEmpresa" style="width:250px;" multiple="multiple">

                        @foreach (var item in Model.ListaEmpresas)
                        {
                            <option value="@item.Emprcodi">@item.Emprnomb</option>
                        }
                    </select>
                    <input type="hidden" id="hfEmpresa" name="hfEmpresa" />
                </td>
                <td> Centrales:</td>
                <td>
                    <div id="centrales"></div>
                </td>
                <td>Tipo de Generacion:</td>
                <td>
                    <div id="TipoGeneracion"></div>
                </td>
                <td>
                    <input type="button" id="btnGrafico" value="Gráfico" />
                </td>

                @Html.Partial("CargarMenuFilter")
            </tr>
        </table>
    </div>

    @Html.Partial("~/Areas/IEOD/Views/Shared/_FuenteDatos.cshtml")

    <div class="content-tabla">
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado_S">
        </div>
        <br />
        <div class="table-list" id="listado_N">
        </div>
    </div>

    <div style="clear:both; height:30px"></div>

    <div id="idGraficoContainer" style="min-width: 310px; height: 750px; max-width: 800px; margin: 0 auto; display: none;"></div>

    <div id="idGrafico2">

        <span class="button b-close"><span>X</span></span>

        <div class="popup-title"><span>Vista Gráfica</span></div>
        <div style="clear:both; height:10px"></div>

        <div id="idVistaGrafica" style="height:750px; width:1250px;">
        </div>
    </div>
</div>


