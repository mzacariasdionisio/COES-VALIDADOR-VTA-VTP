@model COES.MVC.Intranet.Areas.IEOD.Models.BusquedaIEODModel
@{
    ViewBag.Title = "Anexo A";
    Layout = "~/Areas/IEOD/Views/Shared/ViewSharedNumerales.cshtml";

}
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<script src="~/Content/Scripts/multiselect.js"></script>
<link href="~/Areas/IEOD/Content/Css/css.css?v=1.6" rel="stylesheet" />

<script src="~/Content/Scripts/highcharts.js"></script>

<link href="~/Areas/IEOD/Content/Css/menuPR5.css?v=1.6" rel="stylesheet" />
<script src="~/Areas/IEOD/Content/Scripts/anexoa/pr5cargarmenu.js?v=1.6"></script>
<script src="~/Areas/IEOD/Content/Scripts/anexoa/pr5globalFuncion.js?v=1.5"></script>

<script src="~/Areas/IEOD/Content/Scripts/anexoa/variaciones.js?id=1.8.3"></script>

@Html.Partial("CargarMenu")

<div class="form-title">
    <div class="content-titulo" style="width:600px">
        <div class="title_anexoa">@Html.Raw(Model.TituloWeb)</div>
    </div>
</div>

<div class="content-hijo" id="mainLayout">
    <div class="search-content">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td> GPS:</td>
                <td>
                    <div id="gps"></div>
                </td>

                @Html.Partial("CargarMenuFilter")

            <td>
                <input type="button" id="btnConfigurarGPS" value="Configuar GPS" />
            </td>
            </tr>
        </table>
    </div>
    @Html.Partial("~/Areas/IEOD/Views/Shared/_FuenteDatos.cshtml")
    <div class="content-tabla">
        <div>
            <table style="width: 312px;margin-bottom: 15px;">
                <tr>
                    <td><img src='~/Content/Images/ContextMenu/grafico.png' style='padding-top:7px'></td>
                    <td style="vertical-align: bottom;">Gráfico de Distribución de la frecuencia instantánea</td>
                </tr>
            </table>
        </div>

        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado"></div>
    </div>

    <div style="clear:both; height:30px"></div>

</div>

<div id="idGrafico2" style="width:1050px;height:500px">
    <span class="button b-close"><span>X</span></span>

    <div class="popup-title"><span>Gráfico</span></div>
    <div style="clear:both; height:10px"></div>

    <div id="idVistaGrafica" style="height: 450px; width: 520px;; float: left"></div>
    <div id="idVistaGrafica2" style="height: 450px; width: 520px; float: left">

    </div>
</div>

<div id="popupGPS" class="general-popup" style="width:400px">
    <div><span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Configurar GPS</span></div>
    <div class="popup-text" id="contenidoGPS">

    </div>
</div>
