@model COES.MVC.Intranet.Models.FuenteDatosModel

<div style="clear:both; height:15px"></div>
<table border="0" class="pretty tabla-icono" cellspacing="0" id="tabla" style="max-width: 95%">
    <thead>
        <tr>
            <th></th>
            <th>Código</th>
            <th>Fuente de Datos</th>
            <th>Periodo</th>
            <th>Usuario registro</th>
            <th>Fecha registro</th>
            <th>Usuario modificación</th>
            <th>Fecha modificación</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaSiPlazoEnvioFuenteDatos)
        {
            <tr>
                <td><a href="JavaScript:editarPlazo(@item.Plazcodi);"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar Fuente de Datos" alt="Editar Fuente de Datos" /></a></td>
                <td style="text-align: center">@item.Plazcodi</td>
                <td style="text-align: left; padding-left: 5px;">@item.Fdatnombre</td>
                <td>@item.Periodo</td>
                <td>@item.Plazusucreacion</td>
                <td>@item.PlazfeccreacionDesc</td>
                <td>@item.Plazusumodificacion</td>
                <td>@item.PlazfecmodificacionDesc</td>
            </tr>
        }
    </tbody>
</table>