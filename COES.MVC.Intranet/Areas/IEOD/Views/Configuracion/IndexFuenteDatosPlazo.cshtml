@{
    ViewBag.Title = "Plazo de Envío Fuente de Datos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Content/Scripts/jquery.mask.js"></script>
<link href="~/Content/Css/cmxform.css" rel="stylesheet" />
<script src="~/Content/Scripts/jquery.validate.js"></script>
<script src="~/Content/Scripts/moment.js"></script>

<script src="~/Areas/IEOD/Content/Scripts/fuenteDatosPlazo.js?v=1.2"></script>

<div class="form-title">
    <div class="content-titulo">Plazo de Envío - Fuente de Datos</div>
    <div class="content-botonera">
        <input type="button" id="btnConsultar" value="Consultar" />
    </div>
</div>

<div class="content-hijo" id="mainLayout">

    <div class="content-tabla">
        <div class="tabla-header">
            <div class="table-paginado" id="paginado"></div>
        </div>
        <div class="table-list" id="listado"></div>
    </div>

    <div style="clear:both; height:30px"></div>
    <div id="detalle"></div>

</div>

<div id="popupEditarPlazo" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Editar Plazo de Envio Fuente de Datos</span></div>
    <div style="clear:both; height:20px"></div>
    <div id="editarPlazo"></div>
</div>