@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Content/Scripts/highcharts.js"></script>
<script src="~/Areas/IndicadoresSup/Content/Scripts/informescucge.js"></script>
<link href="~/Areas/IndicadoresSup/Content/Css/css.css?v=1.2" rel="stylesheet" />
<link href="~/Content/Css/tabs.css" rel="stylesheet">
<script src="~/Content/Scripts/jquery.easytabs.min.js"></script>

<div class="form-title">
    <div class="content-titulo" style="width:auto;">@Model.Titulo</div>
</div>

<div class="content-hijo" id="mainLayout">
    <div class="search-content">
        <table class="content-tabla-search" style="width: auto">
            <tr>
                <td>Fecha:</td>
                <td>
                    <input type="text" id="txtMesAnio" class="pickermes" value="@Model.MesAnio" style="width: 100px" />
                </td>
            </tr>
        </table>
    </div>
    <div class="content-tabla">
        <div id="tab-container" class='tab-container'>
            <ul class='etabs'>
                <li class='tab'><a href="#paso1"> CUCGE </a></li>
                <li class='tab'><a href="#paso2"> CUCSS y RF </a></li>
            </ul>
            <div class='panel-container'>
                <div id="paso1" style="overflow: auto">

                    <div class="table-list" id="listado1"></div>
                </div>
                <div id="paso2" style="overflow: auto">
                    <div class="table-list" id="listado2"></div>
                </div>
            </div>
        </div>
    </div>
</div>
