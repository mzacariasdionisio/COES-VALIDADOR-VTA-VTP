@model COES.MVC.Intranet.Areas.IndicadoresSup.Models.ReporteNumeralModel
@{
    ViewBag.Title = "Generar Reporte";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Areas/IndicadoresSup/Content/Scripts/generarreporte.js?v=1.4.1"></script>

<div class="form-title">
    <div class="content-titulo">Generación de Reporte de Numerales Osinergmin</div>
    <div class="content-botonera">
        <input type="button" id="btnGenerarDB" value="Generar Datos Base" />
        <input type="button" id="btnGenerar" value="Generar Reporte" style="display:none;" />
    </div>
</div>

<div class="content-hijo">

    <div id="mensaje"></div>

    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Mes:</td>
                <td>
                    <input type="text" id="txtFecha" value="@Model.Fecha" style="width: 78px"/>
                </td>
                <td>
                    <input type="button" id="btnConsultar" value='Consultar' />
                </td>
            </tr>
        </table>
    </div>

    <div id="listado"></div>

    <div id="popupDetalle" class="general-popup">
        <div><span class="button b-close"><span>X</span></span></div>
        <div class="popup-title"><span>Detalle Reporte</span></div>
        <div class="popup-text" id="contenidoEdicion" style="margin-top:0">
            <div id="listadoDetalle">

            </div>
        </div>
    </div>

</div>