@model COES.MVC.Intranet.Areas.InformesOsinergmin.Models.InformeModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Content/Scripts/jquery.easytabs.min.js"></script>
<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<script src="~/Areas/InformesOsinergmin/Content/Scripts/informe.js"></script>


<div class="form-title">
    <div class="content-titulo">Generación Diaria - Numeral 5.1 Informe Osinergmin</div>
    <div class="content-botonera">
        <input type="button" id="btnExportar" value='Exportar' style="display:none" />                
    </div>
</div>

<div class="content-hijo">

    <div id="mensaje"></div>

    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Mes:</td>
                <td>
                    <input type="text" id="txtFecha" value="@Model.Fecha" />
                </td>               
                <td>
                    <input type="button" id="btnConsultar" value="Consultar" />
                </td>
            </tr>
        </table>
    </div>

    <div id="listado">

    </div>

    <br />
    <br />
</div>

<input type="hidden" id="hfIndicadorPublico" value="N" />








