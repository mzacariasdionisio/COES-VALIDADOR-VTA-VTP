@model COES.MVC.Intranet.Areas.Interconexiones.Models.InterconexionesModel
@{
    ViewBag.Title = "ReporteHistorico";
    Layout = "~/Views/Shared/_Layout.cshtml"; 
}
<script src="~/Areas/Interconexiones/Content/Scripts/reportes.js?v=2"></script>
<script src="~/Content/Scripts/highcharts.js"></script>
<link href="~/Areas/Interconexiones/Content/Css/css.css" rel="stylesheet" />
<script src="~/Content/Scripts/jquery.format.js"></script>
<div class="form-title">
    <div class="content-titulo">Evolución de la Energía y Potencia</div>
    <div class="content-botonera">
          
          <input type="button" id="btnExpotar" value="Exportar" />
    </div>
</div>

<div class="content-hijo" id="mainLayout" >
    <div class="search-content">
        <table class="content-tabla-search" style="width:auto">
            <tr >
                <td>Interconexión:</td>
                <td style="width:300px" >
                    <select id="cbInterconexion" name="IdInterconexion" style="width:280px;">
                        @foreach (var item in Model.ListaInterconexion)
                        {
                            <option value="@item.Intercodi">@item.Interdecrip @item.Interenlace</option>
                        }
                    </select>
                    <input type="hidden" id="hfInterconexion" value="@Model.IdPtomedicion"/>
                </td>
                <td>Fecha desde:</td>
                <td>
                    <input type="text" id="FechaDesde" name="FechaInicio" value="@Model.FechaInicio"  />
                    <input type="hidden" id="hfFechaDesde"  value="@Model.FechaInicio"  />
                </td>
                <td>Fecha hasta:</td>
                <td>
                    <input type="text" id="FechaHasta" name="FechaFin" value="@Model.FechaFin" />
                    <input type="hidden" id="hfFechaHasta" value="@Model.FechaFin" />
                </td>  
                <td>
                    <input type="button" id="btnBuscar" value="Consultar" />                 
                </td>                                
            </tr>                                             
        </table>
    </div>
    <div class="content-tabla">
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado"></div>
    </div>       
</div>
