@model COES.MVC.Intranet.Areas.Intervenciones.Models.FactorF1F2Model
@{
    ViewBag.Title = "Generar versión mensual F1 – F2";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Content/Scripts/plupload.full.min.js"></script>

<link href="~/Areas/Intervenciones/Content/Css/Intervenciones.css?v=9.0.0" rel="stylesheet" />

<script src="~/Areas/Intervenciones/Content/Scripts/FactorF1F2/intervencionesArchivo.js?v=9.0.0"></script>
<script src="~/Areas/Intervenciones/Content/Scripts/FactorF1F2/indexSustento.js?v=9.0.0"></script>

<input type="hidden" id="VersionDesc" value="@Model.VersionDesc" />
<input type="hidden" id="infverfechaperiodo" value="@Model.Mes" />
<input type="hidden" id="hfVersionF1F2" value="@Model.Version" />
<input type="hidden" id="hfVersionFinal" value="@Model.Infverflagfinal" />
<input type="hidden" id="hfHojaF1F2" value="@Model.Hoja" />
<input type="hidden" id="AccionEditar" value="@Model.AccionEditar" />

<input type="hidden" id="hfEmpresaF1F2" value="-1" />
<input type="hidden" id="hfUbicacionF1F2" value="-1" />
<input type="hidden" id="hfEquipoF1F2" value="-1" />

<div class="form-title">
    <div class="content-titulo">Listado Ejecutado</div>
    <div class="content-botonera">
        <div class="botonera-item">
            <input type="button" value="Regresar" id="btnRegresar2" />
        </div>
    </div>
</div>

<div class="content-hijo" id="mainLayout">

    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto" role="presentation">
            <tr>
                <td class="tbform-label" style="padding-left: 10px;padding-right: 2px;">Empresa:</td>
                <td>
                    <div id="div_empresa_filtroF1F2">
                        <select id="cbEmpresaF1F2">
                            <option value="-1">--TODOS--</option>
                            @foreach (var item in Model.ListaEmpresa)
                            {
                                <option value="@item.Emprcodi">@item.Emprnomb</option>
                            }
                        </select>
                    </div>
                </td>

                <td class="tbform-label" style="padding-left: 10px;padding-right: 2px;">Ubicación:</td>
                <td>
                    <div id="div_ubicacion_filtroF1F2">
                        <select id="cbUbicacionF1F2">
                            <option value="-1">--TODOS--</option>
                            @foreach (var item in Model.ListaUbicacion)
                            {
                                <option value="@item.Areacodi">@item.Areanomb</option>
                            }
                        </select>
                    </div>
                </td>

                <td class="tbform-label" style="padding-left: 10px;padding-right: 2px;">Equipo:</td>
                <td>
                    <div id="div_equipo_filtroF1F2">
                        <select id="cbEquipoF1F2">
                            <option value="-1">--TODOS--</option>
                        </select>
                    </div>
                </td>

                <td class="tbform-label" style="padding-left: 10px;padding-right: 2px;">Similitud (%):</td>
                <td>
                    <input type="text" value="50.0" id="textSimilitud" style="width: 43px;" />
                    <input type="button" value="Consultar" id="btnConsultar" />
                </td>

                <td class="content-action">
                    <a href="#" id="btnExportarExcel">
                        <div class="content-item-action">
                            <img src="~/Content/Images/ExportExcel.png" alt="logo" style="height: 23px;" /><br />
                        </div>
                    </a>
                </td>
            </tr>
        </table>
    </div>

    <div class="content-tabla">
        <div id="div_cambio" style="display: none">
            <div style="display: inline-table;">
                <a href="#" title="Guardar cambios" id="btnGuardarCambio">
                    <img src="~/Content/Images/grabar.png" alt="logo" style="height: 23px;" />
                </a>
                Guardar cambios de la tabla web.
            </div>
        </div>

        <div id="listado"></div>
    </div>

    <div style="clear:both; height:30px"></div>
</div>

<!--Ventana flotante de Archivos Adjuntos-->
<div id="popupAdjuntos" class="general-popup popup_formulario" style="width: 600px; min-height: 600px; max-width: 1070px">
</div>
