@model COES.MVC.Intranet.Areas.Intervenciones.Models.FactorF1F2Model
@{
    ViewBag.Title = "Dashboard F1 F2";
    Layout = "~/Views/Shared/_LayoutAlter.cshtml";
}

<script src="~/Content/Scripts/highstock.js?v=1.1.7"></script>
<script src="~/Content/Scripts/highcharts-more.js?v=1.1.7"></script>
<script src="~/Content/Scripts/highcharts-exporting.js?v=1.1.7"></script>

<script src="~/Areas/Intervenciones/Content/Scripts/Registro/Dashboard.js?v=1.0.2"></script>

<div class="form-title">
    <div class="content-titulo" style="width:1000px;">Dashboard de Indicadores F1/F2 SPR</div>
</div>

<div class="content-hijo">
    <div id="mensaje" class="action-message" style="margin-bottom:5px; margin-top:0px">Para el cálculo del Factor F2, los programados semanales o diarios son tratados como si fuesen ejecutados</div>
    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td style="padding-left: 15px;">Periodo:</td>
                <td><input type="text" id="txtFecha" name="txtFecha" value="@Model.Mes" style="width: 83px;" /></td>

                <td style="padding-left: 15px;">
                    <input type="button" id="btnConsultar" value="Consultar" />
                </td>
                <td style="padding-left: 5px;">
                    <input type="button" id="btnExportar" value="Exportar" />
                </td>
            </tr>
        </table>
    </div>

    <div class="content-tabla">

        <div id="tacometroF1" style="float: left; width: 50%; height: auto;"></div>
        <div id="reporteMensualF1" style="float: left; width: 50%; height: 400px;"></div>
        <div id="tacometroF2" style="float: left; width: 50%; height: auto;"></div>
        <div id="reporteMensualF2" style="float: left; width: 50%; height: 400px;"></div>

    </div>

</div>


