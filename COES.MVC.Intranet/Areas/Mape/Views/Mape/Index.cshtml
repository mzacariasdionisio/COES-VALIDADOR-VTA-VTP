@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="form-title">
    <div class="content-titulo" style="width:1000px;">Control de calidad de pronóstico de la demanda - MAPE </div>
</div>

<div class="content-hijo">

    <div id="mensaje" class="action-message" style="margin-bottom:5px; margin-top:0px">Seleccione la fecha a ser procesada y presione el botón "Generar"</div>

    <div class="search-content" style="margin-bottom:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Fecha:</td>
                <td>
                    <input type="text" id="txtFecha" value="@Model.Fecha" />
                </td>
                <td>
                    <input type="button" id="btnGenerar" value='Generar' />
                    <input type="button" id="btnConsultar" value='Consultar' />
                </td>
            </tr>
        </table>
    </div>

    <div id="listado">

    </div>
    
    
    <div id="popupDetalle" class="general-popup">
        <div><span class="button b-close"><span>X</span></span></div>
        <div class="popup-title"><span>Detalle Map Medición 48</span></div>
        <div class="popup-text" id="contenidoEdicion" style="margin-top:0">
            <div id="listadoDetalle">

            </div>
        </div>
    </div>

    <br />
</div>
<script src="~/Areas/Mape/Content/Scripts/mapmedicion48.js?v=4.1"></script>

