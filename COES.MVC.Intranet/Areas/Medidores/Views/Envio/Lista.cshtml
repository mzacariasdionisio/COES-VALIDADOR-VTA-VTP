@model COES.MVC.Intranet.Areas.Medidores.Models.GestionAdministradorModel


<div style="clear:both; height:15px"></div>
<table border="0" class="pretty tabla-adicional" cellspacing="0" width="100%" id="tabla">
    <thead>
        <tr>
            <th scope="col">ID Envío</th>
            <th scope="col">Fecha Periodo</th>
            <th scope="col">Empresa</th>
            <th scope="col">RUC</th>
            <th scope="col">Estado</th>
            <th scope="col">Cumplimiento</th>
            <th scope="col">Fecha Envío</th>
            <th scope="col">Formato</th>
            <th scope="col">Usuario</th>
            <th scope="col">Correo</th>
            <th scope="col">Telefono</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item  in Model.ListaEnvio)
        {
            var eplazo = "";
            DateTime fecha = (DateTime)item.Enviofecha;
            if (item.Envioplazo == "F")
            {
                eplazo = "Fuera de Plazo";
            }
            else
            {
                eplazo = "En Plazo";
            }
            var colorEstado = "";
            switch (@item.Estenvcodi)
            {
                case 3:
                    colorEstado = "SeaGreen";
                    break;
                default:
                    colorEstado = "red";
                    eplazo = "";
                    break;
            }
           
            <tr >
                <td>@item.Enviocodi</td>
                <td>@item.FechaPeriodo</td>
                <td>@item.Emprnomb</td>
                <td>@item.RucEmpresa</td>
                <td style="color:@colorEstado">@item.Estenvnombre</td>
                <td>@eplazo</td>
                <td>@fecha.ToString("dd/MM/yyyy HH:mm:ss")</td>
                <td>@item.Formatnombre</td>
                <td>@item.Username</td>
                <td>@item.Lastuser</td>
                <td>@item.Usertlf</td>
                <td onclick="descargarEnvio(@item.Enviocodi,@item.Formatcodi);" style="width:30px;cursor:pointer;"><img src="~/Content/Images/csv.png" /><a href ="#" ></a></td>
            </tr>
        }
    </tbody>
</table>