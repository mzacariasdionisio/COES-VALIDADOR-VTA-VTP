@model COES.MVC.Intranet.Areas.Medidores.Models.ParametroModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Hora Punta para Potencia Activa";
}

<link href="~/Areas/Medidores/Content/Css/parametro.css" rel="stylesheet" />
<script src="~/Areas/Medidores/Content/Scripts/parametro.js"></script>
<script src="~/Areas/Medidores/Content/Scripts/parametroHPPotenciaActiva.js?v=1.1.1"></script>

<div class="form-title">
    <div class="content-titulo">Parámetros de Hora Punta para Potencia Activa</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevoHPPotenciaActiva" value="Nuevo" />
    </div>
</div>

<div>
    <div class="content-tabla">
        <div id="mensaje" style="width:500px; margin:0"></div>

        <div id="hpPotenciaActiva">
        </div>
    </div>
</div>

<!--popup hpPotenciaActiva-->
<div id="popupVerHPPotenciaActiva" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Hora Punta para Potencia Activa</span></div>
    <div id="verHPPotenciaActiva"></div>
</div>

<div id="popupNuevoHPPotenciaActiva" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Nuevo Hora Punta para Potencia Activa</span></div>
    <div id="nuevoHPPotenciaActiva"></div>
</div>

<div id="popupEditarHPPotenciaActiva" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Editar Hora Punta para Potencia Activa</span></div>
    <div id="editarHPPotenciaActiva"></div>
</div>
