@model COES.MVC.Intranet.Areas.Medidores.Models.ParametroModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Rango de Operación de Centrales Solares";
}

<link href="~/Areas/Medidores/Content/Css/parametro.css" rel="stylesheet" />
<script src="~/Areas/Medidores/Content/Scripts/parametro.js"></script>
<script src="~/Areas/Medidores/Content/Scripts/parametroRangoOPCentralSolar.js"></script>

<div class="form-title">
    <div class="content-titulo">Parámetros de Rango de Operación de Centrales Solares</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevoRangoSolar" value="Nuevo" />
    </div>
</div>

<div>
    <div class="content-tabla">
        <div id="mensaje" style="width:500px; margin:0"></div>

        <div id="rangoOpCentral"></div>
    </div>
</div>

<!--popup rangoOpCentral -->
<div id="popupVerRangoSolar" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Rango de Operación de Centrales Solares</span></div>
    <div id="verRangoSolar"></div>
</div>

<div id="popupNuevoRangoSolar" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Nuevo Rango de Operación de Centrales Solares</span></div>
    <div id="nuevoRangoSolar"></div>
</div>

<div id="popupEditarRangoSolar" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Editar Rango de Operación de Centrales Solares</span></div>
    <div id="editarRangoSolar"></div>
</div>
