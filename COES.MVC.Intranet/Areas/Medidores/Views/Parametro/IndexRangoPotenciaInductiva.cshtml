@model COES.MVC.Intranet.Areas.Medidores.Models.ParametroModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Rango de Análisis de Potencia Inductiva";
}

<link href="~/Areas/Medidores/Content/Css/parametro.css" rel="stylesheet" />
<script src="~/Areas/Medidores/Content/Scripts/parametro.js"></script>
<script src="~/Areas/Medidores/Content/Scripts/parametroRangoPotenciaInductiva.js"></script>

<div class="form-title">
    <div class="content-titulo">Parámetros de Rango de Análisis de Potencia Inductiva</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevoRangoPotenciaInductiva" value="Nuevo" />
    </div>
</div>

<div>
    <div class="content-tabla">
        <div id="mensaje" style="width:500px; margin:0"></div>

        <div id="rangoPotenciaInductiva"></div>
    </div>
</div>

<!--popup rangoPotenciaInductiva -->
<div id="popupVerRangoPotenciaInductiva" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Rango de Análisis de Potencia Inductiva</span></div>
    <div id="verRangoPotenciaInductiva"></div>
</div>

<div id="popupNuevoRangoPotenciaInductiva" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Nuevo Rango de Análisis de Potencia Inductiva</span></div>
    <div id="nuevoRangoPotenciaInductiva"></div>
</div>

<div id="popupEditarRangoPotenciaInductiva" class="general-popup">
    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Editar Rango de Análisis de Potencia Inductiva</span></div>
    <div id="editarRangoPotenciaInductiva"></div>
</div>
