@model COES.MVC.Intranet.Areas.IEOD.Models.BusquedaIEODModel
@{
    ViewBag.Title = "Anexo A";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<script src="~/Content/Scripts/multiselect.js"></script>
<script src="~/Areas/Migraciones/Content/Scripts/anexoa/validacionesFiltros.js?id=1"></script>
<script src="~/Areas/Migraciones/Content/Scripts/anexoa/globalFuncionPR5.js?id=1"></script>
<link href="~/Areas/IEOD/Content/Css/css.css" rel="stylesheet" />
<script src="~/Content/Scripts/highcharts.js"></script>
<script src="~/Content/Scripts/highcharts-3d.js"></script>

<script src="~/Areas/Migraciones/Content/Scripts/anexoa/cargarmenupr5.js?id=1.1"></script>
<script src="~/Areas/Migraciones/Content/Scripts/anexoa/demandaporarea.js?id=1.3"></script>

@Html.Partial("CargarMenu")
<div class="form-title" style="text-indent:10px">
    <div style="width:auto" class="content-titulo">Reporte de Demanda por Área  (@Model.FechaInicio)</div>
    @Html.Partial("CargarMenuFilter")
</div>

<div class="content-hijo" id="mainLayout">
    <div class="search-content">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Area:</td>
                <td>
                    <div id="areaOperativa"></div>
                </td>
                <td style="display: none"> Equipo:</td>
                <td style="display: none">
                    <div id="equipo"></div>
                </td>
            </tr>
            <tr>
                <td>
                    <input type="hidden" id="txtFechaInicio" name="txtFechaInicio" value="@Model.FechaInicio" />
                </td>
                <td>
                    <input type="hidden" id="txtFechaFin" name="txtFechaFin" value="@Model.FechaFin" />
                </td>
            </tr>
        </table>
    </div>

    <div class="content-tabla">
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado"></div>

        <div id="leyenda" style="display: none">
            <p>LEYENDA:</p>
            <table border='0' style='width: 500px;'>
                <tbody>
                    <tr>
                        <td style='width: 50px; border: 1px solid black; background-color: #c0504d !important; color: white;'></td>
                        <td>Potencia recibida del Ecuador</td>
                    </tr>
                    <tr>
                        <td style='width: 50px; border: 1px solid black; background-color: #4f81bd !important; color: white;'></td>
                        <td>Potencia entregada al Ecuador</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <div style="clear:both; height:30px"></div>

    <div id="idGraficoContainer" style="min-width: 310px; height: 400px; max-width: 800px; margin: 0 auto; display: none;"></div>

    <div id="grafico1" style="height: 450px; margin: 0 auto;"></div>
</div>