@model COES.MVC.Intranet.Areas.PotenciaFirmeRemunerable.Models.PotenciaFirmeRemunerableModel
<div style="clear:both; height:15px"></div>
<table id="tablaListadoGrupodat" border="0" class="pretty tabla-icono" cellspacing="0">
    <thead>
        <tr>
            <th style="width: 50px">Opciones</th>
            <th style="width: 80px">Fecha de Vigencia</th>
            @if (Model.Conceptocodi == 541)//MR
            { <th style="width: 80px">Valor (%)</th>}
            @if (Model.Conceptocodi == 643) //canon de agua
            {<th style="width: 80px">Valor (S/./kwh)</th>}
            @if (Model.Conceptocodi == 5) //CR
            {<th style="width: 80px">Valor (S/./kwh)</th>}



            <th style="width: 120px">Usuario modificación</th>
            <th style="width: 120px">Fecha modificación</th>
            <th style="width: 50px">Estado</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model.ListaParametros)
        {
            string claseFila = "";
            if (item.Deleted == 1) { claseFila = "clase_eliminado"; }
            <tr class="@claseFila">
                <td>
                    @if (Model.AccesoEditar)
                    {
                        if (item.Deleted == 0)
                        {
                            <a href="JavaScript:editarGrupodat('@item.FechadatDesc',@item.Formuladat, '@item.FechaactDesc', '@item.Deleted');" title="Editar Registro">
                                <img src="~/Content/Images/btn-edit.png" alt="Editar Registro" />
                            </a>
                            <a href="JavaScript:eliminarGrupodat(@item.Grupocodi, @item.Concepcodi,'@item.FechadatDesc',@item.Deleted);" title="Eliminar Registro">
                                <img src="~/Content/Images/Trash.png" alt="Eliminar Registro" />
                            </a>
                        }
                    }
                </td>
                <td>@item.FechadatDesc</td>
                <td>@item.Formuladat</td>
                <td>@item.Lastuser</td>
                <td>@item.FechaactDesc</td>
                <td>@item.EstadoDesc</td>
            </tr>
        }

    </tbody>
</table>