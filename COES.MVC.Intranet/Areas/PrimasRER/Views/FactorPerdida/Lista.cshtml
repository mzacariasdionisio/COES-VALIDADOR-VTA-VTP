<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th>Nombre Archivo</th>
            <th>Fecha desde</th>
            <th>Fecha hasta</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListFacPerMedDTO)
        {
            <tr id="fila_@item.Rerfpmcodi">
                <td class="text_left">@item.Rerfpmnombrearchivo</td>
                <td class="text_center">@item.Rerfpmdesde.ToString("dd/MM/yyyy")</td>
                <td class="text_center">@item.Rerfpmhasta.ToString("dd/MM/yyyy")</td>
                <td style="width:100px;"><a href="@Url.Action("Index","FactorPerdidaDetalle", new { id = item.Rerfpmcodi })"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar el registro" alt="Editar el registro" /></a></td>
                <td style="width:100px;"><a href="#" id="eliminar_@item.Rerfpmcodi" class="delete"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a></td>
            </tr>
        }

    </tbody>
</table>
<div id="popup"></div>