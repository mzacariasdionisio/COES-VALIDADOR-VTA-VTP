@model COES.MVC.Intranet.Areas.PrimasRER.Models.PrimasRERModel

<script src="~/Areas/PrimasRER/Content/Scripts/parametrorermeses.js"></script>
<div class='panel-container'>
    <div class="content-botonera">
        <input type="button" id="btnNuevaRelacion" value="Nuevo" alt="Insertar un nuevo registro" title="Insertar un nuevo registro" onclick="nuevaRelacion()" />
        @if (Model.ParametroRER.Rerpprmes != 5)
        {
            <input type="button" id="btnCopiarMesAnterior" value="Copiar Mes Anterior" alt="Copiar mes anterior" title="Copiar mes anterior" onclick="copiarRelacionMesAnterior()" />

        }
    </div>
    <input type="hidden" name="ParametroRER.Rerpprcodi" id="Rerpprcodi" value="@Model.ParametroRER.Rerpprcodi" />
    <input type="hidden" name="AnioVersion.Reravaniotarif" id="Reravaniotarif" value="@Model.AnioVersion.Reravaniotarif" />
    <input type="hidden" name="ParametroRER.Rerpprmes" id="Rerpprmes" value="@Model.ParametroRER.Rerpprmes" />
    <table border="0" class="pretty tabla-icono" id="tablameses">
        <thead>
            <tr>
                <th>Central RER</th>
                <th>Cantidad de retiros</th>
                <th style="width:30px;">&nbsp;</th>
                <th style="width:30px;">&nbsp;</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListRerCentralCodRetiro)
            {
                <tr id="fila_@item.Rercencodi">
                    <td class="text_center" style="padding: 10px;">@item.Equinomb</td>
                    <td class="text_center">@item.Cantidad</td>
                    <td style="width:30px;"><a href="JavaScript:editarRelacionBarraTransferencia(@Model.ParametroRER.Rerpprcodi, @item.Rercencodi)" title="Editar registro"><img src="~/Content/Images/btn-edit.png" title="Editar el registro" alt="Editar el registro"></a></td>
                    <td style="width:30px;"><a href="JavaScript:eliminarRelacionBarraTransferencia(@Model.ParametroRER.Rerpprcodi, @item.Rercencodi)" title="Lista de revisiones"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro"></a></td>
                </tr>
            }
        </tbody>
    </table>
</div>


<div id="popupRelacionCodigoRetiroEdit">
    <span class="button b-close"><span>X</span></span>
    <div id="editRelacionCodigoRetiro"></div>
</div>

<div id="popupRelacionCodigoRetiroNew">
    <span class="button b-close"><span>X</span></span>
    <div id="newRelacionCodigoRetiro"></div>
</div>
