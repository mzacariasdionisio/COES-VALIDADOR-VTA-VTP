@model COES.MVC.Intranet.Areas.PronosticoDemanda.Models.ConfiguracionModel
@Html.AntiForgeryToken()
@{
    ViewBag.Title = "PRODEM - Configuración General";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var sMensajeExito = TempData["sMensajeExito"];
    string sSelected = "";
}

<link href="~/Content/Scripts/Charts/jquery.jqplot.min.css" rel="stylesheet" />
<link href="~/Content/Css/calendar.css" rel="stylesheet" />
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<link href="~/Areas/PronosticoDemanda/Content/Css/css.css" rel="stylesheet" />
<link href="~/Areas/PronosticoDemanda/Content/Css/fixer.css" rel="stylesheet" />
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />

<script src="~/Content/Scripts/jquery.easytabs.min.js"></script>
<script src="~/Content/Scripts/multiselect.js"></script>
<script src="~/Content/Scripts/datepicker.js"></script>
<script src="~/Content/Scripts/datatable.js"></script>
<script src="~/Content/Scripts/jqueryui.js"></script>

<script src="~/Areas/PronosticoDemanda/Content/Scripts/configuracion.js?v=3.3"></script>

<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<script src="~/Content/Scripts/highcharts.js"></script>
<script src="https://rawgithub.com/highcharts/draggable-points/master/draggable-points.js"></script>

<div class="ast">
    <div class="form-title">
        <div class="content-titulo">Configuración General</div>
    </div>
    <div class="content-hijo">
        <div class="menu">
            <div class="menu-item options selected">
                <label id="opt-parametros" title="Configurar parametros de los procesos">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-parametros.png" />
                    <br />
                    <label>Parámetros</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-exogenas" title="Información de las variables exógenas">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-exogenas.png" />
                    <br />
                    <label>Exógenas</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-motivos" title="Configurar motivos">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-motivo.png" alt="Configurar motivos" />
                    <br />
                    <label>Motivos</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-agrupaciones" title="Configurar agrupaciones de puntos de medición">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-agrupacion.png" />
                    <br />
                    <label>Agrupaciones</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-perfiles" title="Consulta de perfiles patrón y mantenimiento de los perfiles defecto">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-glineas.png" />
                    <br />
                    <label>Perfiles</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-PerdidasTransversales" title="Registrar perdidas transversales">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-perdidas.png" />
                    <br />
                    <label>Pérdidas Transversales</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-AgrupacionULFormula" title="Registrar perdidas transversales">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-aform.png" />
                    <br />
                    <label>Agrupacion UL - Formulas</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-ServiciosAuxiliares" title="Registrar perdidas transversales">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-ssaa.png" />
                    <br />
                    <label>Servicios Auxiliares</label>
                </label>
            </div>

        </div>
        <div id="message" style="display: none">
            Aquí se agregan los posibles mensajes...
        </div>

        <div id="module">

        </div>
    </div>
</div>