<script src="~/Areas/PronosticoDemanda/Content/Scripts/ServiciosAuxiliares.js?v=2.0"></script>

<div class="h-panel">
    <div id="main" style="width:100%" data-msg="@Model.Mensaje" data-tpo="@Model.TipoMensaje">
        <table id="tb-barras" class="pretty tabla-icono" style="width: 100%;"></table>
    </div>
</div>

<div id="popup" class="general-popup" style="min-width: 500px; min-height: 500px;">

    <span class="button b-close"><span>X</span></span>

    <div class="popup-title">
        <span>Relacionar Barra con Formulas</span>
        <div style="float: right;">
            <input id="btn-guardar" type="button" value="Aceptar" />
        </div>
    </div>

    <div class="popup-text" style="overflow-y: auto; padding: 0px 10px 0px 0px; height: 600px;">

        <div id="pop-mensaje" style="display: none"></div>

        <div style="margin-bottom: 10px;">
            <div style="font-size: 15px; font-weight: bold; color: #3D90CB;">
                Seleccionados
            </div>
            <div>
                <table id="tb-seleccionados" class="pretty tabla-icono" style="width: 550px;"></table>
            </div>
        </div>

        <div>
            <div style="font-size: 15px; font-weight: bold; color: #3D90CB;">
                Formulas disponibles
            </div>
            <div>
                <div>
                    <table id="tb-disponibles" class="pretty tabla-icono" style="width: 550px;"></table>
                </div>
            </div>
        </div>

    </div>

</div>
