<div class="ast">
    <div class="form-title">
        <div class="content-titulo">Consultas al Estimador de Estados del TNA</div>
    </div>
    <div class="content-hijo">
        <div class="menu">
            <div class="menu-item options selected">
                <label id="opt-generadores" title="Depuración manual de la información reportada a nivel de puntos">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-generador.png" />
                    <br />
                    <label>Generadores</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-lineas" title="Depuración manual de la información reportada a nivel de agrupaciones">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-lineas.png" />
                    <br />
                    <label>Líneas</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-transformadores" title="Permite reporcesar la información enviada desde Extranet por los agentes">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-transformador.png" />
                    <br />
                    <label>Transformadores</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-barras" title="Permite reporcesar la información enviada desde Extranet por los agentes">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-barra.png" />
                    <br />
                    <label>Barras</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-cargas" title="Permite reporcesar la información enviada desde Extranet por los agentes">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-carga.png" />
                    <br />
                    <label>Cargas</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-shunts" title="Permite reporcesar la información enviada desde Extranet por los agentes">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-shunt.png" />
                    <br />
                    <label>Shunts</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-importados" title="Permite reporcesar la información enviada desde Extranet por los agentes">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-importacion.png" />
                    <br />
                    <label>Cargar Archivos</label>
                </label>
            </div>
        </div>
        <div id="message" style="display: none">
            Aquí se agregan los posibles mensajes...
        </div>
        <div id="module">

        </div>
    </div>
