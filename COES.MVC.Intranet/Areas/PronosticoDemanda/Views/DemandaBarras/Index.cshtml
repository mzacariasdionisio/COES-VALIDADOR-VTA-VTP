<div class="ast">
    <div class="form-title">
        <div class="content-titulo">Demanda por Barras</div>
    </div>
    <div class="content-hijo">
        <div class="menu">
            <div class="menu-item options selected">
                <label id="opt-pronosticoporbarras" title="Permite consultar y pronosticar la demanda a nivel de barras CP">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-glineas.png" />
                    <br />
                    <label>Pronóstico por Barras</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-trasladocarga" title="Permite ejecutar el traslado de carga">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-glineas.png" />
                    <br />
                    <label>Traslado de Carga</label>
                </label>
            </div>
            <div class="menu-item options">
                <label id="opt-demandabarras" title="Permite consultar la demanda por barras agrupada por áreas operativas">
                    <img src="~/Areas/PronosticoDemanda/Content/Images/prn-ico-glineas.png" />
                    <br />
                    <label>Demanda en barras por Áreas</label>
                </label>
            </div>
        </div>
        <div id="message" style="display: none">
            Aquí se agregan los posibles mensajes...
        </div>
        <div id="module"></div>
    </div>
