input[type="text"]:disabled {
    background-color: #f0f0f0; /* Cambia el color de fondo */
    color: #a0a0a0; /* Cambia el color del texto */
    border: 1px solid #ccc; /* Cambia el color del borde */
    cursor: not-allowed; /* Cambia el cursor para indicar que no es interactivo */

}

input[type="number"]:disabled {
    background-color: #f0f0f0; /* Cambia el color de fondo */
    color: #a0a0a0; /* Cambia el color del texto */
    border: 1px solid #ccc; /* Cambia el color del borde */
    cursor: not-allowed; /* Cambia el cursor para indicar que no es interactivo */
}

select:disabled {
    background-color: #f0f0f0; /* Cambia el color de fondo */
    color: #a0a0a0; /* Cambia el color del texto */
    border: 1px solid #ccc; /* Cambia el color del borde */
    cursor: not-allowed; /* Cambia el cursor para indicar que no es interactivo */
}

textarea:disabled {
    background-color: #f0f0f0 !important;  /* Cambia el color de fondo */
    color: #a0a0a0; /* Cambia el color del texto */
    border: 1px solid #ccc; /* Cambia el color del borde */
    cursor: not-allowed; /* Cambia el cursor para indicar que no es interactivo */
}


input[type="number"] {
    border-radius: 5px;
    border: 1px solid #ccc;
    height: 20px;
    text-indent: 8px;
    font: 12px "arial", sans-serif;
}


