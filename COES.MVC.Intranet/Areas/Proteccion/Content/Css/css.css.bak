.div-importar {
    border: 3px solid #2980B9;
    width: 460px;
    min-height: 160px;
    position: absolute;
    display: none;
    font-size: 12px;
    right: 20px;
    z-index: 20000;
    background-color: #fff;
    height: auto;
}

.div-importar-close {
    float: right;
    font-weight: bold;
    font-size: 14px;
    margin: 5px;
    height: 20px;
    cursor: pointer;
}

.div-importar-close:hover {
    color: #FF770D;
}

.div-importar-content {
    width: auto;
    height: auto;
    margin: 20px;
}

.div-importar-boton {
    width: 70px;
    margin: auto;
    margin-top: 10px;
}

.form-importar-content {
    width: auto;
    background: #E8F6FF;
    border: 2px solid #B8E2FB;
    padding: 5px;
    text-indent: 15px;
    border-radius: 6px 6px 6px 6px;
    -moz-border-radius: 6px 6px 6px 6px;
    -webkit-border-radius: 6px 6px 6px 6px;
    line-height: 20px;
    margin-top: 10px;
}

.form-importar-item {
    float: left;
    width: auto;
}

.label_filtro {
    font-weight: bold;
    color: #3D90CB;
}

