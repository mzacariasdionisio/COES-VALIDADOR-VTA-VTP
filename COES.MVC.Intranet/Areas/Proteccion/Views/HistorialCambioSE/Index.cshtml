@model COES.MVC.Intranet.Areas.Proteccion.Models.HistorialCambioSEModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script>

    $(function () {

        $('#btnGrabar').click(function () {
        });

        $('#fFechaCreacion').Zebra_DatePicker({
            show_clear_date: 0
        });

        $('#fFechaUltActualizacion').Zebra_DatePicker({
            show_clear_date: 0
        });

    });


</script>

<style>
    .text-wrap {
        white-space: normal;
    }

    .width-200 {
        width: 200px;
    }

    textarea {
        max-width: 510px; 
        max-height: 300px; 
    }

        textarea:disabled {
            color: #a0a0a0;
        }
</style>
<div class="content-botonera">
    <input type="button" value="Consultar" id="btnBuscar" name="btnBuscar" class="form-action" style="margin-top: 15px" />
    <input type="button" value="Nuevo" id="btnNuevo" name="btnNuevo" class="form-action" style="margin-top: 15px" />
    <input type="button" value="Exportar" id="btnExportar" name="btnExportar" class="form-action" style="margin-top: 15px" />
</div>
<div class="form-title">Memorias de Cálculo Subestación</div>

<div id="mensaje"></div>
<div class="content-hijo" id="mainLayout">
    <div id="idpanel" style="overflow:auto; background-color:#fff">

        <div class="search-content" style="margin-bottom:0px; padding:15px">
            <table class="content-tabla-search" style="width:auto">
                <tr>
                    <th style="width:4%"></th>
                    <th style="width:20%"></th>
                    <th style="width:4%"></th>
                    <th style="width:20%"></th>
                    <th style="width:9%"></th>
                    <th style="width:15%"></th>
                    <th style="width:8%"></th>
                    <th style="width:15%"></th>
                </tr>
                <tr id="SeccionProgramaExistente">
                    <td>Subestación: </td>
                    <td>
                        @(Html.DropDownList("idSubestacion", new SelectList(Model.listaSubestacion, "Areacodi", "Areanomb", Model.idSubestacion),"TODOS"))
                    </td>
                    <td>Zona: </td>
                    <td>
                        @(Html.DropDownList("idZona", new SelectList(Model.ListaArea, "Areacodi", "Areanomb", 0), "TODOS", new { style = "width: 100%;" }))
                    </td>


                </tr>
            </table>
        </div>
        <br />
        <br />
        <div id="popUpEditar">
            <span class="button b-close"><span>X</span></span>
            <div class="popup-title"><span id="tituloEditar" name="tituloEditar">Crear Memoria de Cálculo Subestación</span></div>
            <div id="editarArea"></div>
        </div>

        <div class="content-hijo" id="mainLayout">
            <div id='lista'></div>
        </div>
    </div>
</div>

<!--Menu contextual-->
<link href="~/Areas/Proteccion/Content/Css/css.css" rel="stylesheet" />
