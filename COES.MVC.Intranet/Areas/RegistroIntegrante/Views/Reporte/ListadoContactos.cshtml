@model COES.MVC.Intranet.Areas.RegistroIntegrante.Models.ReporteModel
@using COES.Servicios.Aplicacion.RegistroIntegrantes;
<div style="clear:both; height:15px"></div>
<table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tabla">
    <thead>
        <tr>
            <th>Nro</th>
            <th style="display:none">Estado</th>
            <th>Tipo Empresa</th>
            <th>RUC</th>
            <th>Sigla</th>
            <th>Razón Social</th>
            <th>Nombre Comercial</th>
            @*<th>Tipo</th>
            <th>Tipo Representante</th>*@
            <th>Nombre Contacto</th>
            <th>Nro Documento Identidad</th>
            <th>Cargo Empresa</th>
            <th>Correo Electrónico</th>
            <th>Teléfono</th>
            <th style="display:none">Baja</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaIntegrantes)
            {
            <tr>
                <td>
                    @item.Emprcodi
                </td>
                <td style="display:none">@item.Emprestado</td>
                <td>@item.Tipoemprdesc</td>
                <td>@item.Emprruc</td>
                <td>@item.Emprsigla</td>
                <td>@item.Emprrazsocial</td>
                <td>@item.Emprnombrecomercial</td>
                @*<td>@item.RpteTipo</td>
                <td>@item.RpteTipRepresentanteLegal</td>*@
                <td>@item.RpteNombres</td>
                <td>@item.RpteDocIdentidad</td>
                <td>@item.RpteCargoEmpresa</td>
                <td>@item.RpteCorreoElectronico</td>
                <td style="display:none">@item.RpteTelefono</td>
                @if (item.RpteBaja == ConstantesRegistroIntegrantes.RepresentanteBajaSi)
                {
                    <td>SI</td>
                }
                else
                {
                    <td>NO</td>
                }
            </tr>
        }
    </tbody>
</table>
