@model COES.MVC.Intranet.Areas.RegistroIntegrante.Models.SolicitudPendienteModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Content/Scripts/messaje-operation.js"></script>
<script src="~/Areas/RegistroIntegrante/Content/Scripts/solicitud.js?v=1.12"></script>
<link href="~/Areas/RegistroIntegrante/Content/Css/css.css" rel="stylesheet" />

<div class="form-title" style="color: #278CCC; margin-bottom:0px;">Gestión de Solicitudes</div>
<div class="content-hijo" id="mainLayout">
    <div id="mensaje"></div>
    <div style="clear:both; height:5px;"></div>
    <div class="search-content" style="margin-bottom:0px; padding:10px">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Estado de solicitud:</td>
                <td>
                    <select id="cbEstadoSolicitud" style="width:140px">
                        @foreach (var item in Model.ListaEstadoSolicitud)
                        {
                            <option value="@item.Soliestadocodi">@item.Soliestado</option>
                        }
                    </select>
                </td>
                <td>
                    <input type="button" id="btnConsultar" value="Consultar" />
                </td>
            </tr>
        </table>
    </div>
    <div style="clear:both"></div>
    <div class="content-tabla">
        <div class="tabla-header">
            <div class="table-paginado" id="paginado"></div>
        </div>
        <div class="table-list" id="listado"></div>
        <div style="clear:both; margin-bottom:40px"></div>
    </div>

    <div id="MessagesClose">
        <div class="popup-title"><span>Realizar Conformidad</span></div>
        <div class="popup-text content-messajes-text">

        </div>
        <div class="formulario-boton">
            <input id="btnAceptarMessages" type="button" value="Aceptar" />
            <input id="btnCancelarMessages" type="button" value="Cancelar" style="display:none;" />
        </div>
    </div>

    <div id="popupFlujo" class="general-popup" style="min-width:700px">
        <div><span class="button b-close"><span>X</span></span></div>
        <div class="popup-text" id="contenidoFlujo">
        </div>
    </div>

</div>