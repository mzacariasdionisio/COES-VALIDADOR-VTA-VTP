@model COES.MVC.Intranet.Areas.RegistroIntegrante.Models.FlujoModel
<link href="~/Areas/RegistroIntegrante/Content/Css/solicitud.css" rel="stylesheet" />
<link href="~/Content/Css/cmxform.css" rel="stylesheet" />
<script src="~/Content/Scripts/jquery.validate.js"></script>

<div class="popup-title"><span>Documentos Mesa de Partes</span></div>
<div class="popup-text" id="contenidoEdicion">
    <div id="mensaje" class="action-message" style="display: none;">Por favor complete los datos solicitados.</div>
    <table border="0" class="pretty tabla-icono" cellspacing="0" cellpadding="0" width="100%" id="tabla">
        <thead>
            <tr>                
                <th>Nro</th>
                <th>Fecha Recepción</th>
                <th>Comentario</th>                
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListaFlujo)
            {
                <tr>                    
                    <td>@item.corrnumproc</td>
                    <td>@item.FechaRecepcion</td>
                    <td>@item.Observacion</td>  
                    <td>
                        @*<a href="JavaScript:verDocumentoFlujo('@item.DocumentoAdjunto');" id="verDocumentoFlujo" title="Ver">
                            <img src="~/Content/Images/Visualizar.png" />
                        </a>
                        <a href="JavaScript:descargarDocumentoFlujo('@item.DocumentoAdjunto', '@item.DocumentoAdjuntoFileName');" id="descargarDocumentoFlujo" title="Descargar">
                            <img src="~/Content/Images/Download.png" />
                        </a>*@

                        <a href="@item.EnlaceArchivo" title="Descargar">
                            <img src="~/Content/Images/Download.png" />
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>

</div>






