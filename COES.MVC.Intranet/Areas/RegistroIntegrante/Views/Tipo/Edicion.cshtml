@model COES.MVC.Intranet.Areas.RegistroIntegrante.Models.TipoModel
<link href="~/Areas/RegistroIntegrante/Content/Css/solicitud.css" rel="stylesheet" />
<link href="~/Content/Css/cmxform.css" rel="stylesheet" />
<script src="~/Content/Scripts/jquery.validate.js"></script>

<div class="popup-title"><span>Visualizar Tipo Agente</span></div>
<div class="popup-text" id="contenidoEdicion">
    <div id="mensaje" class="action-message" style="display: none;">Por favor complete los datos solicitados.</div>
    <table class="table-form-content">
       
            <tr>
                <td><label>6.1 Tipo Agente:</label></td>
                <td>
                    <input type="text" id="txtTipoAgente" value="@Model.Tipotipagente" style="width:280px;" readonly>
                </td>
            </tr>
            <tr>
                <td><label>6.2 Documento Sustentatorio:</label></td>
                <td>               
                    <input type="text" id="txtTipodocsustentatorio"  value="@Model.Tipodocsustentatorio" style="width:280px;" readonly>
                </td>
            </tr>
            <tr>
                <td><label>6.3 Archivo Digitalizado:</label></td>
                <td>
                    <a href="JavaScript:verDigitalizado();" id="verDigitalizado" title="Ver">
                        <img src="~/Content/Images/Visualizar.png" />
                    </a>
                    <a href="JavaScript:descargarDigitalizado();" id="descargarDigitalizado" title="Descargar">
                        <img src="~/Content/Images/Download.png" />
                    </a>
                </td>
            </tr>
            <tr class="Generador">
                <td><label>6.4 Potencia Instalada:</label></td>
                <td><input type="text" id="txtPotenciaInstaladaTI" class="GeneradorField Required"  value="@Model.Tipopotenciainstalada" data-validator-id="spnPotenciaInstaladaTI" style="width:280px;" readonly></td>
            </tr>
            <tr class="Generador">
                <td><label>6.5 Número de Centrales:</label></td>
                <td><input type="text" id="txtNumeroCentralesTI" class="GeneradorField Required" value="@Model.Tiponrocentrales" data-validator-id="spnNumeroCentralesTI" style="width:280px;" readonly></td>
            </tr>
            <tr class="Transmisor" height="35px">
                <td><label>6.4 Línea de Transmisión:</label></td>
                <td></td>
            </tr>
            <tr class="Transmisor">
                <td style="padding-left:80px" colspan="2">
                    <table cellpadding="7">
                        <tr class="TransmisorRow">
                            <td>
                            @if (Model.Tipolineatrans500 == "S")
                            {
                                <input type="checkbox" id="chk500" checked="checked" disabled="disabled" />
                            }
                            else
                            {
                                <input type="checkbox" id="chk500" disabled="disabled" />
                            }                            
                            </td>
                            <td>500 kV</td>
                            <td><input type="text" id="val500" class="SumTrans" readonly value="@Model.Tipolineatrans500km" /></td>
                            <td>km</td>
                        </tr>
                        <tr class="TransmisorRow">
                            <td>
                                @if (Model.Tipolineatrans220 == "S")
                                {                                    
                                    <input type="checkbox" id="chk200" checked="checked" disabled="disabled" />
                                }
                                else
                                {
                                    <input type="checkbox" id="chk200" disabled="disabled"  />
                                }                            
                            </td>
                            <td>200 kV</td>
                            <td><input type="text" id="val200" class="SumTrans" readonly value="@Model.Tipolineatrans220km" /></td>
                            <td>km</td>
                        </tr>
                        <tr class="TransmisorRow">
                            <td>
                                @if (Model.Tipolineatrans138 == "S")
                                {                                    
                                    <input type="checkbox" id="chk138" checked="checked" disabled="disabled" />
                                }
                                else
                                {
                                    <input type="checkbox" id="chk138" disabled="disabled" />
                                }                            
                            </td>
                            <td>138 kV</td>
                            <td><input type="text" id="val138" class="SumTrans" readonly value="@Model.Tipolineatrans138km" /></td>
                            <td>km</td>
                        </tr>
                        <tr>
                            <td colspan="2">Total Líneas de transmisión:</td>
                            <td><input type="text" id="txtTotalTransmision" value="@Model.Tipototallineastransmision" readonly /></td>
                            <td><b>km</b><span style="padding-left:15px;" id="spnIntegranteTransmisor"></span></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr class="Distribuidor">
                <td><label>6.4 Máxima demanda coincidente anual:</label></td>
                <td><input type="text" id="txtMaximaDemandaCoincidenteAnual" class="DistribuidorField Required" readonly value="@Model.Tipomaxdemandacoincidente" style="width:280px;" readonly><span style="padding-left:10px;padding-right:10px">MW</span></td>
            </tr>
            <tr class="UsuarioLibre">
                <td><label>6.4 Máxima demanda contratada:</label></td>
                <td><input type="text" id="txtMaximaDemandaContratada" class="UsuarioLibreField Required" readonly value="@Model.Tipomaxdemandacontratada" style="width:280px;" readonly><span style="padding-left:10px;padding-right:10px">MW</span></td>
            </tr>
            <tr class="UsuarioLibre">
                <td><label>6.5 Número de Suministrador:</label></td>
                <td><input type="text" id="txtNumeroSuministrador" class="UsuarioLibreField Required" readonly value="@Model.Tiponumsuministrador" style="width:280px;" readonly></td>
            </tr>
        
        <tr>
            <td colspan="2" style="text-align:center;">
                <input id="btnAceptar" type="button" value="Aceptar" style="margin: 5px 0 5px 0;" />
            </td>
        </tr>
    </table>

</div>
<input type="hidden" id="hdfTipoEmpresa" value="@Model.Tipoemprcodi" />
<input type="hidden" id="hdfNombreArchivoDigitalizado" value="@Model.Tipoarcdigitalizadofilename" />
<input type="hidden" id="hdfAdjuntoDigitalizado" value="@Model.Tipoarcdigitalizado" />









