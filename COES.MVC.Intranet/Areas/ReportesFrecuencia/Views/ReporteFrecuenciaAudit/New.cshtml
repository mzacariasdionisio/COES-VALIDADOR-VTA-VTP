@model COES.MVC.Intranet.Areas.ReportesFrecuencia.Models.ReporteFrecuenciaAuditModel
@{
    ViewBag.Title = "Equipo GPS - Colocar NULL";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <link href="~/Areas/ReportesFrecuencia/Content/Css/ReportesFrecuencia.css" rel="stylesheet" />
    <link href="~/Areas/ReportesFrecuencia/Content/Css/Validate.css" rel="stylesheet" />
    <script src="~/Areas/ReportesFrecuencia/Content/Scripts/Reportes.js?v=@DateTime.Now.Minute"></script>

    @using (Html.BeginForm("Save", "ReporteFrecuenciaAudit", FormMethod.Post, new { id = "frmEquipoGPS", @class = "cmxform" }))
    {
        @Html.AntiForgeryToken()
        @Html.Hidden("Usuario", User.Identity.Name)
        @Html.Hidden("ID", 0)
        @Html.Hidden("Accion", "Save")
        <div class="ast">
            <div class="form-title">
                <div class="content-titulo"> @Model.Nombre - Reemplazar por NULL</div>
                <div class="content-botonera">
                    <input type="submit" value="Guardar" alt="Guardar" title="Guardar" />
                    <input type="button" value="Cancelar" onclick="location.href='@Url.Action("Index","ReporteFrecuenciaAudit")/Index/@Model.IdGPS'" alt="Retornar" title="Retornar" />

                </div>
            </div>
            <div style="padding:20px; width:500px;">
                <table cellspacing="2" cellpadding="5">

                    <tr>
                        <td colspan="2">
                            <div class="editor-label" style="color:red;" id="sError">

                            </div>
                        </td>
                    </tr>

                    <tr>
                        <td>
                            <div class="editor-label">
                                @Html.Label("Código")

                            </div>
                        </td>
                        <td>
                            <div class="editor-field">
                                <input type="text" id="IdGPS" class="form-control2" value="@Model.IdGPS" readonly="readonly" />
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="editor-label">
                                @Html.Label("Fecha Inicio")
                                (*)
                            </div>
                        </td>
                        <td>
                            <div class="editor-field">
                                <input type="datetime-local" id="FechaInicial" class="formatFechaHora datepicker" step="1" value="@Model.FechaInicial.ToString("yyyy-MM-ddTHH:mm:ss")" />
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="editor-label">
                                @Html.Label("Fecha Fin")
                                (*)
                            </div>
                        </td>
                        <td>
                            <div class="editor-field">
                                <input type="datetime-local" id="FechaFinal" class="formatFechaHora datepicker" step="1" value="@Model.FechaFinal.ToString("yyyy-MM-ddTHH:mm:ss")" />

                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                    </tr>
                    <TextField label="Fecha y hora de salida o llegada *"
                               type="datetime-local"
                               defaultValue={moment().format("yyyy-MM-DDThh:mm:ss")}
                               color='primary'
                               InputLabelProps={{
     shrink: true,
   }}
                               fullWidth />
                </table>
            </div>

        </div>
    }


</div>