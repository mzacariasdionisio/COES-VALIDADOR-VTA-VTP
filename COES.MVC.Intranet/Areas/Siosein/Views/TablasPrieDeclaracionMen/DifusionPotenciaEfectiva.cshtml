<script src="~/Content/Scripts/highcharts-3d.js"></script>
<script src="https://code.highcharts.com/modules/data.js"></script>
<script src="https://code.highcharts.com/modules/drilldown.js"></script>
<script src="~/Content/Scripts/Charts/graficosBase.js"></script>
<script src="~/Areas/Siosein/Content/Scripts/tablaprie/3.difusion/difusionpotenciaefectiva.js"></script>
<script src="~/Areas/Siosein/Content/Scripts/reporteportal/generales.js"></script>