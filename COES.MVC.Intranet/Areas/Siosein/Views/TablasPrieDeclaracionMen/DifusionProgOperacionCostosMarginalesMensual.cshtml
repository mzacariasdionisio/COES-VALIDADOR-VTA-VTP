@model COES.MVC.Intranet.Areas.Siosein.Models.SioseinModel
@{
    ViewBag.Title = "Difusion Programa de Operacion Costos Marginales Mensual";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Content/Scripts/highcharts.js"></script>
<script src="https://code.highcharts.com/highcharts-3d.js"></script>
<script src="~/Content/Scripts/plupload.full.min.js"></script>
<link href="~/Content/Css/multiselect.css" rel="stylesheet" />
<script src="~/Content/Scripts/multiselect.js"></script>

<script src="~/Areas/Siosein/Content/Scripts/tablasprie/difusionprogoperacioncostosmarginalesmensual.js"></script>
<script src="~/Areas/Siosein/Content/Scripts/reporteportal/generales.js"></script>

<div class="form-title">
    <div class="content-titulo" style="width:100%;">Programa de Operación Costos Marginales Mensual (POCM) - Tabla 27</div>
    <div class="content-botonera">
        <input type="button" id="btnVolverListado" value="Volver al listado" />
    </div>
</div>

<div class="content-hijo" id="mainLayout">
    <div class="search-content">
        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td>Mes-Año:</td>
                <td>
                    <input type="text" disabled="disabled" id="txtFecha" name="Fecha" value="@Model.Mes" />
                </td>
                <td>Barra del Sein:</td>
                <td>
                    <select id="cbBarra" name="cbBarra" style="width:250px;" multiple="multiple">
                        @foreach (var item in Model.Barras)
                        {
                            <option value="@item.codigo">@item.text</option>
                        }
                    </select>
                    <input type="hidden" id="hfBarra" name="hfBarra" />
                </td>
                <td>Nivel de Tension (kV):</td>
                <td>
                    <select id="cbTension" name="cbTension" style="width:250px;" multiple="multiple">
                        @foreach (var item in Model.Tensiones)
                        {
                            <option value="@item.id">@item.text</option>
                        }
                    </select>
                    <input type="hidden" id="hfTension" name="hfTension" />
                </td>
                @*<td>
                    <input type="button" id="btnEjecutar" value="Ejecutar" />
                </td>*@
            </tr>
            <tr>
             <!--   <td>Empresa:</td>
                <td>
                    <select id="cbEmpresa" name="cbEmpresa" style="width:250px;" multiple="multiple">
                      foreach (var item in Model.ListaEmpresas)
                        {
                            <option value="item.id">item.text</option>
                        }
                    </select>
                    <input type="hidden" id="hfEmpresa" name="hfEmpresa" />
                </td>-->
                <td>
                    <input type="hidden" id="hfEmpresa" name="hfEmpresa" /></td>
            </tr>
           
        </table>
    </div>
    <div class="content-tabla">
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado1"></div>
        <div style="clear:both; height:5px"></div>
        <table>
            <tr>
                <td><div class="table-list" id="listado2"></div></td>
                <td><div id="idGrafico1" style="min-width: 310px; height: 600px; width: 900px; margin: 0 auto"></div></td>
            </tr>
        </table>
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado3"></div>
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado4"></div>
        <div style="clear:both; height:5px"></div>
        <div id="idGrafico2" style="min-width: 310px; height: auto; max-width: 900px; margin: 0 auto"></div>
        <div style="clear:both; height:5px"></div>
        <div id="idGrafico3" style="min-width: 310px; height: auto; max-width: 900px; margin: 0 auto"></div>
        <div style="clear:both; height:5px"></div>
        <div class="table-list" id="listado5"></div>
    </div>
</div>