@model COES.MVC.Intranet.Areas.SistemasTransmision.Models.TransmisorModel

<table border="0" class="pretty tabla-icono" id="tabla_2">
    <thead>
        <tr>
            <th style="width:150px">Empresa Transmisora</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaEmpresaTransmisor)
        {
            <tr id="filaET_@item.Stranscodi">
                <td style="text-align:left">@item.Emprnomb</td>
                <td style="width:30px;">
                    @if (Model.bEditar)
                    {
                        <a href="@Url.Action("Edit", new { id = item.Stranscodi })"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar el registro" alt="Editar el registro"/></a>
                    }
                </td>
                <td style="width:30px;">
                    @if (Model.bEliminar)
                    {
                        <a href="#" id="eliminar_@item.Stranscodi" class="deleteET"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
<div id="popup">    </div>