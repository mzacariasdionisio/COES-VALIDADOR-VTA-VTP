        <table class="content-tabla-search" style="width:auto">
            <tr>
                <td><input id="txtFecha" name="FechaGeneral" value="@Model.FechaGeneral" type="text" /></td>
            </tr>

        </table>
    </div>

    <div class="content-tabla">
    </div>

    <div id="tab-container" class='tab-container'>
        <ul class='etabs'>
            <li class='tab' id="tab1"><a href="#Despacho">Despacho AGC</a></li>
            <li class='tab'><a href="#Valoragua">Valor Agua</a></li>
            <li class='tab'><a href="#Costovariable">Costo Variable</a></li>
            <li class='tab'><a href="#Mantenimiento">Mantenimiento</a></li>
            <li class='tab'><a href="#Regulacionasignada">Regulaci&oacute;n Asignada</a></li>
            <li class='tab'><a href="#Log">Log</a></li>

        </ul>
        <div class='panel-container'>
            <div id="Despacho">
                <div id="tab-containerDespacho" class='tab-container'>
                    <ul class='etabs'>
                        <li class='tab-alter'><a href="#DespachoExportacion">Exportaci&oacute;n</a></li>
                        <li class='tab-alter'><a href="#DespachoConfiguracionPrincipal">Configuraci&oacute;n Unidades</a></li>
                    </ul>

                    <div class='panel-container'>

                        <div id="DespachoExportacion">
                            <table border="0">
                                <tr>
                                    <td>
                                        <fieldset>
                                            <legend>Opciones de exportaci&oacute;n</legend>
                                            <input id="rbDespExpOpcPdiario" name="rbDespExpOpc" type="radio" />Programa Diario <br>
                                            <input id="rbDespExpOpcReprograma" name="rbDespExpOpc" type="radio" value=listaHoras />Reprograma
                                            @Html.DropDownListFor(model => model.DespExpOpcReprograma, listaHoras)

                                        </fieldset>
                                    </td>
                                    <td rowspan="2">
                                        <p><p><input id="btnDespExpExportar" type="button" value="Exportar XML" />
                                    </td>
                                </tr>
                                <tr>
                                    <td>
                                        <fieldset>
                                            <legend>Mantenimiento / Restricciones Operativas</legend>
                                            <input id="rbDespExpMantEjecutado" name="rbDespExpMant" type="radio" value="" />Ejecutado<br>
                                            <input id="rbDespExpMantPdiario" name="rbDespExpMant" type="radio" value="" />Programa Diario
                                        </fieldset>


                                    </td>

                                </tr>
                            </table>
                        </div>

                        <div id="DespachoConfiguracionPrincipal">

                            <table>
                                <tr>
                                    <td>
                                        Estado:
                                        <input id="rbControlActivo" name="rbControl" type="radio" value="radiobutton" />Activo
                                        <input id="rbControlInactivo" name="rbControl" type="radio" value="radiobutton" />No activo
                                        <input id="rbControlTodos" name="rbControl" type="radio" value="radiobutton" />Todos
                                        <input id="btnControlRecuperar" type="button" value="Recuperar" />

                                        <input id="btnControlNuevo" type="button" value="Nuevo" />

                                    </td>
                                </tr>
                            </table>
                            <div class="tabla-header">
                                <div class="table-paginado" id="paginado"></div>
                            </div>
                            <div id="DespachoConfiguracion">
                            </div>

                        </div>

                    </div>


                </div>


            </div>

            <div id="Valoragua">

                <table border="0">
                    <tr>
                        <td>
                            <fieldset>
                                <legend>Opciones de importaci&oacute;n / exportaci&oacute;n</legend>
                                <input id="rbVaguaOpcPdiario" name="rbVaguaOpc" type="radio" value="" />Programa Diario <br>
                                <input id="rbVaguaOpcReprograma" name="rbVaguaOpc" type="radio" value="" />Reprograma
                                @Html.DropDownListFor(model => model.VaguaOpcReprograma, listaHoras, "-- Bloque --", new { @disabled = "disabled" })
                            </fieldset>
                        </td>
                        <td>&nbsp;</td>
                        <td>

                            <input id="btnVaguaImportar" type="button" value="Importar" />
                            <label id="fileInfoVagua" class="info">No se eligió un archivo.</label>
                            <label id="progreso" class="info"></label>
                            <div id="containerVagua"></div>
                            <input id="nombrearchivoVagua" type="hidden" name="Archivo" value="@Model.Archivo" />
                            <input type="hidden" id="hfArchivoVagua" />
                        </td>

                        <td><p><p><input id="btnVaguaExportar" type="button" value="Exportar XML" disabled /></td>
                        <td><p><p><input id="btnVaguaRecuperar" type="button" value="Recuperar" disabled /></td>
                    </tr>
                </table>

                <div id="ValoraguaTab">
                    <div id="tab-containerValoragua" class='tab-container'>
                        <ul class='etabs'>
                            <li class='tab-alter'><a href="#ValoraguaDatos">Datos</a></li>
                            <li class='tab-alter'><a href="#ValoraguaConfiguracion">Configuraci&oacute;n</a></li>

                        </ul>

                        <div class='panel-container'>
                            <div id="ValoraguaDatos"></div>
                            <div id="ValoraguaConfiguracion"></div>
                        </div>


                    </div>


                </div>


            </div>


            <div id="Costovariable">

                <div id="CostovariableTab">
                    <div id="tab-containerCostovariable" class='tab-container'>
                        <ul class='etabs'>
                            <li class='tab-alter'><a href="#CostovariableExportacion">Exportaci&oacute;n</a></li>
                            <li class='tab-alter'><a href="#CostovariableConfiguracion">Configuraci&oacute;n</a></li>


                        </ul>

                        <div class='panel-container'>
                            <div id="CostovariableExportacion">

                                <table border="0">
                                    <tr>
                                        <td>
                                            <table border="0">
                                                <tr>
                                                    <td>Inicio</td>
                                                    <td><input id="txtCvarExpFechaIni" name="FechaCVIni" value="@Model.FechaCVIni" type="text" /></td>
                                                </tr>
                                                <tr>
                                                    <td>Final</td>
                                                    <td><input id="txtCvarExpFechaFin" name="FechaCVFin" value="@Model.FechaCVFin" type="text" /></td>
                                                </tr>
                                            </table>
                                        </td>
                                        <td><input id="btnCvarExpRecuperar" type="button" value="Recuperar" /></td>
                                        <td><input id="btnCvarExpExportar" type="button" value="Exportar XML" /></td>

                                    </tr>
                                </table>

                                <div id="CostovariableEscenario"></div>


                            </div>
                            <div id="CostovariableConfiguracion"></div>

                        </div>


                    </div>


                </div>


            </div>


            <div id="Mantenimiento">



                <div id="MantenimientoTab">
                    <div id="tab-containerMantenimiento" class='tab-container'>
                        <ul class='etabs'>
                            <li class='tab-alter'><a href="#MantenimientoExportacion">Exportaci&oacute;n</a></li>
                            <li class='tab-alter'><a href="#MantenimientoUnidades">Unidades Configuradas</a></li>

                        </ul>

                        <div class='panel-container'>
                            <div id="MantenimientoExportacion">

