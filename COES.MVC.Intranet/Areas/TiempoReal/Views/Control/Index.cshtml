@model COES.MVC.Intranet.Areas.TiempoReal.Models.BusquedaAgcControlModel

<script src="~/Content/Scripts/plupload.full.min.js" type="text/javascript"></script>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <script src="~/Content/Scripts/jquery.easytabs.min.js"></script>

    <link href="~/Areas/TiempoReal/Content/Css/normal.css" rel="stylesheet" />
    <link href="~/Content/Css/tabs.css" rel="stylesheet" />


}

<script src="~/Areas/TiempoReal/Content/Scripts/control.js?v=1"></script>
<link href="~/Areas/TiempoReal/Content/Css/css.css" rel="stylesheet" />
<script src="~/Content/Scripts/inputmaskmin.js"></script>

<div class="form-title">
    <div class="form-title">
        <div class="content-titulo">Formatos AGC</div>
        <div class="content-botonera">
        </div>
    </div>
</div>

