                                                <input id="rbMantExpOpcEjecutado" name="rbMantExpOpc" type="radio" value="" />Ejecutado

                                            </fieldset>
                                        </td>
                                        <td>&nbsp;</td>

                                        <td><p><p><input id="btnMantExpExportar" type="button" value="Exportar XML" /></td>

                                    </tr>
                                </table>

                            </div>
                            <div id="MantenimientoUnidades"></div>
                        </div>


                    </div>


                </div>


            </div>



            <div id="Regulacionasignada">

                <table border="0">
                    <tr>
                        <td>
                            <fieldset>
                                <legend>Opciones de importaci&oacute;n / exportaci&oacute;n</legend>
                                <input id="rbPotenciaOpcPdiario" name="rbPotenciaOpc" type="radio" value="" />Programa Diario <br>
                                <input id="rbPotenciaOpcReprograma" name="rbPotenciaOpc" type="radio" value="" />Reprograma
