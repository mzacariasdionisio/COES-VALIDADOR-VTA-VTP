<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th>Barra de Transferencia</th>
            <th>Cliente</th>
            <th>Ruc Cliente</th>
            <th>Tipo Usuario</th>
            <th>Inicio de Operación</th>
            <th>Fin de Operación</th>
            <th>Código de Retiro</th>
            <th>Estado</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
            <th style="width:30px;">&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaCodigoRetiroSinContrato)
        {
            <tr id="fila_@item.CodRetiSinConCodi">
                <td class="text_left">@item.BarrNombBarrTran</td>
                <td class="text_left">@item.CliNombre</td>
                <td class="text_left">@item.CliRuc</td>
                <td class="text_left">@item.TipUsuNomb</td>
                <td>@item.CodRetiSinConFechaInicio.ToString("dd/MM/yyyy")</td>
                <td>
                    @if (item.CodRetiSinConFechaFin != null)
                    {<span>@item.CodRetiSinConFechaFin.ToString("dd/MM/yyyy")</span>}
            </td>
            <td class="text_left">@item.CodRetiSinConCodigo</td>
            <td class="text_left">
                @if (item.CodRetiSinConEstado == "ACT")
                {<span>Activo</span> }
                else
                { <span>Inactivo</span>}
            </td>
            <td style="width:30px;"><a href="#" id="view_@item.CodRetiSinConCodi" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>
            <td style="width:30px;">
                @if (Model.bEditar)
                {
                    <a href="@Url.Action("Edit", new { id = item.CodRetiSinConCodi })"><img src="@Url.Content("~/Content/Images/btn-edit.png")" title="Editar el registro" alt="Editar el registro" /></a>
                }
            </td>
            <td style="width:30px;">
                @if (Model.bEliminar)
                {
                    <a href="#" id="eliminar_@item.CodRetiSinConCodi" class="delete"><img src="~/Content/Images/btn-cancel.png" title="Eliminar el registro" alt="Eliminar el registro" /></a>
                }
            </td>
            </tr>
        }
    </tbody>

</table>
<div id="popup"></div>