@model COES.MVC.Intranet.Areas.Transferencias.Models.ConceptoModel
@Html.AntiForgeryToken()
@{
    ViewBag.Title = "Transferencias - Registro Otros Conceptos";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var sMensajeExito = TempData["sMensajeExito"];
    string sSelected = "";
}
<script src="~/Areas/Transferencias/Content/Scripts/concepto.js?v=1.18"></script>
<script src="~/Content/Scripts/multiselect.js"></script>
<script src="~/Content/Scripts/datatable.js"></script>


<div class="ast">
    @if (@sMensajeExito != null)
    {<div class="exito mensajes">@sMensajeExito</div>}

    <div class="form-title">
        <div class="content-titulo">Registro Otros Conceptos</div>
        <div class="content-botonera">
            <input type="button" value="Nuevo" id="btnNuevo" />
        </div>
    </div>

    <div class="content-hijo">
        <table id="tbConceptos" class="pretty tabla-icono"></table>
    </div>

    <div class="content-hijo">
        <table id="tbDetalleVersiones" class="pretty tabla-icono"></table>
    </div>
</div>

<div id="popNuevo" class="general-popup" style="display:none">

    <span class="button b-close"><span>X</span></span>
    <div class="popup-title"><span>Datos del registro</span></div>
    <div id="depgrid" class="popup-text" style="height:200px; width: 300px">
        <input type="hidden" id="Infadicodi" value="0" />
        <table>
            <tr></tr>
            <tr>
                <td>Concepto</td>
                <td>
                    <input type="text" id="txtConcepto" style="width: 150px" />
                </td>
            </tr>
            <tr>
                <td>Cod. Osinergmin</td>
                <td>
                    <input type="text" id="txtCodOsinergmin" style="width: 150px" />
                </td>
            </tr>
            <tr>
                <td>Tipo de Empresa</td>
                <td>
                    <select id="cboTipoEmpresa" style="width: 150px"></select>
                </td>
            </tr>
            <tr>
                <td>Empresa</td>
                <td>
                    <select id="cboEmpresa" style="width: 150px"></select>
                </td>
            </tr>
            <tr><td colspan="2">&nbsp;</td></tr>
            <tr>
                <td colspan="2" style="text-align:center">
                    <input type="button" id="btnGrabar" value="Grabar" style="width: 100px" />
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <input type="button" id="btnCancelar" value="Cancelar" style="width: 100px" />
                </td>
            </tr>
        </table>
    </div>
</div>