
<table border="0" class="pretty tabla-icono" id="tabla">
    <thead>
        <tr>
            <th>Empresa</th>
            <th>Cliente</th>
            <th>Cliente Ruc</th>
            <th>Barra Transferencia</th>
            <th>Fecha Inicio</th>
            <th>Fecha Fin</th>
            <th>Tipo Contrato</th>
            <th>Tipo Usuairo</th>
            <th>Descripcion</th>
            <th>Codigo</th>
            <th style="width:30px;">&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaCodigoRetiro)
        {
            if (item.SoliCodiRetiObservacion.Equals("SOLBAJANO"))
            {@:<tr id="fila_@item.SoliCodiRetiCodi">
                    @: <td class="text_left">@item.EmprNombre</td>
                    @: <td class="text_left">@item.CliNombre</td>
                    @: <td class="text_left">@item.CliRuc</td>
                    @: <td class="text_left">@item.BarrNombBarrTran</td>
                    @: <td>@item.SoliCodiRetiFechaInicio.ToString("dd/MM/yyyy")</td>
                    @: <td>@if (item.SoliCodiRetiFechaFin != null)
                    {<span>@item.SoliCodiRetiFechaFin.ToString("dd/MM/yyyy")</span>}</td>
                    @: <td class="text_left">@item.TipoContNombre</td>
                    @: <td class="text_left">@item.TipoUsuaNombre</td>
                    @: <td class="text_left">@item.SoliCodiRetiDescripcion</td>
                    @: <td class="text_left"><b>@item.SoliCodiRetiCodigo</b></td>
                    @: <td style="width:30px;"><a href="#" id="view_@item.SoliCodiRetiCodi" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>                    
                @:</tr>
            }
            else if (item.SoliCodiRetiObservacion.Equals("SOLBAJAOK"))
            { @:<tr id="fila_@item.SoliCodiRetiCodi">
                    @: <td class="text_left" style="background-color: #FFDDDD;">@item.EmprNombre</td>
                    @: <td class="text_left" style="background-color: #FFDDDD;">@item.CliNombre</td>
                    @: <td class="text_left" style="background-color: #FFDDDD;">@item.CliRuc</td>
                    @: <td class="text_left" style="background-color: #FFDDDD;">@item.BarrNombBarrTran</td>
                    @: <td style="background-color: #FFDDDD;">@item.SoliCodiRetiFechaInicio.ToString("dd/MM/yyyy")</td>
                    @: <td style="background-color: #FFDDDD;">@if (item.SoliCodiRetiFechaFin != null)
                        {<span>@item.SoliCodiRetiFechaFin.ToString("dd/MM/yyyy")</span>}</td>
                @: <td class="text_left" style="background-color: #FFDDDD;">@item.TipoContNombre</td>
                @: <td class="text_left" style="background-color: #FFDDDD;">@item.TipoUsuaNombre</td>
                @: <td class="text_left" style="background-color: #FFDDDD;">@item.SoliCodiRetiDescripcion</td>
                @: <td class="text_left" style="background-color: #FFDDDD;">@item.SoliCodiRetiCodigo</td>
                @: <td style="width:30px; background-color: #FFDDDD;"><a href="#" id="view_@item.SoliCodiRetiCodi" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>                
    @:</tr>
}
else if (item.SoliCodiRetiObservacion.Equals("SOLBAJAPEN"))
{ @:<tr id="fila_@item.SoliCodiRetiCodi" class="amarillo">
        @: <td class="text_left">@item.EmprNombre</td>
        @: <td class="text_left">@item.CliNombre</td>
        @: <td class="text_left">@item.CliRuc</td>
        @: <td class="text_left">@item.BarrNombBarrTran</td>
        @: <td>@item.SoliCodiRetiFechaInicio.ToString("dd/MM/yyyy")</td>
        @: <td>@if (item.SoliCodiRetiFechaFin != null){<span>@item.SoliCodiRetiFechaFin.ToString("dd/MM/yyyy")</span>}</td>
        @: <td class="text_left">@item.TipoContNombre</td>
        @: <td class="text_left">@item.TipoUsuaNombre</td>
        @: <td class="text_left">@item.SoliCodiRetiDescripcion</td>
        @: <td class="text_left">@item.SoliCodiRetiCodigo</td>
        @: <td style="width:30px;"><a href="#" id="view_@item.SoliCodiRetiCodigo" class="view"><img src="@Url.Content("~/Content/Images/btn-open.png")" title="Ver informacion del registro" alt="Ver informacion del registro" /></a></td>        
        @:</tr>
}
}
    </tbody>
</table>
<div id="popup"></div>