@model COES.MVC.Intranet.Areas.Transferencias.Models.ValorTransferenciaModel

@Html.AntiForgeryToken()
<table border="0" class="pretty" id="tabla">
    <thead>
        <tr>
            <th>Empresa</th>
            <th>Barra de Transferencia</th>
            <th>Tipo usuario</th>
            <th>Tipo de contrato</th>
            <th>Entrega / Retiro</th>
            <th>Energía MW.h</th>
            <th>Valorización S/.</th>            
            <th>Renta de Congestión de Licitación S/.</th>
            <th>Renta de Congestión de Bilateral S/.</th>
            <th>Información</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListaValorTransferencia)
        {
            <tr id="fila_@item.ValoTranCodi">
                <td class="text_left">@item.EmprNomb</td>
                <td class="text_left">@item.BarrNombBarrTran</td>
                <td class="text_left">@item.VtranUserName</td>
                <td class="text_left">@item.CentGeneNombre</td>
                <td class="text_left">@item.ValoTranCodEntRet</td>
                <td style="text-align:right;">@item.VTTotalEnergia.ToString("#,##0.000")</td>
                <td style="text-align:right;">@item.VTTotalDia.ToString("#,##0.00")</td>                
                <td style="text-align:right;">@item.RcLicitacion.ToString("#,##0.00")</td>
                <td style="text-align:right;">@item.RcBilateral.ToString("#,##0.00")</td>
                <td class="text_left">@item.VTTipoInformacion </td>
            </tr>
        }
    </tbody>
</table>>