                    {
                        <option value="@empresa.Emprcodi" data-id="@empresa.Emprcodi">@empresa.Emprnomb</option>
                    }
                </select>
                Fecha de Inicio: <input type="text" id="txtFechaInicioMC" value="@ViewBag.FechaFormatoMesAnio" />
                Fecha Final: <input type="text" id="txtFechaFinalMC" value="@ViewBag.FechaFormatoMesAnio" />
                <input type="button" id="btnBuscarMontoCapacidad" value="Consultar" />
                <input type="button" id="btnExportarExcelMC" value="Exportar" />
            </div>

            <div class="content-tabla">
                <div class="tabla-header">
                    <div class="table-paginado" id="PaginadoMontoCapacidad"></div>
                </div>
                <div style="clear:both; margin-bottom:10px"></div>
                <div class="table-list" id="ListadoMontoCapacidad"></div>

                <div style="clear:both; margin-bottom:40px"></div>
            </div>
        </div>

        <div id="MontoPorPeaje">
