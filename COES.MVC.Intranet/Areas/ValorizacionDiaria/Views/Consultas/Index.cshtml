@{
    ViewBag.Title = "Consultas";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<link href="~/Content/Css/form.css" rel="stylesheet" />
<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<link href="~/Content/Css/popup.css" rel="stylesheet" />

<div class="form-title">
    <div class="content-titulo">Consultas</div>
    <div class="content-botonera"></div>
</div>

<div class="content-hijo" id="mainLayout">

    <div id="tab-container" class="tab-container" style="min-height:700px">
        <ul class='etabs'>
            <li class='tab'><a href="#MontoPorEnergia">Monto Por Energía</a></li>
            <li class='tab'><a href="#MontoPorCapacidad">Monto Por Capacidad</a></li>
            <li class='tab'><a href="#MontoPorPeaje">Monto por Peaje por conexión en SPT y peaje por trans. en el SGT</a></li>
            <li class='tab'><a href="#CalculosSCIO">Montos por SC e IO</a></li>
            <li class='tab'><a href="#MontoPorExceso">Monto Por Exceso de consumo de Energía Reactiva</a></li>
        </ul>

        <div id="MontoPorEnergia">
            <div style="padding-top:20px;">
            </div>
            <div class="search-content">
                <label>Empresa:</label>
                <select id="cbxEmpresaME">
                    @foreach (var empresa in Model.Empresa)
                    {
                        <option value="@empresa.Emprcodi" data-id="@empresa.Emprcodi">@empresa.Emprnomb</option>
                    }
                </select>
                Fecha de Inicio: <input type="text" id="txtFechaInicioMR" value="@ViewBag.Fecha" />
                Fecha Final: <input type="text" id="txtFechaFinalMR" value="@ViewBag.Fecha" />
                <input type="button" id="btnBuscarMontoEnergia" value="Consultar" />
                <input type="button" id="btnExportarExcelME" value="Exportar" />
            </div>

            <div class="content-tabla">
                <div class="tabla-header">
                    <div class="table-paginado" id="PaginadoMontoEnergia"></div>
                </div>
                <div style="clear:both; margin-bottom:10px"></div>
                <div class="table-list" id="ListadoMontoPorEnergia"></div>

                <div style="clear:both; margin-bottom:40px"></div>
            </div>
        </div>

        <div id="MontoPorCapacidad">
            <div style="padding-top:20px;">
            </div>
            <div class="search-content">
                <label>Empresa:</label>
                <select id="cbxEmpresaMC">
