                        <option value="@empresa.Emprcodi" data-id="@empresa.Emprcodi">@empresa.Emprnomb</option>
                    }
                </select>
                Fecha de Inicio: <input type="text" id="txtFechaInicioMEx" value="@ViewBag.Fecha" />
                Fecha Final: <input type="text" id="txtFechaFinalMEx" value="@ViewBag.Fecha" />
                <input type="button" id="btnBuscarMontoPorExceso" value="Consultar" />
                <input type="button" id="btnExportarExcelMEx" value="Exportar" />
