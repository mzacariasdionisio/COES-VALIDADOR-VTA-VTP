                <div style="clear:both; margin-bottom:40px"></div>
            </div>
        </div>

        <div id="MontoPorExceso">
            <div style="padding-top:20px;">
            </div>
            <div class="search-content">
                <label>Empresa:</label>
                <select id="cbxEmpresaMEx">
                    @foreach (var empresa in Model.Empresa)
