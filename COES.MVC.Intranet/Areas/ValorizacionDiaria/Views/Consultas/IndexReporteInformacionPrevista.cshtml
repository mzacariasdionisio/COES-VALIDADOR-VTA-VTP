@model COES.MVC.Intranet.Areas.ValorizacionDiaria.Models.ConsultasModel

@{
    ViewBag.Title = "Reporte";
    Layout = "~/Views/Shared/_Layout.cshtml";

}
<script src="~/Areas/ValorizacionDiaria/Content/Scripts/globales.js?id=13.2.7"></script>
<script src="~/Areas/ValorizacionDiaria/Content/Scripts/excelweb.js?v=13.2.7"></script>
<link href="~/Content/Css/handsontable.full.min.css" rel="stylesheet" />
<script src="~/Content/Scripts/Handson/moment.js"></script>
@*<script src="~/Areas/ValorizacionDiaria/Content/Scripts/handson.js"></script>*@
<script src="~/Content/Scripts/handsontable.full.min.js"></script>
<script src="~/Areas/ValorizacionDiaria/Content/Scripts/handsontable.js?id=13.2.8"></script>
<script src="~/Areas/ValorizacionDiaria/Content/Scripts/util.js"></script>
<link href="~/Content/Css/form.css" rel="stylesheet" />
<link href="~/Content/Css/tabs.css" rel="stylesheet" />
<link href="~/Content/Css/popup.css" rel="stylesheet" />

<div class="form-title">
    <div class="content-titulo">Información Prevista Remitida Por el Participante</div>
    <div class="content-botonera">
        <input type="button" id="btnExportarExcelIPRP" value="Exportar" />
    </div>
</div>

<div class="content-hijo" id="mainLayout">
    <div class="search-content">
        <label>Empresa:</label>
        <select id="cbxEmpresa">
            @foreach (var empresa in Model.Empresa)
            {
                <option value="@empresa.Emprcodi" data-id="@empresa.Emprcodi">@empresa.Emprnomb</option>
            }
        </select>
        <select id="cbxFormato">           
            <option value="101">DIARIA</option>            
            <option value="102">TRIMESTRAL</option>            
        </select>
        <label>Fecha de Inicio:</label>
        <input type="text" name="Fecha" id="txtFechaInicioIPRP" value="@ViewBag.Fecha" />
        <label>Fecha Final:</label>
        <input type="text" name="Fecha" id="txtFechaFinalIPRP" value="@ViewBag.Fecha" />
        <input type="button" id="btnBuscarInformacionPRP" value="Consultar" />
    </div>
    <div class="bodyexcel" id="detalleFormato"></div>
    <div class="content-tabla" style="overflow:auto;">
        <div class="tabla-header">
            <div class="table-paginado" id="PaginadoInformacionPRP"></div>
        </div>
        <div style="clear:both; margin-bottom:10px"></div>
        <div class="table-list" id="ListadoInformacionPRP"></div>
        <div style="clear:both; margin-bottom:40px"></div>
    </div>
</div>

<script src="~/Content/Scripts/jquery.js"></script>
<script src="~/Content/Scripts/jquery.easytabs.min.js"></script>
<script src="~/Content/Scripts/popup.js"></script>
<script src="~/Content/Scripts/datepicker.js"></script>
<script src="~/Content/Scripts/jquery-ui-autocomplete.js"></script>
<script src="~/Content/Scripts/jquery.select-to-autocomplete.min.js"></script>
<script src="~/Areas/ValorizacionDiaria/Content/Scripts/InformacionPrevista.js"></script>