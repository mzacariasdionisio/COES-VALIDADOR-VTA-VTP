@model COES.MVC.Intranet.Areas.Web.Models.TipoEventoModel
@{
    ViewBag.Title = "Calendario COES";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/Css/form.css" rel="stylesheet" />
<link href="~/Areas/Web/Content/Css/css.css" rel="stylesheet" />
<script src="~/Areas/Web/Content/Scripts/tipoevento.js"></script>
<script src="~/Content/Scripts/jqueryui.js"></script>
<link href="~/Areas/Web/Content/Scripts/Calendar/evol-colorpicker.min.css" rel="stylesheet" />
<script src="~/Areas/Web/Content/Scripts/Calendar/evol-colorpicker.min.js"></script>


<div class="form-title">
    <div class="content-titulo">Tipos de Eventos</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevo" value="Nuevo" />
        <input type="button" id="btnCancelar" value="Cancelar" />
    </div>
</div>

<div class="content-hijo">

    <div id="mensaje" class="action-message">Seleccione un registro para editar</div>
    <div id="listado">

    </div>

</div>

<div id="popupEdicion" class="general-popup" style="width:400px">
    <div><span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Datos del Registro</span></div>
    <div class="popup-text" id="contenidoEdicion">

    </div>
</div>