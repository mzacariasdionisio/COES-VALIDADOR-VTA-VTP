@model COES.MVC.Intranet.Areas.Web.Models.SubscripcionModel
@{
    ViewBag.Title = "Administrar publicaciones";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Areas/Web/Content/Scripts/publicacion.js"></script>
<link href="~/Content/Css/form.css" rel="stylesheet" />

<div class="form-title">
    <div class="content-titulo">Administración de Publicaciones</div>
    <div class="content-botonera">
        <input type="button" id="btnNuevo" value="Nuevo" />
        <input type="button" id="btnCancelar" value="Cancelar" />
    </div>
</div>

<div class="content-hijo">
    <div id="mensaje" class="action-message">Configure los datos por cada publicación</div>
    
    <div id="listado">
    </div>
    <br />
    <br />
</div>

<div id="popupEdicion" class="general-popup" style="width:500px">
    <div><span class="button b-close"><span>X</span></span></div>
    <div class="popup-title"><span>Datos de la Publicación</span></div>
    <div class="popup-text" id="contenidoEdicion">

    </div>
</div>