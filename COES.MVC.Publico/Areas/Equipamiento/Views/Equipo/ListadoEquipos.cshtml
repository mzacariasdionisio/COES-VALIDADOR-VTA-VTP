@model COES.MVC.Publico.Areas.Equipamiento.Models.IndexEquipoModel

<table border="0" class="tabla-icono table table-hover" cellspacing="0" width="100%" id="tabla">
    <thead>
        <tr>
            <th>Empresa</th>
            <th>Tipo de Equipo</th>
            <th>Ubicación</th>
            <th>Nombre de equipo</th>
            <th>Abreviatura</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ListadoEquipamiento)
        {
            <tr>
                <td style="@item.Osigrupocodi">@item.EMPRNOMB.Trim()</td>
                <td style="@item.Osigrupocodi">@item.Famnomb</td>
                <td style="@item.Osigrupocodi">@item.AREANOMB</td>
                <td style="@item.Osigrupocodi">@item.Equinomb</td>
                <td style="@item.Osigrupocodi">@item.Equiabrev</td>
            </tr>
        }
    </tbody>
</table>