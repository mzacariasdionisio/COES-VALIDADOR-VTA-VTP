@model COES.MVC.Publico.Areas.Interconexiones.Models.InterconexionesModel
@{
    System.Globalization.NumberFormatInfo nfi = new System.Globalization.CultureInfo("en-US", false).NumberFormat;
    nfi.NumberGroupSeparator = " ";
    nfi.NumberDecimalSeparator = ",";
    nfi.NumberDecimalDigits = 3;    
    }
<div style="clear:both; height:15px"></div>
<div id="reporte" class="marcoGrafico" style="float:left;height:600px;">
<table border="0" class="pretty tabla-icono" cellspacing="0" width="430" id="tabla2">
    <thead>
        <tr>
            <th>Fecha</th>
            <th>Energía <br />Exportada <br />(MWh)</th>
            <th>Máxima Demanda <br />Exportada <br />(MW)</th>
            <th>Energía <br />Importada <br />(MWh)</th>
            <th>Máxima Demanda <br />Importada <br />(MW)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var reg in Model.ListaEnerPot)
        {
            var enerExp = ((decimal)reg.H1).ToString("N", nfi);
            var mdExp = ((decimal)reg.H2).ToString("N", nfi);
            var enerImp = ((decimal)reg.H3).ToString("N", nfi);
            var mdImp = ((decimal)reg.H4).ToString("N", nfi);
            
        <tr>
            <td>@reg.Medifecha.ToString("dd/MM/yyyy")</td>
            <td>@enerExp</td>
            <td>@mdExp</td>
            <td>@enerImp</td>
            <td>@mdImp</td>
        </tr>
        }
    </tbody>
</table>
</div>

<div class="chart-wrapper" style="position: relative;width:50%;float:left;">
<div id="graficos2" class="marcoGrafico" style="width: 100%;height:600px;">
</div>    
</div>