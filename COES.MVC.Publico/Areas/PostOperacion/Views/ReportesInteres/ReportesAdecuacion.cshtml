<script>
    var calendarAnioActual = new Date().getFullYear();

    $('.listado-archivo [data-year]').each(function () {
        var currentElement = $(this);
        if (currentElement.attr("data-year") == calendarAnioActual) {
            $(currentElement).show();
        } else {
            $(currentElement).hide();
        }

    });

    $("[data-target]").on("click", function () {
        calendarAnioActual = $(this).text();

        $('.listado-archivo [data-year]').each(function () {
            var currentElement = $(this);

            if (currentElement.attr("data-year") == calendarAnioActual) {
                $(currentElement).show();
            }
            else {
                $(currentElement).hide();
            }
        });
    });

</script>