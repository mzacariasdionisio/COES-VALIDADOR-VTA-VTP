@model COES.MVC.Publico.Areas.RegistroIntegrante.Models.IntegranteModel
@using COES.Servicios.Aplicacion.RegistroIntegrantes;
@{
    ViewBag.Title = "Integrante";
    Layout = "~/Views/Shared/_Layout.cshtml";
    SelectList slTipoAgente = new SelectList(Model.TipoEmpresa, "Tipoemprcodi", "Tipoemprdesc", Model.Empresa.Tipoemprcodi);
    SelectList slTipoDocumentoSustentatorio = new SelectList(Model.TipoDocumentoSustentario, "Id", "Nombre");
}
@*<script src='https://www.google.com/recaptcha/api.js' type="text/javascript"></script>*@
<script src="~/Areas/RegistroIntegrante/Content/Script/integrantes.js?v=8.25"></script>
@*<link href="~/Areas/RegistroIntegrante/Content/Css/css.css" rel="stylesheet" />*@

@{
    var Disabled = "";
}

<main>
    <div class="main-banner" style="background-image: url(../../Content/Images/Banners/RegistroDeIntegrantes.jpg)"></div>
    <div style="clear:both"></div>


    <div class="content-area">
        <div class="container">
            <div class="coes-box">
                <div class="coes-tabs directory-form--container">
                    <nav class="coes-tabs--navigation">
                        <button class="coes-tab coes-tab--active" data-tab-target="1" id="btnTab1" onclick="switchToTab('tabs1-css')">1. Solicitud de Ingreso</button>
                        <button class="coes-tab" data-tab-target="2" id="btnTab2" onclick="switchToTab('tabs2-css')">2. Responsable del Trámite</button>
                        <button class="coes-tab" data-tab-target="3" id="btnTab3" onclick="switchToTab('tabs3-css')">3. Datos de Empresa</button>
                        <button class="coes-tab" data-tab-target="4" id="btnTab4" onclick="switchToTab('tabs4-css')">4. Tipo de Integrante</button>
                        <button class="coes-tab" data-tab-target="5" id="btnTab5" onclick="switchToTab('tabs5-css')">5. Representante Legal</button>
                        <button class="coes-tab" data-tab-target="6" id="btnTab6" onclick="switchToTab('tabs6-css')">6. Persona de Contacto</button>
                    </nav>

                    <div class="coes-tab--content" id="tabs1-css" style="display: contents;">
                        <div class="coes-box--content container pt-5 pb-5 pe-5 ps-5">
                            <div class="coes-form-item mb-3">
                                <ul>
                                    <li>
                                        <p>
                                            En el Artículo 3 “Integrantes y Registro” del Reglamento del COES, se establecen las condiciones que deben cumplir los
                                            Generadores, Transmisores, Distribuidores y Usuarios Libres, a fin de que sean Integrantes del COES (Decreto Supremo N°
                                            027-2008-EM, actualizado al 05 de mayo de 2011).
                                        </p>
                                    </li>
                                    <li>
                                        <p>
                                            Para inscribirse como Integrante del COES, las instalaciones del Agente deben de contar con el Certificado de
                                            Conformidad del Estudio de Operatividad, emitido por el COES según el Procedimiento Técnico "Ingreso,
                                            Modificación y Retiro de las Instalaciones en el SEIN (PR-20)".
                                        </p>
                                    </li>
                                    <li>
                                        <p>
                                            La empresa solicitará su inscripción una vez culminado el registro de sus datos mediante el presente formulario
                                            y luego será revisado y aprobado por el COES.
                                        </p>
                                    </li>
                                    <li>
                                        <p>
                                            De no presentarse alguna observación en un plazo máximo de 15 días hábiles, el COES pronunciará la
                                            aceptación del Integrante mediante un envió automático desde el sistema de Registro de Integrantes,
                                            proporcionándoles sus accesos respectivos a los Representantes legales acreditados.
                                        </p>
                                    </li>
                                </ul>
                            </div>
                            <br />
                            <div class="coes-form-item d-flex">
                                <input type="button" class="coes-form-item--submit mb-3" id="btnTab1ToTab2" value="Siguiente" onclick="Tabs('tabs2-css')">
                            </div>
                        </div>
                    </div>

                    <div id="tabs2-css" class="coes-tab--content">
                        <div class="coes-box--content container pt-5 pb-5 pe-5 ps-5">
                            <form action="" class="w-100">
                                <div class="coes-form-item--info coes-form-item coes-box coes-box--content pt-3 pe-5 ps-5 mb-5">
                                    <p>
                                        @Model.TituloPersonaResponsable
                                    </p>
                                </div>
                                <div id="mensajeRT" class="coes-form-item--error coes-form-item coes-box coes-box--content pt-3 pb-3 pe-5 ps-5 mb-3" style="display: none;">
                                    <span>
                                        Por favor complete los datos solicitados.
                                    </span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">2.1 Nombres:</label>
                                    <input type="text" id="txtNombresRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rptenombres" disabled readonly data-validator-id="spnNombresRT">
                                    <span class="ps-2" id="spnNombresRT">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">2.2 Apellidos:</label>
                                    <input type="text" id="txtApellidosRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rpteapellidos" disabled readonly data-validator-id="spnApellidosRT">
                                    <span class="ps-2" id="spnApellidosRT">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">2.3 Cargo en la empresa:</label>
                                    <input type="text" id="txtCargoEmpresaRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rptecargoempresa" disabled readonly data-validator-id="spnCargoEmpresaRT">
                                    <span class="ps-2" id="spnCargoEmpresaRT">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">2.4 Teléfono:</label>
                                    <input type="text" id="txtTelefonoRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rptetelefono" disabled readonly data-validator-id="spnTelefonoRT">
                                    <span class="ps-2" id="spnTelefonoRT">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">2.5 Teléfono Móvil:</label>
                                    <input type="text" id="txtTelefonoMovilRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rptetelfmovil" disabled readonly>
                                    <span class="ps-2">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">2.6 Correo Electrónico:</label>
                                    <input type="text" id="txtCorreoElectronicoRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rptecorreoelectronico" disabled readonly data-validator-id="spnCorreoElectronicoRT">
                                    <span class="ps-2" id="spnCorreoElectronicoRT">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">Repetir Correo Electrónico:</label>
                                    <input type="text" id="txtCorreoElectronicoRepetirRT" class="ResponsableTramite" value="@Model.ResponsableTramite.Rptecorreoelectronico" disabled readonly data-validator-id="spnCorreoElectronicoRepetirRT">
                                    <span class="ps-2" id="spnCorreoElectronicoRepetirRT">(*)</span>
                                </div>
                                <div class="coes-form-item">
                                    <p>(*) Datos obligatorios</p>
                                </div>
                                <br />
                                <div class="coes-form-item d-flex">
                                    <input type="button" class="coes-form-item--submit me-3 mb-3" id="btnTab2ToTab1" value="Anterior" onclick="Tabs('tabs1-css')">
                                    <input type="button" class="coes-form-item--submit mb-3" id="btnTab2ToTab3" value="Siguiente" onclick="Tabs('tabs3-css')">
                                </div>

                            </form>
                        </div>
                    </div>


                    <div class="coes-tab--content" id="tabs3-css">
                        <div class="coes-box--content container pt-5 pb-5 pe-5 ps-5">
                            <form action="" class="w-100">
                                <div class="coes-form-item--info coes-form-item coes-box coes-box--content pt-3 pe-5 ps-5 mb-5">
                                    <p>
                                        @Model.TituloEmpresa
                                    </p>
                                </div>
                                <div id="mensajeEMP" class="coes-form-item--error coes-form-item coes-box coes-box--content pt-3 pb-3 pe-5 ps-5 mb-3" style="display: none;">
                                    <span>
                                        Por favor complete los datos solicitados.
                                    </span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.1 Número de RUC:</label>
                                    <input type="text" id="txtNumeroRucDE" class="DatosEmpresa" value="@Model.Empresa.Emprruc" disabled readonly data-validator-id="spntxtNumeroRucDE" data-required="true" style="width:280px;">
                                    <span class="ps-2" id="spntxtNumeroRucDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.2 Nombre Comercial:</label>
                                    <input type="text" id="txtNombreComercialDE" class="DatosEmpresa" value="@Model.Empresa.Emprnombrecomercial" disabled readonly data-validator-id="spntxtNombreComercialDE" data-required="true">
                                    <span class="ps-2" id="spntxtNombreComercialDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.3 Denominación o Razón Social:</label>
                                    <input type="text" id="txtDenominacionRazonSocialDE" class="DatosEmpresa" value="@Model.Empresa.Emprrazsocial" disabled readonly data-validator-id="spnDenominacionRazonSocialDE" data-required="true">
                                    <span class="ps-2" id="spnDenominacionRazonSocialDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.4 Domicilio Legal:</label>
                                    <input type="text" id="txtDomicilioLegalDE" class="DatosEmpresa" value="@Model.Empresa.Emprdomiciliolegal" disabled readonly data-validator-id="spnDomicilioLegalDE" data-required="true">
                                    <span class="ps-2" id="spnDomicilioLegalDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.5 Sigla:</label>
                                    <input type="text" id="txtSiglaDE" class="DatosEmpresa" value="@Model.Empresa.Emprsigla" disabled readonly data-validator-id="spnSiglaDE" data-required="true" maxlength="5">
                                    <span class="ps-2" id="spnSiglaDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.6 Número de Partida Registral:</label>
                                    <input type="text" id="txtNumeroPartidaRegistralDE" class="DatosEmpresa" value="@Model.Empresa.Emprnumpartidareg" disabled readonly data-validator-id="spnNumeroPartidaRegistralDE" data-required="true">
                                    <span class="ps-2" id="spnNumeroPartidaRegistralDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.7 Teléfono:</label>
                                    <input type="text" id="txtTelefonoDE" class="DatosEmpresa" value="@Model.Empresa.Emprtelefono" disabled readonly data-required="true" data-validator-id="spnTelefonoDE">
                                    <span class="ps-2" id="spnTelefonoDE">(*)</span>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.8 Fax:</label>
                                    <input type="text" id="txtFaxDE" class="DatosEmpresa" value="@Model.Empresa.Emprfax" disabled readonly>
                                </div>

                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">4.9 Página web:</label>
                                    <input type="text" id="txtPaginaWebDE" class="DatosEmpresa" value="@Model.Empresa.Emprpagweb" disabled readonly>
                                </div>
                                <div class="coes-form-item">
                                    <p>(*) Datos obligatorios</p>
                                </div>
                                <div class="coes-form-item d-flex">
                                    <input type="button" class="coes-form-item--submit me-3 mb-3" id="btnTab3ToTab2" value="Anterior" onclick="Tabs('tabs2-css')">
                                    <input type="button" class="coes-form-item--submit mb-3" id="btnTab3ToTab4" value="Siguiente" onclick="Tabs('tabs4-css')">
                                </div>

                            </form>
                        </div>
                    </div>

                    <div class="coes-tab--content" id="tabs4-css">
                        <div class="coes-box--content container pt-5 pb-5 pe-5 ps-5">
                            <form action="" class="w-100">
                                <div class="coes-form-item--info coes-form-item coes-box coes-box--content pt-3 pe-5 ps-5 mb-5">
                                    <p>
                                        @Model.TituloTipoIntegrante
                                    </p>
                                </div>
                                <div id="mensajeTipo" class="coes-form-item--error coes-form-item coes-box coes-box--content pt-3 pb-3 pe-5 ps-5 mb-3" style="display: none;">
                                    <span>
                                        Por favor complete los datos solicitados.
                                    </span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">6.1 Tipo Agente:</label>
                                    @Html.DropDownListFor(x => x.Empresa.Tipoemprcodi, slTipoAgente, "Seleccione", new { @id = "TipoEmpresa", @class = "Tipo", @style = "width:280px" })
                                    <span class="ps-2" id="spnPaginaWeb">(*)</span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <label class="w-100">6.2 Documento Sustentatorio:</label>
                                    @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipodocsustentatorioComentario))
                                    {
                                        <script>
                                            $("#TipoDocumentoSustentario").prop('disabled', true);
                                        </script>
                                    }
                                    else
                                    {
                                        Disabled = "";
                                    }
                                    @Html.DropDownListFor(x => x.TipoComportamientoPrincipal.IdTipodocsustentatorio, slTipoDocumentoSustentatorio, new { @id = "TipoDocumentoSustentario", @class = "Tipo Required", @style = "width:280px", @data_validator_id = "spnTIPODOCSUSTENTATORIO", @title = Model.TipoComportamientoPrincipal.TipodocsustentatorioComentario })
                                    <span id="spnTIPODOCSUSTENTATORIO">(*)</span>
                                    @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipodocsustentatorioComentario))
                                    {
                                        <span class="Alerta" data-alerta="@Model.TipoComportamientoPrincipal.TipodocsustentatorioComentario" onclick="VerAlerta(this)"><img src="~/Content/Images/ico-warning.gif" /></span>
                                    }
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    <div>&nbsp;&nbsp;&nbsp;&nbsp;</div>
                                    <label class="w-100">6.2.1 Archivo Digitalizado:</label>
                                    @if (string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipoarcdigitalizadoComentario))
                                    {
                                        Disabled = "disabled";
                                        <input type="file" id="flArchivoDigitalizadoTI" @Disabled class="Tipo " accept=".pdf" data-file="@Model.TipoComportamientoPrincipal.Tipoarcdigitalizado" data-filename="@Model.TipoComportamientoPrincipal.Tipoarcdigitalizadofilename" data-validator-id="spnArchivoDigitalizadoTI" style="width:280px;">
                                    }
                                    else
                                    {
                                        Disabled = "";
                                        <input type="file" id="flArchivoDigitalizadoTI" @Disabled class="Tipo Required" accept=".pdf" data-validator-id="spnArchivoDigitalizadoTI" style="width:280px;">
                                        <span id="spnArchivoDigitalizadoTI">(*)</span>
                                    }
                                    <span style="padding-left:10px;padding-right:10px">PDF (Máximo 8Mb)</span>

                                    <span>
                                        <a href='JavaScript:descargarTA(@string.Format("' {0}*{1}'",Model.TipoComportamientoPrincipal.Tipoarcdigitalizado,Model.TipoComportamientoPrincipal.Tipoarcdigitalizadofilename))' title="@Model.TipoComportamientoPrincipal.TipoarcdigitalizadoComentario">
                                            <img src="~/Content/Images/pdficon.png" />
                                        </a>
                                    </span>
                                    @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipoarcdigitalizadoComentario))
                                    {
                                        <span class="Alerta" data-alerta="@Model.TipoComportamientoPrincipal.TipoarcdigitalizadoComentario" onclick="VerAlerta(this)"><img src="~/Content/Images/ico-warning.gif" /></span>
                                    }
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    @if (string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipopotenciainstaladaComentario))
                                    {
                                        Disabled = "disabled";
                                    }
                                    else
                                    {
                                        Disabled = "";
                                    }
                                    <label class="w-100">6.3 Potencia Instalada:</label>
                                    <input type="text" id="txtPotenciaInstaladaTI" class="GeneradorField Required" @Disabled title="@Model.TipoComportamientoPrincipal.TipopotenciainstaladaComentario" value="@Model.TipoComportamientoPrincipal.Tipopotenciainstalada" data-validator-id="spnPotenciaInstaladaTI" style="width:280px;">
                                    <label>MW</label>
                                    <span id="spnPotenciaInstaladaTI">(*)</span>
                                    @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipopotenciainstaladaComentario))
                                    {
                                        <span class="Alerta" data-alerta="@Model.TipoComportamientoPrincipal.TipopotenciainstaladaComentario" onclick="VerAlerta(this)"><img src="~/Content/Images/ico-warning.gif" /></span>
                                    }
                                    <span style="padding-left:15px;" id="spnIntegranteGenerador"></span>
                                </div>
                                <div class="coes-form-item d-flex align-items-center mb-3">
                                    @if (string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TiponrocentralesComentario))
                                    {
                                        Disabled = "disabled";
                                    }
                                    else
                                    {
                                        Disabled = "";
                                    }
                                    <label class="w-100">6.4 Número de Centrales:</label>
                                    <input type="text" id="txtNumeroCentralesTI" class="GeneradorField Required" @Disabled title="@Model.TipoComportamientoPrincipal.TiponrocentralesComentario" value="@Model.TipoComportamientoPrincipal.Tiponrocentrales" data-validator-id="spnNumeroCentralesTI" style="width:280px;"><span id="spnNumeroCentralesTI">(*)</span>
                                    @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TiponrocentralesComentario))
                                    {
                                        <span class="Alerta" data-alerta="@Model.TipoComportamientoPrincipal.TiponrocentralesComentario" onclick="VerAlerta(this)"><img src="~/Content/Images/ico-warning.gif" /></span>
                                    }
                                </div>
                                <div class="coes-form-item align-items-center mb-3" id="Transmisor1">
                                    <label class="w-100">6.3 Línea de Transmisión:</label>
                                </div>
                                <div class="coes-form-item align-items-baseline mb-3" id="Transmisor2">
                                    @if (string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipototallineastransmisionComentario))
                                    {
                                        Disabled = "disabled";
                                    }
                                    else
                                    {
                                        Disabled = "";
                                    }
                                    <div class="Transmisor">
                                        <div style="padding-left:80px">
                                            <div class="TransmisorRow" style="margin: 10px;">
                                                @if (Model.TipoComportamientoPrincipal.Tipolineatrans500 == "S")
                                                {
                                                    <input type="checkbox" id="chk500" checked="checked" @Disabled />
                                                }
                                                else
                                                {
                                                    <input type="checkbox" id="chk500" @Disabled />
                                                }
                                                <label style="width: 500px;">500 kV</label>
                                                <input type="text" id="val500" class="SumTrans" @Disabled value="@Model.TipoComportamientoPrincipal.Tipolineatrans500km" />
                                                <label>km</label>
                                            </div>
                                            <div class="TransmisorRow" style="margin: 10px;">
                                                @if (Model.TipoComportamientoPrincipal.Tipolineatrans220 == "S")
                                                {
                                                    <input type="checkbox" id="chk200" checked="checked" @Disabled />
                                                }
                                                else
                                                {
                                                    <input type="checkbox" id="chk200" @Disabled />
                                                }
                                                <label style="width: 500px;">200 kV</label>
                                                <input type="text" id="val200" class="SumTrans" @Disabled value="@Model.TipoComportamientoPrincipal.Tipolineatrans220km" />
                                                <label>km</label>
                                            </div>
                                            <div class="TransmisorRow" style="margin: 10px;">
                                                @if (Model.TipoComportamientoPrincipal.Tipolineatrans138 == "S")
                                                {
                                                    <input type="checkbox" id="chk138" checked="checked" @Disabled />
                                                }
                                                else
                                                {
                                                    <input type="checkbox" id="chk138" @Disabled />
                                                }
                                                <label style="width: 500px;">138 kV</label>
                                                <input type="text" id="val138" class="SumTrans" @Disabled value="@Model.TipoComportamientoPrincipal.Tipolineatrans138km" />
                                                <label>km</label>
                                            </div>
                                            <div style="margin: 30px 10px 10px 30px;">
                                                <label style="width: 500px;">Total Líneas de transmisión:</label>
                                                <input type="text" id="txtTotalTransmision" title="@Model.TipoComportamientoPrincipal.TipototallineastransmisionComentario" value="@Model.TipoComportamientoPrincipal.Tipototallineastransmision" readonly />
                                                <b>km</b>
                                                <span style="padding-left:15px;" id="spnIntegranteTransmisor"></span>
                                                @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipototallineastransmisionComentario))
                                                {
                                                    <span class="Alerta" data-alerta="@Model.TipoComportamientoPrincipal.TipototallineastransmisionComentario" onclick="VerAlerta(this)"><img src="~/Content/Images/ico-warning.gif" /></span>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="coes-form-item align-items-center mb-3" id="Distribuidor1">
                                    @if (string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipomaxdemandacoincidenteComentario))
                                    {
                                        Disabled = "disabled";
                                    }
                                    else
                                    {
                                        Disabled = "";
                                    }
                                    <label class="w-100">6.3 Máxima demanda coincidente anual:</label>
                                    <input type="text" id="txtMaximaDemandaCoincidenteAnual" class="DistribuidorField Required" @Disabled title="@Model.TipoComportamientoPrincipal.TipomaxdemandacoincidenteComentario" value="@Model.TipoComportamientoPrincipal.Tipomaxdemandacoincidente" style="width:280px;">
                                    <span style="padding-left:10px;padding-right:10px">MW</span>
                                    <span class="ps-2" id="spnPaginaWeb">(*)</span>
                                    @if (!string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipomaxdemandacoincidenteComentario))
                                    {
                                        <span class="Alerta" data-alerta="@Model.TipoComportamientoPrincipal.TipomaxdemandacoincidenteComentario" onclick="VerAlerta(this)"><img src="~/Content/Images/ico-warning.gif" /></span>
                                    }
                                    <span style="padding-left:15px;" id="spnIntegranteDistribuidor"></span>
                                </div>
                                <div class="coes-form-item align-items-center mb-3" id="UsuarioLibre1">
                                    @if (string.IsNullOrEmpty(Model.TipoComportamientoPrincipal.TipomaxdemandacontratadaComentario))
                                    {
                                        Disabled = "disabled";
                                    }
                                    else
                                    {
                                        Disabled = "";
