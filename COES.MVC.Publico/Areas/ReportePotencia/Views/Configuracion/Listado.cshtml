@model COES.MVC.Publico.Areas.ReportePotencia.Models.ListadoConfiguracion

   <div class="popup-title" style="margin-bottom:10px"></div>
            <table cellpadding="0" cellspacing="0" class="pretty tabla-icono" id="tbMedidores">
                <thead>
                    <tr>
                        <th></th>
                        <th>Empresa</th>
                        <th>Modo Operación</th>
                        <th>Abreviatura</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.ListadoModosOperacionConfigurados)
                    {
                        <tr>
                            <td>
                                <input type="checkbox" name="cbMedidor" value="@item.ConfiguradoReportePe" onclick="seleccionarFila(this);" />
                            </td>
                            <td>@item.EmprNomb</td>
                            <td>@item.Gruponomb</td>
                            <td>@item.Grupoabrev</td>
                        </tr>
                    }
                </tbody>
            </table>
