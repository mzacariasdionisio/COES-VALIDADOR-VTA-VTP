<script type="text/javascript">
    $(function () {
      
        $('#btnConsultar').click(function () {
            buscarArchivos();
        });

        $('#campo7').keypress(function (e) {
            if (e.keyCode == '13') {
                buscarArchivos();
                return false;
            }
        });

        $('#busquedaDoble').click(function () {
            $('#campo7').val('');
            $('#campo39').val('');
            $('#campo42').val('');
        });


